/**
 * Generate notification email plain text
 * @param {Object} data - Template data
 * @param {string} data.title - Notification title
 * @param {string} data.message - Main notification message
 * @param {string} [data.recipientName] - Recipient's name
 * @param {Object} [data.action] - Optional action button
 * @param {string} [data.action.text] - Button text
 * @param {string} [data.action.url] - Button URL
 * @param {Array} [data.details] - Optional list of key-value details
 * @returns {string} Plain text email
 */
export function notificationEmailText(data) {
  /* istanbul ignore next */
  cov_29slcp7vh4().f[2]++;
  const {
    title,
    message,
    recipientName,
    action,
    details =
    /* istanbul ignore next */
    (cov_29slcp7vh4().b[5][0]++, [])
  } =
  /* istanbul ignore next */
  (cov_29slcp7vh4().s[8]++, data);
  const greeting =
  /* istanbul ignore next */
  (cov_29slcp7vh4().s[9]++, recipientName ?
  /* istanbul ignore next */
  (cov_29slcp7vh4().b[6][0]++, `Hi ${recipientName},\n\n`) :
  /* istanbul ignore next */
  (cov_29slcp7vh4().b[6][1]++, ''));
  const detailsList =
  /* istanbul ignore next */
  (cov_29slcp7vh4().s[10]++, details.length > 0 ?
  /* istanbul ignore next */
  (cov_29slcp7vh4().b[7][0]++, '\n' + details.map(({
    label,
    value
  }) => {
    /* istanbul ignore next */
    cov_29slcp7vh4().f[3]++;
    cov_29slcp7vh4().s[11]++;
    return `${label}: ${value}`;
  }).join('\n') + '\n') :
  /* istanbul ignore next */
  (cov_29slcp7vh4().b[7][1]++, ''));
  const actionLink =
  /* istanbul ignore next */
  (cov_29slcp7vh4().s[12]++, action ?
  /* istanbul ignore next */
  (cov_29slcp7vh4().b[8][0]++, `\n${action.text}: ${action.url}\n`) :
  /* istanbul ignore next */
  (cov_29slcp7vh4().b[8][1]++, ''));
  /* istanbul ignore next */
  cov_29slcp7vh4().s[13]++;
  return `
${title.toUpperCase()}
${'='.repeat(title.length)}

${greeting}${message}
${detailsList}${actionLink}
---
This is an automated message from Chartwarden. Please do not reply to this email.
  `.trim();
}

/**
