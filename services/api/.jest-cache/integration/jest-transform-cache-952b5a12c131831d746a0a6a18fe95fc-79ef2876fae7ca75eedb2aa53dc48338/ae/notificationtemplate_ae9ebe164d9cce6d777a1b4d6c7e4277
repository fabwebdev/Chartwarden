      label: 'Due Date',
      value: dueDate
    }, {
      label: 'Priority',
      value:
      /* istanbul ignore next */
      (cov_29slcp7vh4().b[12][0]++, priorityLabels[priority]) ||
      /* istanbul ignore next */
      (cov_29slcp7vh4().b[12][1]++, priority)
    }],
    action: {
      text: 'View in Chartwarden',
      url:
      /* istanbul ignore next */
      (cov_29slcp7vh4().b[13][0]++, process.env.APP_URL) ||
      /* istanbul ignore next */
      (cov_29slcp7vh4().b[13][1]++, 'http://localhost:3000')
    }
  });
}
