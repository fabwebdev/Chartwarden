/**
 * Run a test case
 */
export async function runTestCase(request, reply) {
  /* istanbul ignore next */
  cov_hwo38gkxd().f[10]++;
  cov_hwo38gkxd().s[83]++;
  try {
    const {
      policyId,
      testCaseId
    } =
    /* istanbul ignore next */
    (cov_hwo38gkxd().s[84]++, request.params);
    const testCases =
    /* istanbul ignore next */
    (cov_hwo38gkxd().s[85]++, await abacService.getTestCases(parseInt(policyId)));
    const testCase =
    /* istanbul ignore next */
    (cov_hwo38gkxd().s[86]++, testCases.find(tc => {
      /* istanbul ignore next */
      cov_hwo38gkxd().f[11]++;
      cov_hwo38gkxd().s[87]++;
      return tc.id === parseInt(testCaseId);
    }));
    /* istanbul ignore next */
    cov_hwo38gkxd().s[88]++;
    if (!testCase) {
      /* istanbul ignore next */
      cov_hwo38gkxd().b[21][0]++;
      cov_hwo38gkxd().s[89]++;
      return reply.code(404).send({
        success: false,
        error: {
          code: "NOT_FOUND",
          message: "Test case not found"
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_hwo38gkxd().b[21][1]++;
    }
    const result =
    /* istanbul ignore next */
    (cov_hwo38gkxd().s[90]++, await abacService.runPolicyTest(parseInt(policyId), testCase));
    /* istanbul ignore next */
    cov_hwo38gkxd().s[91]++;
    return reply.send({
      success: true,
      data: result
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_hwo38gkxd().s[92]++;
    logger.error("Error running test case:", error);
    /* istanbul ignore next */
    cov_hwo38gkxd().s[93]++;
    return reply.code(500).send({
      success: false,
      error: {
        code: "INTERNAL_ERROR",
        message: "Failed to run test case"
      }
    });
  }
}

