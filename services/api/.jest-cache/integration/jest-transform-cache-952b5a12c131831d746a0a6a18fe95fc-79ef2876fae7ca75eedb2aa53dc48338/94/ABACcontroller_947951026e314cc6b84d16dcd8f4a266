/**
 * Get access logs with filtering
 */
export async function getAccessLogs(request, reply) {
  /* istanbul ignore next */
  cov_hwo38gkxd().f[12]++;
  cov_hwo38gkxd().s[94]++;
  try {
    const {
      userId,
      resourceType,
      decision,
      startDate,
      endDate,
      limit
    } =
    /* istanbul ignore next */
    (cov_hwo38gkxd().s[95]++, request.query);
    const logs =
    /* istanbul ignore next */
    (cov_hwo38gkxd().s[96]++, await abacService.getAccessLogs({
      userId: userId ?
      /* istanbul ignore next */
      (cov_hwo38gkxd().b[22][0]++, parseInt(userId)) :
      /* istanbul ignore next */
      (cov_hwo38gkxd().b[22][1]++, undefined),
      resourceType,
      decision,
      startDate: startDate ?
      /* istanbul ignore next */
      (cov_hwo38gkxd().b[23][0]++, new Date(startDate)) :
      /* istanbul ignore next */
      (cov_hwo38gkxd().b[23][1]++, undefined),
      endDate: endDate ?
      /* istanbul ignore next */
      (cov_hwo38gkxd().b[24][0]++, new Date(endDate)) :
      /* istanbul ignore next */
      (cov_hwo38gkxd().b[24][1]++, undefined),
      limit: limit ?
      /* istanbul ignore next */
      (cov_hwo38gkxd().b[25][0]++, parseInt(limit)) :
      /* istanbul ignore next */
      (cov_hwo38gkxd().b[25][1]++, 100)
    }));
    /* istanbul ignore next */
    cov_hwo38gkxd().s[97]++;
    return reply.send({
      success: true,
      data: logs,
      meta: {
        total: logs.length
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_hwo38gkxd().s[98]++;
    logger.error("Error fetching access logs:", error);
    /* istanbul ignore next */
    cov_hwo38gkxd().s[99]++;
    return reply.code(500).send({
      success: false,
      error: {
        code: "INTERNAL_ERROR",
        message: "Failed to fetch access logs"
      }
    });
  }
}

