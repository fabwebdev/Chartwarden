   * GET /api/email/verify
   * Admin only
   */
  fastify.get('/verify', {
    preHandler: [authenticate, requireRole(ROLES.ADMIN)]
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_9gein4i5n().f[1]++;
    cov_9gein4i5n().s[1]++;
    try {
      const isValid =
      /* istanbul ignore next */
      (cov_9gein4i5n().s[2]++, await Mail.verify());
      /* istanbul ignore next */
      cov_9gein4i5n().s[3]++;
      return {
        status: 200,
        message: isValid ?
        /* istanbul ignore next */
        (cov_9gein4i5n().b[0][0]++, 'Mail transporter verified successfully') :
        /* istanbul ignore next */
        (cov_9gein4i5n().b[0][1]++, 'Mail transporter verification failed'),
        data: {
          verified: isValid,
          mailer:
          /* istanbul ignore next */
          (cov_9gein4i5n().b[1][0]++, process.env.MAIL_MAILER) ||
          /* istanbul ignore next */
          (cov_9gein4i5n().b[1][1]++, 'smtp'),
          host:
          /* istanbul ignore next */
          (cov_9gein4i5n().b[2][0]++, process.env.MAIL_HOST) ||
          /* istanbul ignore next */
          (cov_9gein4i5n().b[2][1]++, 'not configured'),
          port:
          /* istanbul ignore next */
          (cov_9gein4i5n().b[3][0]++, process.env.MAIL_PORT) ||
          /* istanbul ignore next */
          (cov_9gein4i5n().b[3][1]++, 'not configured'),
          fromAddress:
          /* istanbul ignore next */
          (cov_9gein4i5n().b[4][0]++, process.env.MAIL_FROM_ADDRESS) ||
          /* istanbul ignore next */
          (cov_9gein4i5n().b[4][1]++, 'not configured')
        }
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_9gein4i5n().s[4]++;
      logger.error('Mail verification error:', error);
      /* istanbul ignore next */
      cov_9gein4i5n().s[5]++;
      return reply.code(500).send({
        status: 500,
        message: 'Failed to verify mail configuration',
        error: error.message
      });
    }
  });

  /**
   * Send a test email
   * POST /api/email/test
   * Admin only
   * Body: { to: string, template?: 'welcome' | 'password-reset' | 'notification' }
   */
  /* istanbul ignore next */
  cov_9gein4i5n().s[6]++;
  fastify.post('/test', {
    preHandler: [authenticate, requireRole(ROLES.ADMIN)],
    schema: {
      body: {
        type: 'object',
        required: ['to'],
        properties: {
          to: {
            type: 'string',
            format: 'email'
          },
          template: {
            type: 'string',
            enum: ['welcome', 'password-reset', 'notification'],
            default: 'notification'
          }
        }
      }
    }
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_9gein4i5n().f[2]++;
    cov_9gein4i5n().s[7]++;
    try {
      const {
        to,
        template =
        /* istanbul ignore next */
        (cov_9gein4i5n().b[5][0]++, 'notification')
      } =
      /* istanbul ignore next */
      (cov_9gein4i5n().s[8]++, request.body);
      const testData =
      /* istanbul ignore next */
      (cov_9gein4i5n().s[9]++, {
        name: 'Test User',
        email: to,
        loginUrl:
        /* istanbul ignore next */
        (cov_9gein4i5n().b[6][0]++, process.env.APP_URL) ||
        /* istanbul ignore next */
        (cov_9gein4i5n().b[6][1]++, 'http://localhost:3000'),
        supportEmail:
        /* istanbul ignore next */
        (cov_9gein4i5n().b[7][0]++, process.env.SUPPORT_EMAIL) ||
        /* istanbul ignore next */
        (cov_9gein4i5n().b[7][1]++, 'support@chartwarden.com'),
        resetUrl: `${
        /* istanbul ignore next */
        (cov_9gein4i5n().b[8][0]++, process.env.APP_URL) ||
        /* istanbul ignore next */
        (cov_9gein4i5n().b[8][1]++, 'http://localhost:3000')}/auth/reset-password?token=test-token`,
        expiresInMinutes: 60,
        title: 'Test Notification',
        message: 'This is a test notification email from Chartwarden. If you received this email, the email system is working correctly.',
        type: 'info',
        recipientName: 'Test User'
      });
      let subject, html, text;
      /* istanbul ignore next */
      cov_9gein4i5n().s[10]++;
      switch (template) {
        case 'welcome':
          /* istanbul ignore next */
          cov_9gein4i5n().b[9][0]++;
          cov_9gein4i5n().s[11]++;
          subject = 'Test: Welcome to Chartwarden';
          /* istanbul ignore next */
          cov_9gein4i5n().s[12]++;
          html = welcomeEmailHtml(testData);
          /* istanbul ignore next */
          cov_9gein4i5n().s[13]++;
          text = welcomeEmailText(testData);
          /* istanbul ignore next */
          cov_9gein4i5n().s[14]++;
          break;
        case 'password-reset':
          /* istanbul ignore next */
          cov_9gein4i5n().b[9][1]++;
          cov_9gein4i5n().s[15]++;
          subject = 'Test: Password Reset Request';
          /* istanbul ignore next */
          cov_9gein4i5n().s[16]++;
          html = passwordResetEmailHtml(testData);
          /* istanbul ignore next */
          cov_9gein4i5n().s[17]++;
          text = passwordResetEmailText(testData);
          /* istanbul ignore next */
          cov_9gein4i5n().s[18]++;
          break;
        case 'notification':
          /* istanbul ignore next */
          cov_9gein4i5n().b[9][2]++;
        default:
          /* istanbul ignore next */
          cov_9gein4i5n().b[9][3]++;
          cov_9gein4i5n().s[19]++;
          subject = 'Test: Chartwarden Notification';
          /* istanbul ignore next */
          cov_9gein4i5n().s[20]++;
          html = notificationEmailHtml(testData);
          /* istanbul ignore next */
          cov_9gein4i5n().s[21]++;
          text = notificationEmailText(testData);
          /* istanbul ignore next */
          cov_9gein4i5n().s[22]++;
          break;
      }
      const result =
      /* istanbul ignore next */
      (cov_9gein4i5n().s[23]++, await Mail.send({
        to,
        subject,
        html,
        text
      }));
      /* istanbul ignore next */
      cov_9gein4i5n().s[24]++;
      logger.info('Test email sent:', {
        to,
        template,
        messageId: result.messageId
      });
      /* istanbul ignore next */
      cov_9gein4i5n().s[25]++;
      return {
        status: 200,
        message: 'Test email sent successfully',
        data: {
          to,
          template,
          messageId: result.messageId
        }
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_9gein4i5n().s[26]++;
      logger.error('Test email error:', error);
      /* istanbul ignore next */
      cov_9gein4i5n().s[27]++;
      return reply.code(500).send({
        status: 500,
        message: 'Failed to send test email',
        error: error.message
      });
    }
  });

  /**
   * Preview email template (returns HTML)
   * GET /api/email/preview/:template
   * Admin only
   */
  /* istanbul ignore next */
  cov_9gein4i5n().s[28]++;
  fastify.get('/preview/:template', {
    preHandler: [authenticate, requireRole(ROLES.ADMIN)],
    schema: {
      params: {
        type: 'object',
        required: ['template'],
        properties: {
          template: {
            type: 'string',
            enum: ['welcome', 'password-reset', 'notification']
          }
        }
      }
    }
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_9gein4i5n().f[3]++;
    cov_9gein4i5n().s[29]++;
    try {
      const {
        template
      } =
      /* istanbul ignore next */
      (cov_9gein4i5n().s[30]++, request.params);
      const sampleData =
      /* istanbul ignore next */
      (cov_9gein4i5n().s[31]++, {
        name: 'John Doe',
        email: 'john.doe@example.com',
        loginUrl: 'http://localhost:3000',
        supportEmail: 'support@chartwarden.com',
        resetUrl: 'http://localhost:3000/auth/reset-password?token=sample-token-12345',
        expiresInMinutes: 60,
        title: 'Sample Notification',
        message: 'This is a sample notification message for preview purposes.',
        type: 'info',
        recipientName: 'John Doe',
        action: {
          text: 'View Details',
          url: 'http://localhost:3000/details'
        },
        details: [{
          label: 'Patient',
          value: 'Jane Smith'
        }, {
          label: 'MRN',
          value: '123456'
        }, {
          label: 'Date',
          value: new Date().toLocaleDateString()
        }]
      });
      let html;
      /* istanbul ignore next */
      cov_9gein4i5n().s[32]++;
      switch (template) {
        case 'welcome':
          /* istanbul ignore next */
          cov_9gein4i5n().b[10][0]++;
          cov_9gein4i5n().s[33]++;
          html = welcomeEmailHtml(sampleData);
          /* istanbul ignore next */
          cov_9gein4i5n().s[34]++;
          break;
        case 'password-reset':
          /* istanbul ignore next */
          cov_9gein4i5n().b[10][1]++;
          cov_9gein4i5n().s[35]++;
          html = passwordResetEmailHtml(sampleData);
          /* istanbul ignore next */
          cov_9gein4i5n().s[36]++;
          break;
        case 'notification':
          /* istanbul ignore next */
          cov_9gein4i5n().b[10][2]++;
        default:
          /* istanbul ignore next */
          cov_9gein4i5n().b[10][3]++;
          cov_9gein4i5n().s[37]++;
          html = notificationEmailHtml(sampleData);
          /* istanbul ignore next */
          cov_9gein4i5n().s[38]++;
          break;
      }
      /* istanbul ignore next */
      cov_9gein4i5n().s[39]++;
      reply.type('text/html');
      /* istanbul ignore next */
      cov_9gein4i5n().s[40]++;
      return html;
    } catch (error) {
      /* istanbul ignore next */
      cov_9gein4i5n().s[41]++;
      logger.error('Template preview error:', error);
      /* istanbul ignore next */
      cov_9gein4i5n().s[42]++;
      return reply.code(500).send({
        status: 500,
        message: 'Failed to generate template preview',
        error: error.message
      });
    }
  });

  /**
   * Get mail configuration status
   * GET /api/email/config
   * Admin only
   */
  /* istanbul ignore next */
  cov_9gein4i5n().s[43]++;
  fastify.get('/config', {
    preHandler: [authenticate, requireRole(ROLES.ADMIN)]
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_9gein4i5n().f[4]++;
    cov_9gein4i5n().s[44]++;
    try {
      /* istanbul ignore next */
      cov_9gein4i5n().s[45]++;
      return {
        status: 200,
        message: 'Mail configuration retrieved',
        data: {
          mailer:
          /* istanbul ignore next */
          (cov_9gein4i5n().b[11][0]++, process.env.MAIL_MAILER) ||
          /* istanbul ignore next */
          (cov_9gein4i5n().b[11][1]++, 'smtp'),
          host:
          /* istanbul ignore next */
          (cov_9gein4i5n().b[12][0]++, process.env.MAIL_HOST) ||
          /* istanbul ignore next */
          (cov_9gein4i5n().b[12][1]++, 'not configured'),
          port:
          /* istanbul ignore next */
          (cov_9gein4i5n().b[13][0]++, process.env.MAIL_PORT) ||
          /* istanbul ignore next */
          (cov_9gein4i5n().b[13][1]++, 587),
          encryption:
          /* istanbul ignore next */
          (cov_9gein4i5n().b[14][0]++, process.env.MAIL_ENCRYPTION) ||
          /* istanbul ignore next */
          (cov_9gein4i5n().b[14][1]++, 'tls'),
          fromAddress:
          /* istanbul ignore next */
          (cov_9gein4i5n().b[15][0]++, process.env.MAIL_FROM_ADDRESS) ||
          /* istanbul ignore next */
          (cov_9gein4i5n().b[15][1]++, 'not configured'),
          fromName:
          /* istanbul ignore next */
          (cov_9gein4i5n().b[16][0]++, process.env.MAIL_FROM_NAME) ||
          /* istanbul ignore next */
          (cov_9gein4i5n().b[16][1]++, 'not configured'),
          // Don't expose credentials, just show if they're set
          hasUsername: !!process.env.MAIL_USERNAME,
          hasPassword: !!process.env.MAIL_PASSWORD
        }
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_9gein4i5n().s[46]++;
      logger.error('Config retrieval error:', error);
      /* istanbul ignore next */
      cov_9gein4i5n().s[47]++;
      return reply.code(500).send({
        status: 500,
        message: 'Failed to retrieve mail configuration',
        error: error.message
      });
    }
  });
}
