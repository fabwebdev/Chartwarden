  async listObligations(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[9]++;
    cov_qr1znkavv().s[82]++;
    try {
      const {
        id
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[83]++, req.params);
      const {
        status
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[84]++, req.query);
      let query =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[85]++, db.select().from(asc606_performance_obligations).where(eq(asc606_performance_obligations.contract_id, parseInt(id))));
      /* istanbul ignore next */
      cov_qr1znkavv().s[86]++;
      if (status) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[20][0]++;
        cov_qr1znkavv().s[87]++;
        query = query.where(and(eq(asc606_performance_obligations.contract_id, parseInt(id)), eq(asc606_performance_obligations.satisfaction_status, status)));
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[20][1]++;
      }
      const obligations =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[88]++, await query.orderBy(asc606_performance_obligations.obligation_number));
      /* istanbul ignore next */
      cov_qr1znkavv().s[89]++;
      res.json({
        success: true,
        count: obligations.length,
        data: obligations
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[90]++;
      logger.error('Error listing performance obligations:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[91]++;
      res.status(500).json({
        success: false,
        error: 'Failed to list performance obligations',
        message: error.message
      });
    }
  }

  /**
   * PUT /api/asc606/obligations/:id
   * Update performance obligation
   */
  async updateObligation(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[10]++;
    cov_qr1znkavv().s[92]++;
    try {
      const {
        id
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[93]++, req.params);
      const userId =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[94]++, req.user.id);
      const updateData =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[95]++, req.body);
      const [updatedObligation] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[96]++, await db.update(asc606_performance_obligations).set({
        ...updateData,
        updated_by_id: userId,
        updated_at: new Date()
      }).where(eq(asc606_performance_obligations.id, parseInt(id))).returning());
      /* istanbul ignore next */
      cov_qr1znkavv().s[97]++;
      if (!updatedObligation) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[21][0]++;
        cov_qr1znkavv().s[98]++;
        return res.status(404).json({
          success: false,
          error: 'Obligation not found'
        });
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[21][1]++;
      }
      cov_qr1znkavv().s[99]++;
      res.json({
        success: true,
        message: 'Performance obligation updated successfully',
        data: updatedObligation
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[100]++;
      logger.error('Error updating performance obligation:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[101]++;
      res.status(500).json({
        success: false,
        error: 'Failed to update performance obligation',
        message: error.message
      });
    }
  }

  // ============================================
  // TRANSACTION PRICES (STEP 3)
  // ============================================

  /**
   * POST /api/asc606/contracts/:id/prices
   * Create or update transaction price
   */
  async createTransactionPrice(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[11]++;
    cov_qr1znkavv().s[102]++;
    try {
      const {
        id
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[103]++, req.params);
      const userId =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[104]++, req.user.id);
      const priceData =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[105]++, req.body);

      // Mark any existing current price as superseded
      /* istanbul ignore next */
      cov_qr1znkavv().s[106]++;
      await db.update(asc606_transaction_prices).set({
        is_current: false,
        superseded_date: priceData.effective_date
      }).where(and(eq(asc606_transaction_prices.contract_id, parseInt(id)), eq(asc606_transaction_prices.is_current, true)));

      // Get current version
      const [latestPrice] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[107]++, await db.select().from(asc606_transaction_prices).where(eq(asc606_transaction_prices.contract_id, parseInt(id))).orderBy(desc(asc606_transaction_prices.version)).limit(1));
      const newVersion =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[108]++, (
      /* istanbul ignore next */
      (cov_qr1znkavv().b[22][0]++, latestPrice?.version) ||
      /* istanbul ignore next */
      (cov_qr1znkavv().b[22][1]++, 0)) + 1);
      const [transactionPrice] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[109]++, await db.insert(asc606_transaction_prices).values({
        ...priceData,
        contract_id: parseInt(id),
        version: newVersion,
        is_current: true,
        created_by_id: userId
      }).returning());
      /* istanbul ignore next */
      cov_qr1znkavv().s[110]++;
      res.status(201).json({
        success: true,
        message: 'Transaction price created successfully',
        data: transactionPrice
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[111]++;
      logger.error('Error creating transaction price:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[112]++;
      res.status(500).json({
        success: false,
        error: 'Failed to create transaction price',
        message: error.message
      });
    }
  }

  /**
   * GET /api/asc606/contracts/:id/prices
   * Get transaction price history
   */
  async getTransactionPrices(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[12]++;
    cov_qr1znkavv().s[113]++;
    try {
      const {
        id
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[114]++, req.params);
      const {
        currentOnly
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[115]++, req.query);
      let query =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[116]++, db.select().from(asc606_transaction_prices).where(eq(asc606_transaction_prices.contract_id, parseInt(id))));
      /* istanbul ignore next */
      cov_qr1znkavv().s[117]++;
      if (currentOnly === 'true') {
        /* istanbul ignore next */
        cov_qr1znkavv().b[23][0]++;
        cov_qr1znkavv().s[118]++;
        query = query.where(and(eq(asc606_transaction_prices.contract_id, parseInt(id)), eq(asc606_transaction_prices.is_current, true)));
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[23][1]++;
      }
      const prices =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[119]++, await query.orderBy(desc(asc606_transaction_prices.version)));
      /* istanbul ignore next */
      cov_qr1znkavv().s[120]++;
      res.json({
        success: true,
        count: prices.length,
        data: prices
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[121]++;
      logger.error('Error getting transaction prices:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[122]++;
      res.status(500).json({
        success: false,
        error: 'Failed to get transaction prices',
        message: error.message
      });
    }
  }

  // ============================================
  // PRICE ALLOCATIONS (STEP 4)
  // ============================================

  /**
   * POST /api/asc606/contracts/:id/allocate
   * Allocate transaction price to performance obligations
   */
  async allocatePrices(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[13]++;
    cov_qr1znkavv().s[123]++;
    try {
      const {
        id
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[124]++, req.params);
      const userId =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[125]++, req.user.id);
      const {
        allocations,
        effective_date
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[126]++, req.body);

      // Get current transaction price
      const [transactionPrice] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[127]++, await db.select().from(asc606_transaction_prices).where(and(eq(asc606_transaction_prices.contract_id, parseInt(id)), eq(asc606_transaction_prices.is_current, true))));
      /* istanbul ignore next */
      cov_qr1znkavv().s[128]++;
      if (!transactionPrice) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[24][0]++;
        cov_qr1znkavv().s[129]++;
        return res.status(400).json({
          success: false,
          error: 'No current transaction price found for contract'
        });
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[24][1]++;
      }

      // Mark existing allocations as not current
      cov_qr1znkavv().s[130]++;
      await db.update(asc606_price_allocations).set({
        is_current: false
      }).where(and(eq(asc606_price_allocations.contract_id, parseInt(id)), eq(asc606_price_allocations.is_current, true)));

      // Calculate total SSP for relative allocation
      const totalSSP =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[131]++, allocations.reduce((sum, a) => {
        /* istanbul ignore next */
        cov_qr1znkavv().f[14]++;
        cov_qr1znkavv().s[132]++;
        return sum + a.standalone_selling_price;
      }, 0));

      // Create new allocations
      const createdAllocations =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[133]++, []);
      /* istanbul ignore next */
      cov_qr1znkavv().s[134]++;
      for (const alloc of allocations) {
        const relativeSspPercentage =
        /* istanbul ignore next */
        (cov_qr1znkavv().s[135]++, Math.round(alloc.standalone_selling_price / totalSSP * 10000));
        const allocatedAmount =
        /* istanbul ignore next */
        (cov_qr1znkavv().s[136]++, Math.round(transactionPrice.total_transaction_price * relativeSspPercentage / 10000));

        // Calculate daily accrual rate
        const obligation =
        /* istanbul ignore next */
        (cov_qr1znkavv().s[137]++, await db.select().from(asc606_performance_obligations).where(eq(asc606_performance_obligations.id, alloc.obligation_id)));
        let dailyAccrualRate =
        /* istanbul ignore next */
        (cov_qr1znkavv().s[138]++, null);
        /* istanbul ignore next */
        cov_qr1znkavv().s[139]++;
        if (obligation[0]?.expected_duration_days) {
          /* istanbul ignore next */
          cov_qr1znkavv().b[25][0]++;
          cov_qr1znkavv().s[140]++;
          dailyAccrualRate = Math.round(allocatedAmount / obligation[0].expected_duration_days);
        } else
        /* istanbul ignore next */
        {
          cov_qr1znkavv().b[25][1]++;
        }
        const [allocation] =
        /* istanbul ignore next */
        (cov_qr1znkavv().s[141]++, await db.insert(asc606_price_allocations).values({
          contract_id: parseInt(id),
          transaction_price_id: transactionPrice.id,
          obligation_id: alloc.obligation_id,
          effective_date:
          /* istanbul ignore next */
          (cov_qr1znkavv().b[26][0]++, effective_date) ||
          /* istanbul ignore next */
          (cov_qr1znkavv().b[26][1]++, new Date().toISOString().split('T')[0]),
          standalone_selling_price: alloc.standalone_selling_price,
          ssp_determination_method: alloc.ssp_determination_method,
          ssp_observable_source: alloc.ssp_observable_source,
          relative_ssp_percentage: relativeSspPercentage,
          allocated_amount: allocatedAmount,
          daily_accrual_rate: dailyAccrualRate,
          is_current: true,
          created_by_id: userId
        }).returning());
        /* istanbul ignore next */
        cov_qr1znkavv().s[142]++;
        createdAllocations.push(allocation);

        // Update obligation with allocated price
        /* istanbul ignore next */
        cov_qr1znkavv().s[143]++;
        await db.update(asc606_performance_obligations).set({
          allocated_price: allocatedAmount,
          allocation_percentage: relativeSspPercentage,
          updated_by_id: userId,
          updated_at: new Date()
        }).where(eq(asc606_performance_obligations.id, alloc.obligation_id));
      }
      /* istanbul ignore next */
      cov_qr1znkavv().s[144]++;
      res.status(201).json({
        success: true,
        message: 'Price allocations created successfully',
        data: {
          totalTransactionPrice: transactionPrice.total_transaction_price,
          totalSSP,
          allocations: createdAllocations
        }
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[145]++;
      logger.error('Error allocating prices:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[146]++;
      res.status(500).json({
        success: false,
        error: 'Failed to allocate prices',
        message: error.message
      });
    }
  }

  /**
   * GET /api/asc606/contracts/:id/allocations
   * Get price allocations
   */
  async getAllocations(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[15]++;
    cov_qr1znkavv().s[147]++;
    try {
      const {
        id
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[148]++, req.params);
      const {
        currentOnly
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[149]++, req.query);
      let conditions =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[150]++, [eq(asc606_price_allocations.contract_id, parseInt(id))]);
      /* istanbul ignore next */
      cov_qr1znkavv().s[151]++;
      if (currentOnly === 'true') {
        /* istanbul ignore next */
        cov_qr1znkavv().b[27][0]++;
        cov_qr1znkavv().s[152]++;
        conditions.push(eq(asc606_price_allocations.is_current, true));
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[27][1]++;
      }
      const allocations =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[153]++, await db.select().from(asc606_price_allocations).where(and(...conditions)).orderBy(asc606_price_allocations.obligation_id));
      /* istanbul ignore next */
      cov_qr1znkavv().s[154]++;
      res.json({
        success: true,
        count: allocations.length,
        data: allocations
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[155]++;
      logger.error('Error getting allocations:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[156]++;
      res.status(500).json({
        success: false,
        error: 'Failed to get allocations',
        message: error.message
      });
    }
  }

  // ============================================
  // DAILY ACCRUALS (STEP 5)
  // ============================================

  /**
   * POST /api/asc606/accruals/calculate
   * Calculate daily revenue accruals for a date range
   */
  async calculateDailyAccruals(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[16]++;
    cov_qr1znkavv().s[157]++;
    try {
      const userId =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[158]++, req.user.id);
      const {
        startDate,
        endDate,
        contractId
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[159]++, req.body);
      /* istanbul ignore next */
      cov_qr1znkavv().s[160]++;
      if (
      /* istanbul ignore next */
      (cov_qr1znkavv().b[29][0]++, !startDate) ||
      /* istanbul ignore next */
      (cov_qr1znkavv().b[29][1]++, !endDate)) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[28][0]++;
        cov_qr1znkavv().s[161]++;
        return res.status(400).json({
          success: false,
          error: 'startDate and endDate are required'
        });
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[28][1]++;
      }

      // Get active contracts with allocations
      let contractQuery =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[162]++, db.select().from(asc606_contracts).where(and(eq(asc606_contracts.is_valid_asc606_contract, true), eq(asc606_contracts.contract_status, 'ACTIVE'))));
      /* istanbul ignore next */
      cov_qr1znkavv().s[163]++;
      if (contractId) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[30][0]++;
        cov_qr1znkavv().s[164]++;
        contractQuery = contractQuery.where(eq(asc606_contracts.id, parseInt(contractId)));
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[30][1]++;
      }
      const contracts =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[165]++, await contractQuery);
      const accrualResults =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[166]++, []);
      /* istanbul ignore next */
      cov_qr1znkavv().s[167]++;
      for (const contract of contracts) {
        // Get current allocations
        const allocations =
        /* istanbul ignore next */
        (cov_qr1znkavv().s[168]++, await db.select().from(asc606_price_allocations).where(and(eq(asc606_price_allocations.contract_id, contract.id), eq(asc606_price_allocations.is_current, true))));
        /* istanbul ignore next */
        cov_qr1znkavv().s[169]++;
        for (const allocation of allocations) {
          // Get the obligation
          const [obligation] =
          /* istanbul ignore next */
          (cov_qr1znkavv().s[170]++, await db.select().from(asc606_performance_obligations).where(eq(asc606_performance_obligations.id, allocation.obligation_id)));
          /* istanbul ignore next */
          cov_qr1znkavv().s[171]++;
          if (!obligation) {
            /* istanbul ignore next */
            cov_qr1znkavv().b[31][0]++;
            cov_qr1znkavv().s[172]++;
            continue;
          } else
          /* istanbul ignore next */
          {
            cov_qr1znkavv().b[31][1]++;
          }

          // Calculate accruals for each day in range
          const start =
          /* istanbul ignore next */
          (cov_qr1znkavv().s[173]++, new Date(startDate));
          const end =
          /* istanbul ignore next */
          (cov_qr1znkavv().s[174]++, new Date(endDate));
          /* istanbul ignore next */
          cov_qr1znkavv().s[175]++;
          for (let date =
          /* istanbul ignore next */
          (cov_qr1znkavv().s[176]++, new Date(start)); date <= end; date.setDate(date.getDate() + 1)) {
            const accrualDate =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[177]++, date.toISOString().split('T')[0]);

            // Skip if outside obligation period
            /* istanbul ignore next */
            cov_qr1znkavv().s[178]++;
            if (accrualDate < obligation.obligation_start_date) {
              /* istanbul ignore next */
              cov_qr1znkavv().b[32][0]++;
              cov_qr1znkavv().s[179]++;
              continue;
            } else
            /* istanbul ignore next */
            {
              cov_qr1znkavv().b[32][1]++;
            }
            cov_qr1znkavv().s[180]++;
            if (
            /* istanbul ignore next */
            (cov_qr1znkavv().b[34][0]++, obligation.obligation_end_date) &&
            /* istanbul ignore next */
            (cov_qr1znkavv().b[34][1]++, accrualDate > obligation.obligation_end_date)) {
              /* istanbul ignore next */
              cov_qr1znkavv().b[33][0]++;
              cov_qr1znkavv().s[181]++;
              continue;
            } else
            /* istanbul ignore next */
            {
              cov_qr1znkavv().b[33][1]++;
            }

            // Check if accrual already exists
            const [existingAccrual] =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[182]++, await db.select().from(asc606_daily_revenue_accruals).where(and(eq(asc606_daily_revenue_accruals.contract_id, contract.id), eq(asc606_daily_revenue_accruals.obligation_id, obligation.id), eq(asc606_daily_revenue_accruals.accrual_date, accrualDate))));
            /* istanbul ignore next */
            cov_qr1znkavv().s[183]++;
            if (existingAccrual) {
              /* istanbul ignore next */
              cov_qr1znkavv().b[35][0]++;
              cov_qr1znkavv().s[184]++;
              continue;
            } else
            /* istanbul ignore next */
            {
              cov_qr1znkavv().b[35][1]++;
            }

            // Calculate daily amount
            const dailyRate =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[185]++,
            /* istanbul ignore next */
            (cov_qr1znkavv().b[36][0]++, allocation.daily_accrual_rate) ||
            /* istanbul ignore next */
            (cov_qr1znkavv().b[36][1]++, 0));
            const fiscalInfo =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[186]++, this._getFiscalInfo(new Date(accrualDate)));
            const isLeapYear =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[187]++, this._isLeapYear(fiscalInfo.fiscalYear));
            const daysInMonth =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[188]++, this._getDaysInMonth(new Date(accrualDate)));

            // Check for partial periods
            const isPartialPeriod =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[189]++,
            /* istanbul ignore next */
            (cov_qr1znkavv().b[37][0]++, accrualDate === obligation.obligation_start_date) ||
            /* istanbul ignore next */
            (cov_qr1znkavv().b[37][1]++, accrualDate === obligation.obligation_end_date));

            // Get cumulative amounts
            const [cumulativeResult] =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[190]++, await db.select({
              total: sql`COALESCE(SUM(adjusted_daily_amount), 0)`
            }).from(asc606_daily_revenue_accruals).where(and(eq(asc606_daily_revenue_accruals.contract_id, contract.id), eq(asc606_daily_revenue_accruals.obligation_id, obligation.id), sql`${asc606_daily_revenue_accruals.accrual_date} < ${accrualDate}`)));
            const cumulativeRecognized =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[191]++, Number(
            /* istanbul ignore next */
            (cov_qr1znkavv().b[38][0]++, cumulativeResult?.total) ||
            /* istanbul ignore next */
            (cov_qr1znkavv().b[38][1]++, 0)) + dailyRate);
            const remainingToRecognize =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[192]++, allocation.allocated_amount - cumulativeRecognized);
            const satisfactionPercentage =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[193]++, Math.round(cumulativeRecognized / allocation.allocated_amount * 10000));
            const [accrual] =
            /* istanbul ignore next */
            (cov_qr1znkavv().s[194]++, await db.insert(asc606_daily_revenue_accruals).values({
              contract_id: contract.id,
              obligation_id: obligation.id,
              allocation_id: allocation.id,
              patient_id: contract.patient_id,
              accrual_date: accrualDate,
              fiscal_year: fiscalInfo.fiscalYear,
              fiscal_quarter: fiscalInfo.fiscalQuarter,
              fiscal_month: fiscalInfo.fiscalMonth,
              period_label: fiscalInfo.periodLabel,
              is_leap_year: isLeapYear,
              days_in_month: daysInMonth,
              is_partial_period: isPartialPeriod,
              recognition_pattern: obligation.recognition_pattern,
              base_daily_rate: dailyRate,
              adjustment_factor: 10000,
              // 100%
              adjusted_daily_amount: dailyRate,
              cumulative_recognized: cumulativeRecognized,
              remaining_to_recognize: remainingToRecognize > 0 ?
              /* istanbul ignore next */
              (cov_qr1znkavv().b[39][0]++, remainingToRecognize) :
              /* istanbul ignore next */
              (cov_qr1znkavv().b[39][1]++, 0),
              satisfaction_percentage: satisfactionPercentage,
              status: 'CALCULATED',
              calculation_timestamp: new Date(),
              calculation_source: 'AUTOMATED_DAILY_JOB',
              created_by_id: userId
            }).returning());
            /* istanbul ignore next */
            cov_qr1znkavv().s[195]++;
            accrualResults.push(accrual);
          }
        }
      }
      /* istanbul ignore next */
      cov_qr1znkavv().s[196]++;
      res.status(201).json({
        success: true,
        message: `Calculated ${accrualResults.length} daily accruals`,
        data: {
          count: accrualResults.length,
          dateRange: {
            startDate,
            endDate
          },
          contractsProcessed: contracts.length
        }
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[197]++;
      logger.error('Error calculating daily accruals:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[198]++;
      res.status(500).json({
        success: false,
        error: 'Failed to calculate daily accruals',
        message: error.message
      });
    }
  }

  /**
   * GET /api/asc606/accruals
   * Get accruals for period
   */
  async getAccruals(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[17]++;
    cov_qr1znkavv().s[199]++;
    try {
      const {
        startDate,
        endDate,
        period,
        contractId,
        status,
        limit =
        /* istanbul ignore next */
        (cov_qr1znkavv().b[40][0]++, 100),
        offset =
        /* istanbul ignore next */
        (cov_qr1znkavv().b[41][0]++, 0)
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[200]++, req.query);
      const conditions =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[201]++, []);
      /* istanbul ignore next */
      cov_qr1znkavv().s[202]++;
      if (
      /* istanbul ignore next */
      (cov_qr1znkavv().b[43][0]++, startDate) &&
      /* istanbul ignore next */
      (cov_qr1znkavv().b[43][1]++, endDate)) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[42][0]++;
        cov_qr1znkavv().s[203]++;
        conditions.push(between(asc606_daily_revenue_accruals.accrual_date, startDate, endDate));
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[42][1]++;
      }
      cov_qr1znkavv().s[204]++;
      if (period) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[44][0]++;
        cov_qr1znkavv().s[205]++;
        conditions.push(eq(asc606_daily_revenue_accruals.period_label, period));
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[44][1]++;
      }
      cov_qr1znkavv().s[206]++;
      if (contractId) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[45][0]++;
        cov_qr1znkavv().s[207]++;
        conditions.push(eq(asc606_daily_revenue_accruals.contract_id, parseInt(contractId)));
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[45][1]++;
      }
      cov_qr1znkavv().s[208]++;
      if (status) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[46][0]++;
        cov_qr1znkavv().s[209]++;
        conditions.push(eq(asc606_daily_revenue_accruals.status, status));
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[46][1]++;
      }
      let query =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[210]++, db.select().from(asc606_daily_revenue_accruals));
      /* istanbul ignore next */
      cov_qr1znkavv().s[211]++;
      if (conditions.length > 0) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[47][0]++;
        cov_qr1znkavv().s[212]++;
        query = query.where(and(...conditions));
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[47][1]++;
      }
      const accruals =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[213]++, await query.orderBy(desc(asc606_daily_revenue_accruals.accrual_date)).limit(parseInt(limit)).offset(parseInt(offset)));

      // Get summary
      const [summary] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[214]++, await db.select({
        total_recognized: sql`SUM(adjusted_daily_amount)`,
        count: sql`COUNT(*)`
      }).from(asc606_daily_revenue_accruals).where(conditions.length > 0 ?
      /* istanbul ignore next */
      (cov_qr1znkavv().b[48][0]++, and(...conditions)) :
      /* istanbul ignore next */
      (cov_qr1znkavv().b[48][1]++, undefined)));
      /* istanbul ignore next */
      cov_qr1znkavv().s[215]++;
      res.json({
        success: true,
        count: accruals.length,
        summary: {
          totalRecognized: Number(
          /* istanbul ignore next */
          (cov_qr1znkavv().b[49][0]++, summary?.total_recognized) ||
          /* istanbul ignore next */
          (cov_qr1znkavv().b[49][1]++, 0)),
          totalRecords: Number(
          /* istanbul ignore next */
          (cov_qr1znkavv().b[50][0]++, summary?.count) ||
          /* istanbul ignore next */
          (cov_qr1znkavv().b[50][1]++, 0))
        },
        data: accruals
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[216]++;
      logger.error('Error getting accruals:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[217]++;
      res.status(500).json({
        success: false,
        error: 'Failed to get accruals',
        message: error.message
      });
    }
  }

  // ============================================
  // CONTRACT MODIFICATIONS
  // ============================================

  /**
   * POST /api/asc606/contracts/:id/modifications
   * Create contract modification
   */
  async createModification(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[18]++;
    cov_qr1znkavv().s[218]++;
    try {
      const {
        id
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[219]++, req.params);
      const userId =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[220]++, req.user.id);
      const modificationData =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[221]++, req.body);

      // Get current contract state
      const [contract] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[222]++, await db.select().from(asc606_contracts).where(eq(asc606_contracts.id, parseInt(id))));
      /* istanbul ignore next */
      cov_qr1znkavv().s[223]++;
      if (!contract) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[51][0]++;
        cov_qr1znkavv().s[224]++;
        return res.status(404).json({
          success: false,
          error: 'Contract not found'
        });
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[51][1]++;
      }
      const [modification] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[225]++, await db.insert(asc606_contract_modifications).values({
        ...modificationData,
        contract_id: parseInt(id),
        previous_total_price: contract.total_contract_value,
        modification_status: 'PENDING',
        created_by_id: userId
      }).returning());
      /* istanbul ignore next */
      cov_qr1znkavv().s[226]++;
      res.status(201).json({
        success: true,
        message: 'Contract modification created successfully',
        data: modification
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[227]++;
      logger.error('Error creating contract modification:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[228]++;
      res.status(500).json({
        success: false,
        error: 'Failed to create modification',
        message: error.message
      });
    }
  }

  /**
   * GET /api/asc606/contracts/:id/modifications
   * List contract modifications
   */
  async listModifications(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[19]++;
    cov_qr1znkavv().s[229]++;
    try {
      const {
        id
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[230]++, req.params);
      const modifications =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[231]++, await db.select().from(asc606_contract_modifications).where(eq(asc606_contract_modifications.contract_id, parseInt(id))).orderBy(desc(asc606_contract_modifications.modification_date)));
      /* istanbul ignore next */
      cov_qr1znkavv().s[232]++;
      res.json({
        success: true,
        count: modifications.length,
        data: modifications
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[233]++;
      logger.error('Error listing modifications:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[234]++;
      res.status(500).json({
        success: false,
        error: 'Failed to list modifications',
        message: error.message
      });
    }
  }

  // ============================================
  // RECONCILIATION
  // ============================================

  /**
   * POST /api/asc606/reconciliation
   * Create period reconciliation
   */
  async createReconciliation(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[20]++;
    cov_qr1znkavv().s[235]++;
    try {
      const userId =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[236]++, req.user.id);
      const reconciliationData =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[237]++, req.body);
      const [reconciliation] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[238]++, await db.insert(asc606_reconciliation).values({
        ...reconciliationData,
        variance_amount: reconciliationData.total_recognized_revenue - reconciliationData.total_billed_revenue,
        variance_percentage: Math.round((reconciliationData.total_recognized_revenue - reconciliationData.total_billed_revenue) / reconciliationData.total_billed_revenue * 10000),
        status: 'DRAFT',
        created_by_id: userId
      }).returning());
      /* istanbul ignore next */
      cov_qr1znkavv().s[239]++;
      res.status(201).json({
        success: true,
        message: 'Reconciliation created successfully',
        data: reconciliation
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[240]++;
      logger.error('Error creating reconciliation:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[241]++;
      res.status(500).json({
        success: false,
        error: 'Failed to create reconciliation',
        message: error.message
      });
    }
  }

  /**
   * GET /api/asc606/reconciliation/:period
   * Get period reconciliation
   */
  async getReconciliation(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[21]++;
    cov_qr1znkavv().s[242]++;
    try {
      const {
        period
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[243]++, req.params);
      const [reconciliation] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[244]++, await db.select().from(asc606_reconciliation).where(eq(asc606_reconciliation.period_label, period)));
      /* istanbul ignore next */
      cov_qr1znkavv().s[245]++;
      if (!reconciliation) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[52][0]++;
        cov_qr1znkavv().s[246]++;
        return res.status(404).json({
          success: false,
          error: 'Reconciliation not found for period'
        });
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[52][1]++;
      }
      cov_qr1znkavv().s[247]++;
      res.json({
        success: true,
        data: reconciliation
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[248]++;
      logger.error('Error getting reconciliation:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[249]++;
      res.status(500).json({
        success: false,
        error: 'Failed to get reconciliation',
        message: error.message
      });
    }
  }

  // ============================================
  // HELPER METHODS
  // ============================================

  _getFiscalInfo(date) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[22]++;
    const year =
    /* istanbul ignore next */
    (cov_qr1znkavv().s[250]++, date.getFullYear());
    const month =
    /* istanbul ignore next */
    (cov_qr1znkavv().s[251]++, date.getMonth() + 1);
    const quarter =
    /* istanbul ignore next */
    (cov_qr1znkavv().s[252]++, Math.ceil(month / 3));
    /* istanbul ignore next */
    cov_qr1znkavv().s[253]++;
    return {
      fiscalYear: year,
      fiscalQuarter: quarter,
      fiscalMonth: month,
      periodLabel: `${year}-${String(month).padStart(2, '0')}`
    };
  }
  _isLeapYear(year) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[23]++;
    cov_qr1znkavv().s[254]++;
    return /* istanbul ignore next */(cov_qr1znkavv().b[53][0]++, year % 4 === 0) &&
    /* istanbul ignore next */
    (cov_qr1znkavv().b[53][1]++, year % 100 !== 0) ||
    /* istanbul ignore next */
    (cov_qr1znkavv().b[53][2]++, year % 400 === 0);
  }
  _getDaysInMonth(date) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[24]++;
    cov_qr1znkavv().s[255]++;
    return new Date(date.getFullYear(), date.getMonth() + 1, 0).getDate();
  }
  async _createAuditEntry(data) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[25]++;
    cov_qr1znkavv().s[256]++;
    try {
      /* istanbul ignore next */
      cov_qr1znkavv().s[257]++;
      await db.insert(asc606_calculation_audit).values({
        ...data,
        event_timestamp: new Date()
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[258]++;
      logger.error('Error creating audit entry:', error);
    }
  }
}
export default new ASC606Controller();
