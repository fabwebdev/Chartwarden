      const userId =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[43]++, req.user.id);
      const updateData =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[44]++, req.body);

      // Get current state for audit
      const [currentContract] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[45]++, await db.select().from(asc606_contracts).where(eq(asc606_contracts.id, parseInt(id))));
