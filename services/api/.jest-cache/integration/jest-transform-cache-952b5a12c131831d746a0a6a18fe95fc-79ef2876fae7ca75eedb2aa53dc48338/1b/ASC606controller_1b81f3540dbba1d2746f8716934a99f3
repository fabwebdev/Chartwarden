  /**
   * POST /api/asc606/contracts/:id/validate
   * Validate contract against ASC 606 criteria
   */
  async validateContract(req, res) {
    /* istanbul ignore next */
    cov_qr1znkavv().f[4]++;
    cov_qr1znkavv().s[54]++;
    try {
      const {
        id
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[55]++, req.params);
      const userId =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[56]++, req.user.id);
      const {
        validationNotes
      } =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[57]++, req.body);
      const [contract] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[58]++, await db.select().from(asc606_contracts).where(eq(asc606_contracts.id, parseInt(id))));
      /* istanbul ignore next */
      cov_qr1znkavv().s[59]++;
      if (!contract) {
        /* istanbul ignore next */
        cov_qr1znkavv().b[15][0]++;
        cov_qr1znkavv().s[60]++;
        return res.status(404).json({
          success: false,
          error: 'Contract not found'
        });
      } else
      /* istanbul ignore next */
      {
        cov_qr1znkavv().b[15][1]++;
      }

      // Check all 5 criteria
      const criteria =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[61]++, {
        approval_commitment: contract.criteria_approval_commitment,
        rights_identified: contract.criteria_rights_identified,
        payment_terms_identified: contract.criteria_payment_terms_identified,
        commercial_substance: contract.criteria_commercial_substance,
        collection_probable: contract.criteria_collection_probable
      });
      const isValid =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[62]++, Object.values(criteria).every(v => {
        /* istanbul ignore next */
        cov_qr1znkavv().f[5]++;
        cov_qr1znkavv().s[63]++;
        return v === true;
      }));
      const failedCriteria =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[64]++, Object.entries(criteria).filter(([, v]) => {
        /* istanbul ignore next */
        cov_qr1znkavv().f[6]++;
        cov_qr1znkavv().s[65]++;
        return !v;
      }).map(([k]) => {
        /* istanbul ignore next */
        cov_qr1znkavv().f[7]++;
        cov_qr1znkavv().s[66]++;
        return k;
      }));
      const [updatedContract] =
      /* istanbul ignore next */
      (cov_qr1znkavv().s[67]++, await db.update(asc606_contracts).set({
        is_valid_asc606_contract: isValid,
        validation_date: new Date(),
        validation_notes:
        /* istanbul ignore next */
        (cov_qr1znkavv().b[16][0]++, validationNotes) ||
        /* istanbul ignore next */
        (cov_qr1znkavv().b[16][1]++, null),
        contract_status: isValid ?
        /* istanbul ignore next */
        (cov_qr1znkavv().b[17][0]++, 'ACTIVE') :
        /* istanbul ignore next */
        (cov_qr1znkavv().b[17][1]++, 'PENDING_VALIDATION'),
        updated_by_id: userId,
        updated_at: new Date()
      }).where(eq(asc606_contracts.id, parseInt(id))).returning());
      /* istanbul ignore next */
      cov_qr1znkavv().s[68]++;
      res.json({
        success: true,
        message: isValid ?
        /* istanbul ignore next */
        (cov_qr1znkavv().b[18][0]++, 'Contract validated successfully') :
        /* istanbul ignore next */
        (cov_qr1znkavv().b[18][1]++, 'Contract validation failed'),
        data: {
          contract: updatedContract,
          criteria,
          isValid,
          failedCriteria
        }
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qr1znkavv().s[69]++;
      logger.error('Error validating ASC 606 contract:', error);
      /* istanbul ignore next */
      cov_qr1znkavv().s[70]++;
      res.status(500).json({
        success: false,
        error: 'Failed to validate contract',
        message: error.message
      });
    }
  }

