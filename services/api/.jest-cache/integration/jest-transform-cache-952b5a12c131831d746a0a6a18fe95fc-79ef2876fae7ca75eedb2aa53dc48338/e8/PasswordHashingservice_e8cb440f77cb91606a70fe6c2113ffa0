    /* istanbul ignore next */
    {
      cov_ykj1h6vwq().b[7][1]++;
    }

    // Check maximum length (DoS prevention)
    cov_ykj1h6vwq().s[34]++;
    if (password.length > this.policy.maxLength) {
      /* istanbul ignore next */
      cov_ykj1h6vwq().b[8][0]++;
      cov_ykj1h6vwq().s[35]++;
      throw new Error(`Password must not exceed ${this.policy.maxLength} characters`);
    } else
    /* istanbul ignore next */
    {
      cov_ykj1h6vwq().b[8][1]++;
    }

    // Unicode validation - ensure password can be normalized
    cov_ykj1h6vwq().s[36]++;
    try {
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[37]++;
      password.normalize('NFC');
    } catch {
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[38]++;
      throw new Error('Password contains invalid characters');
    }
  }

  /**
   * Check if a hash needs to be rehashed (e.g., parameters changed)
   *
   * @param {string} hash - Existing hash to check
   * @returns {Promise<boolean>} True if hash needs updating
   */
  async needsRehash(hash) {
    /* istanbul ignore next */
    cov_ykj1h6vwq().f[5]++;
    cov_ykj1h6vwq().s[39]++;
    try {
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[40]++;
      return argon2.needsRehash(hash, {
        type: this.config.type,
        memoryCost: this.config.memoryCost,
        timeCost: this.config.timeCost
      });
    } catch {
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[41]++;
      // If check fails, assume rehash is needed
      return true;
    }
  }

  /**
   * Check if a password was previously used by the user
   *
   * @param {string} userId - User ID to check history for
   * @param {string} newPassword - New password to check against history
   * @returns {Promise<{isReused: boolean, message: string}>} Reuse check result
   */
  async checkPasswordHistory(userId, newPassword) {
    /* istanbul ignore next */
    cov_ykj1h6vwq().f[6]++;
    cov_ykj1h6vwq().s[42]++;
    try {
      // Normalize password for consistent comparison
      const normalizedPassword =
      /* istanbul ignore next */
      (cov_ykj1h6vwq().s[43]++, newPassword.normalize('NFC'));

      // Fetch password history (most recent first)
      const history =
      /* istanbul ignore next */
      (cov_ykj1h6vwq().s[44]++, await db.select().from(passwordHistory).where(eq(passwordHistory.userId, userId)).orderBy(desc(passwordHistory.createdAt)).limit(this.policy.historyCount));

      // Check against each historical password
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[45]++;
      for (const record of history) {
        const isMatch =
        /* istanbul ignore next */
        (cov_ykj1h6vwq().s[46]++, await this.verifyPassword(normalizedPassword, record.passwordHash));
        /* istanbul ignore next */
        cov_ykj1h6vwq().s[47]++;
        if (isMatch) {
          /* istanbul ignore next */
          cov_ykj1h6vwq().b[9][0]++;
          cov_ykj1h6vwq().s[48]++;
          return {
            isReused: true,
            message: `Password cannot be the same as your last ${this.policy.historyCount} passwords`
          };
        } else
        /* istanbul ignore next */
        {
          cov_ykj1h6vwq().b[9][1]++;
        }
      }
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[49]++;
      return {
        isReused: false,
        message: 'Password is not in history'
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[50]++;
      logger.error('Password history check failed:', error.message);
      // On error, allow the password change (fail open for availability)
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[51]++;
      return {
        isReused: false,
        message: 'Could not verify password history',
        error: true
      };
    }
  }

  /**
   * Add a password to the user's history
   *
   * @param {string} userId - User ID
   * @param {string} passwordHash - Hash of the password to store
   * @returns {Promise<void>}
   */
  async addToPasswordHistory(userId, passwordHash) {
    /* istanbul ignore next */
    cov_ykj1h6vwq().f[7]++;
    cov_ykj1h6vwq().s[52]++;
    try {
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[53]++;
      // Add new password to history
      await db.insert(passwordHistory).values({
        userId,
        passwordHash,
        createdAt: new Date()
      });

      // Clean up old entries beyond the history limit
      const allHistory =
      /* istanbul ignore next */
      (cov_ykj1h6vwq().s[54]++, await db.select({
        id: passwordHistory.id
      }).from(passwordHistory).where(eq(passwordHistory.userId, userId)).orderBy(desc(passwordHistory.createdAt)));

      // Delete entries beyond the limit
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[55]++;
      if (allHistory.length > this.policy.historyCount) {
        /* istanbul ignore next */
        cov_ykj1h6vwq().b[10][0]++;
        const idsToDelete =
        /* istanbul ignore next */
        (cov_ykj1h6vwq().s[56]++, allHistory.slice(this.policy.historyCount).map(h => {
          /* istanbul ignore next */
          cov_ykj1h6vwq().f[8]++;
          cov_ykj1h6vwq().s[57]++;
          return h.id;
        }));
        /* istanbul ignore next */
        cov_ykj1h6vwq().s[58]++;
        for (const id of idsToDelete) {
          /* istanbul ignore next */
          cov_ykj1h6vwq().s[59]++;
          await db.delete(passwordHistory).where(eq(passwordHistory.id, id));
        }
      } else
      /* istanbul ignore next */
      {
        cov_ykj1h6vwq().b[10][1]++;
      }
      cov_ykj1h6vwq().s[60]++;
      logger.debug(`Password history updated for user ${userId}`);
    } catch (error) {
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[61]++;
      logger.error('Failed to update password history:', error.message);
      // Don't throw - password change should still succeed
    }
  }

  /**
   * Update user's password with full security checks
   *
   * @param {string} userId - User ID
   * @param {string} currentPassword - Current password for verification
   * @param {string} newPassword - New password to set
   * @returns {Promise<{success: boolean, message: string, errors?: string[]}>}
   */
  async updatePassword(userId, currentPassword, newPassword) {
    /* istanbul ignore next */
    cov_ykj1h6vwq().f[9]++;
    cov_ykj1h6vwq().s[62]++;
    try {
      // Fetch user
      const [user] =
      /* istanbul ignore next */
      (cov_ykj1h6vwq().s[63]++, await db.select().from(users).where(eq(users.id, userId)).limit(1));
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[64]++;
      if (!user) {
        /* istanbul ignore next */
        cov_ykj1h6vwq().b[11][0]++;
        cov_ykj1h6vwq().s[65]++;
        return {
          success: false,
          message: 'User not found'
        };
      } else
      /* istanbul ignore next */
      {
        cov_ykj1h6vwq().b[11][1]++;
      }

      // Verify current password
      const isCurrentValid =
      /* istanbul ignore next */
      (cov_ykj1h6vwq().s[66]++, await this.verifyPassword(currentPassword, user.password));
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[67]++;
      if (!isCurrentValid) {
        /* istanbul ignore next */
        cov_ykj1h6vwq().b[12][0]++;
        cov_ykj1h6vwq().s[68]++;
        return {
          success: false,
          message: 'Current password is incorrect'
        };
      } else
      /* istanbul ignore next */
      {
        cov_ykj1h6vwq().b[12][1]++;
      }

      // Validate new password format
      cov_ykj1h6vwq().s[69]++;
      try {
        /* istanbul ignore next */
        cov_ykj1h6vwq().s[70]++;
        this.validatePasswordFormat(newPassword);
      } catch (error) {
        /* istanbul ignore next */
        cov_ykj1h6vwq().s[71]++;
        return {
          success: false,
          message: error.message
        };
      }

      // Check password history
      const historyCheck =
      /* istanbul ignore next */
      (cov_ykj1h6vwq().s[72]++, await this.checkPasswordHistory(userId, newPassword));
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[73]++;
      if (historyCheck.isReused) {
        /* istanbul ignore next */
        cov_ykj1h6vwq().b[13][0]++;
        cov_ykj1h6vwq().s[74]++;
        return {
          success: false,
          message: historyCheck.message
        };
      } else
      /* istanbul ignore next */
      {
        cov_ykj1h6vwq().b[13][1]++;
      }

      // Hash new password
      const newHash =
      /* istanbul ignore next */
      (cov_ykj1h6vwq().s[75]++, await this.hashPassword(newPassword));

      // Add current password to history before updating
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[76]++;
      if (user.password) {
        /* istanbul ignore next */
        cov_ykj1h6vwq().b[14][0]++;
        cov_ykj1h6vwq().s[77]++;
        await this.addToPasswordHistory(userId, user.password);
      } else
      /* istanbul ignore next */
      {
        cov_ykj1h6vwq().b[14][1]++;
      }

      // Update user's password
      cov_ykj1h6vwq().s[78]++;
      await db.update(users).set({
        password: newHash,
        password_changed_at: new Date(),
        updatedAt: new Date()
      }).where(eq(users.id, userId));
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[79]++;
      logger.info(`Password updated successfully for user ${userId}`);
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[80]++;
      return {
        success: true,
        message: 'Password updated successfully'
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[81]++;
      logger.error('Password update failed:', error.message);
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[82]++;
      return {
        success: false,
        message: 'Failed to update password'
      };
    }
  }

  /**
   * Migrate a password hash from another algorithm (bcrypt, etc.) to Argon2id
   *
   * @param {string} userId - User ID
   * @param {string} plainPassword - Plain text password (from successful login)
   * @param {string} currentHash - Current hash (may be bcrypt or old argon2)
   * @returns {Promise<boolean>} True if migration was performed
   */
  async migratePasswordHash(userId, plainPassword, currentHash) {
    /* istanbul ignore next */
    cov_ykj1h6vwq().f[10]++;
    cov_ykj1h6vwq().s[83]++;
    try {
      // Check if current hash needs rehashing
      const needsMigration =
      /* istanbul ignore next */
      (cov_ykj1h6vwq().s[84]++, await this.needsRehash(currentHash));
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[85]++;
      if (needsMigration) {
        /* istanbul ignore next */
        cov_ykj1h6vwq().b[15][0]++;
        // Generate new Argon2id hash
        const newHash =
        /* istanbul ignore next */
        (cov_ykj1h6vwq().s[86]++, await this.hashPassword(plainPassword));

        // Update user's password hash
        /* istanbul ignore next */
        cov_ykj1h6vwq().s[87]++;
        await db.update(users).set({
          password: newHash,
          updatedAt: new Date()
        }).where(eq(users.id, userId));
        /* istanbul ignore next */
        cov_ykj1h6vwq().s[88]++;
        logger.info(`Password hash migrated to Argon2id for user ${userId}`);
        /* istanbul ignore next */
        cov_ykj1h6vwq().s[89]++;
        return true;
      } else
      /* istanbul ignore next */
      {
        cov_ykj1h6vwq().b[15][1]++;
      }
      cov_ykj1h6vwq().s[90]++;
      return false;
    } catch (error) {
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[91]++;
