      /* istanbul ignore next */
      cov_ykj1h6vwq().s[92]++;
      return false;
    }
  }

  /**
   * Generate a secure random password
   *
   * @param {number} length - Password length (default: 16)
   * @returns {string} Secure random password
   */
  generateSecurePassword(length =
  /* istanbul ignore next */
  (cov_ykj1h6vwq().b[16][0]++, 16)) {
    /* istanbul ignore next */
    cov_ykj1h6vwq().f[11]++;
    const lowercase =
    /* istanbul ignore next */
    (cov_ykj1h6vwq().s[93]++, 'abcdefghijklmnopqrstuvwxyz');
    const uppercase =
    /* istanbul ignore next */
    (cov_ykj1h6vwq().s[94]++, 'ABCDEFGHIJKLMNOPQRSTUVWXYZ');
    const numbers =
    /* istanbul ignore next */
    (cov_ykj1h6vwq().s[95]++, '0123456789');
    const special =
    /* istanbul ignore next */
    (cov_ykj1h6vwq().s[96]++, '!@#$%^&*()_+-=[]{}|;:,.<>?');
    const allChars =
    /* istanbul ignore next */
    (cov_ykj1h6vwq().s[97]++, lowercase + uppercase + numbers + special);

    // Ensure at least one of each type
    let password =
    /* istanbul ignore next */
    (cov_ykj1h6vwq().s[98]++, '');
    /* istanbul ignore next */
    cov_ykj1h6vwq().s[99]++;
    password += lowercase[crypto.randomInt(lowercase.length)];
    /* istanbul ignore next */
    cov_ykj1h6vwq().s[100]++;
    password += uppercase[crypto.randomInt(uppercase.length)];
    /* istanbul ignore next */
    cov_ykj1h6vwq().s[101]++;
    password += numbers[crypto.randomInt(numbers.length)];
    /* istanbul ignore next */
    cov_ykj1h6vwq().s[102]++;
    password += special[crypto.randomInt(special.length)];

    // Fill the rest randomly
    /* istanbul ignore next */
    cov_ykj1h6vwq().s[103]++;
    for (let i =
    /* istanbul ignore next */
    (cov_ykj1h6vwq().s[104]++, password.length); i < length; i++) {
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[105]++;
      password += allChars[crypto.randomInt(allChars.length)];
    }

    // Shuffle the password using Fisher-Yates
    const arr =
    /* istanbul ignore next */
    (cov_ykj1h6vwq().s[106]++, password.split(''));
    /* istanbul ignore next */
    cov_ykj1h6vwq().s[107]++;
    for (let i =
    /* istanbul ignore next */
    (cov_ykj1h6vwq().s[108]++, arr.length - 1); i > 0; i--) {
      const j =
      /* istanbul ignore next */
      (cov_ykj1h6vwq().s[109]++, crypto.randomInt(i + 1));
      /* istanbul ignore next */
      cov_ykj1h6vwq().s[110]++;
      [arr[i], arr[j]] = [arr[j], arr[i]];
    }
    /* istanbul ignore next */
    cov_ykj1h6vwq().s[111]++;
    return arr.join('');
  }

  /**
   * Get password policy for display to users
   *
   * @returns {Object} Human-readable password policy
   */
  getPasswordPolicy() {
    /* istanbul ignore next */
    cov_ykj1h6vwq().f[12]++;
    cov_ykj1h6vwq().s[112]++;
    return {
      minLength: this.policy.minLength,
      maxLength: this.policy.maxLength,
      requirements: [`Minimum ${this.policy.minLength} characters`, `Maximum ${this.policy.maxLength} characters`, 'At least 3 of: lowercase, uppercase, numbers, special characters', `Cannot reuse last ${this.policy.historyCount} passwords`, `Must change every ${this.policy.maxAgeInDays} days`],
      recommendations: ['Use a unique password not used on other sites', 'Consider using a password manager', 'Avoid personal information like names or birthdays', 'Longer passwords are generally stronger']
    };
  }
}

