      message: 'Validation test routes are active',
      environment:
      /* istanbul ignore next */
      (cov_1z7qx7488j().b[1][0]++, process.env.NODE_ENV) ||
      /* istanbul ignore next */
      (cov_1z7qx7488j().b[1][1]++, 'development')
    };
  });
}
export default validationTestRoutes;
