    cov_1fakmnwv44().s[7]++;
    if (typeof handler !== 'function') {
      /* istanbul ignore next */
      cov_1fakmnwv44().b[2][0]++;
      cov_1fakmnwv44().s[8]++;
      throw new Error('Job handler must be a function');
    } else
    /* istanbul ignore next */
    {
      cov_1fakmnwv44().b[2][1]++;
    }
    cov_1fakmnwv44().s[9]++;
    if (this.registeredJobs.has(jobName)) {
      /* istanbul ignore next */
      cov_1fakmnwv44().b[3][0]++;
      cov_1fakmnwv44().s[10]++;
      throw new Error(`Job "${jobName}" is already registered`);
    } else
    /* istanbul ignore next */
    {
      cov_1fakmnwv44().b[3][1]++;
    }
    cov_1fakmnwv44().s[11]++;
    this.registeredJobs.set(jobName, handler);
  }

  /**
   * Unregister a job handler
   * @param {string} jobName - Job name to unregister
   */
  unregisterJob(jobName) {
    /* istanbul ignore next */
    cov_1fakmnwv44().f[2]++;
    cov_1fakmnwv44().s[12]++;
    this.registeredJobs.delete(jobName);
  }

  /**
   * Get all registered job names
   * @returns {string[]} Array of registered job names
   */
  getRegisteredJobNames() {
    /* istanbul ignore next */
    cov_1fakmnwv44().f[3]++;
    cov_1fakmnwv44().s[13]++;
    return Array.from(this.registeredJobs.keys());
  }

  /**
   * Push a job onto the queue
   * @param {string} queue - The queue name
   * @param {string} jobName - The registered job name to execute
   * @param {Object} data - The job data
   * @returns {string} Job ID
   */
  async push(queue, jobName, data =
  /* istanbul ignore next */
  (cov_1fakmnwv44().b[4][0]++, {})) {
    /* istanbul ignore next */
    cov_1fakmnwv44().f[4]++;
    cov_1fakmnwv44().s[14]++;
    // Validate job is registered
    if (typeof jobName !== 'string') {
      /* istanbul ignore next */
      cov_1fakmnwv44().b[5][0]++;
      cov_1fakmnwv44().s[15]++;
      throw new Error('Job name must be a string (not a function). Use registerJob() first.');
    } else
    /* istanbul ignore next */
    {
      cov_1fakmnwv44().b[5][1]++;
    }
    cov_1fakmnwv44().s[16]++;
    if (!this.registeredJobs.has(jobName)) {
      /* istanbul ignore next */
      cov_1fakmnwv44().b[6][0]++;
      cov_1fakmnwv44().s[17]++;
      throw new Error(`Job "${jobName}" is not registered. Available jobs: ${this.getRegisteredJobNames().join(', ')}`);
    } else
    /* istanbul ignore next */
    {
      cov_1fakmnwv44().b[6][1]++;
    }
    const jobId =
    /* istanbul ignore next */
    (cov_1fakmnwv44().s[18]++, this.generateJobId());
    const jobData =
    /* istanbul ignore next */
    (cov_1fakmnwv44().s[19]++, {
      id: jobId,
      queue: queue,
      jobName: jobName,
      // Store job name instead of function code
      data: data,
      attempts: 0,
      createdAt: new Date()
    });

    // For sync driver, execute immediately
    /* istanbul ignore next */
    cov_1fakmnwv44().s[20]++;
    if (this.connection === 'sync') {
      /* istanbul ignore next */
      cov_1fakmnwv44().b[7][0]++;
      cov_1fakmnwv44().s[21]++;
      try {
        const handler =
        /* istanbul ignore next */
        (cov_1fakmnwv44().s[22]++, this.registeredJobs.get(jobName));
        /* istanbul ignore next */
        cov_1fakmnwv44().s[23]++;
        await handler(data);
        /* istanbul ignore next */
        cov_1fakmnwv44().s[24]++;
        return jobId;
      } catch (error) {
        /* istanbul ignore next */
        cov_1fakmnwv44().s[25]++;
        this.handleFailedJob(jobData, error);
        /* istanbul ignore next */
        cov_1fakmnwv44().s[26]++;
        throw error;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1fakmnwv44().b[7][1]++;
    }

    // For other drivers, store the job
    cov_1fakmnwv44().s[27]++;
    this.jobs.push(jobData);
    /* istanbul ignore next */
    cov_1fakmnwv44().s[28]++;
    return jobId;
  }

  /**
   * Process jobs in the queue
   */
  async process() {
    /* istanbul ignore next */
    cov_1fakmnwv44().f[5]++;
    cov_1fakmnwv44().s[29]++;
    // For sync driver, there's nothing to process
    if (this.connection === 'sync') {
      /* istanbul ignore next */
      cov_1fakmnwv44().b[8][0]++;
      cov_1fakmnwv44().s[30]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1fakmnwv44().b[8][1]++;
    }

    // Process jobs
    cov_1fakmnwv44().s[31]++;
    for (let i =
    /* istanbul ignore next */
    (cov_1fakmnwv44().s[32]++, 0); i < this.jobs.length; i++) {
      const jobData =
      /* istanbul ignore next */
      (cov_1fakmnwv44().s[33]++, this.jobs[i]);
      /* istanbul ignore next */
      cov_1fakmnwv44().s[34]++;
      try {
        // Get the registered job handler (NO EVAL - SECURE!)
        const handler =
        /* istanbul ignore next */
        (cov_1fakmnwv44().s[35]++, this.registeredJobs.get(jobData.jobName));
        /* istanbul ignore next */
        cov_1fakmnwv44().s[36]++;
        if (!handler) {
          /* istanbul ignore next */
          cov_1fakmnwv44().b[9][0]++;
          cov_1fakmnwv44().s[37]++;
          throw new Error(`Job "${jobData.jobName}" is not registered`);
        } else
        /* istanbul ignore next */
        {
          cov_1fakmnwv44().b[9][1]++;
        }
        cov_1fakmnwv44().s[38]++;
        await handler(jobData.data);

        // Remove successful job
        /* istanbul ignore next */
        cov_1fakmnwv44().s[39]++;
        this.jobs.splice(i, 1);
        /* istanbul ignore next */
        cov_1fakmnwv44().s[40]++;
        i--;
      } catch (error) {
        /* istanbul ignore next */
        cov_1fakmnwv44().s[41]++;
        this.handleFailedJob(jobData, error);

        // Remove failed job
        /* istanbul ignore next */
        cov_1fakmnwv44().s[42]++;
        this.jobs.splice(i, 1);
        /* istanbul ignore next */
        cov_1fakmnwv44().s[43]++;
        i--;
      }
    }
  }

  /**
   * Handle a failed job
   * @param {Object} jobData - The job data
   * @param {Error} error - The error that occurred
   */
  handleFailedJob(jobData, error) {
    /* istanbul ignore next */
    cov_1fakmnwv44().f[6]++;
    const failedJob =
    /* istanbul ignore next */
    (cov_1fakmnwv44().s[44]++, {
      ...jobData,
      failedAt: new Date(),
      exception: error.message,
      trace: error.stack
    });
    /* istanbul ignore next */
    cov_1fakmnwv44().s[45]++;
    this.failedJobs.push(failedJob);
    /* istanbul ignore next */
    cov_1fakmnwv44().s[46]++;
    logger.error(`Job ${jobData.id} failed:`, error);
  }

  /**
   * Generate a unique job ID
   * @returns {string} Job ID
   */
  generateJobId() {
    /* istanbul ignore next */
    cov_1fakmnwv44().f[7]++;
    cov_1fakmnwv44().s[47]++;
    return 'job_' + Date.now() + '_' + Math.random().toString(36).substr(2, 9);
  }

  /**
   * Get the number of pending jobs
   * @returns {number} Number of pending jobs
   */
  pending() {
    /* istanbul ignore next */
    cov_1fakmnwv44().f[8]++;
    cov_1fakmnwv44().s[48]++;
    return this.jobs.length;
  }

  /**
   * Get the number of failed jobs
   * @returns {number} Number of failed jobs
   */
  failed() {
    /* istanbul ignore next */
    cov_1fakmnwv44().f[9]++;
    cov_1fakmnwv44().s[49]++;
    return this.failedJobs.length;
  }

  /**
   * Get all jobs
   * @returns {Array} All jobs
   */
  getJobs() {
    /* istanbul ignore next */
    cov_1fakmnwv44().f[10]++;
    cov_1fakmnwv44().s[50]++;
    return this.jobs;
  }

  /**
   * Get all failed jobs
   * @returns {Array} All failed jobs
   */
  getFailedJobs() {
    /* istanbul ignore next */
    cov_1fakmnwv44().f[11]++;
    cov_1fakmnwv44().s[51]++;
    return this.failedJobs;
  }
}
export default new QueueService();
