  /**
   * Hash a value
   * @param {string} value - The value to hash
   * @returns {Promise<string>} The hashed value
   */
  async make(value) {
    /* istanbul ignore next */
    cov_3rf0xbf9s().f[1]++;
    cov_3rf0xbf9s().s[1]++;
    switch (this.config.driver) {
      case 'bcrypt':
        /* istanbul ignore next */
        cov_3rf0xbf9s().b[0][0]++;
        cov_3rf0xbf9s().s[2]++;
