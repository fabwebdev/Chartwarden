import argon2 from 'argon2';
import hashingConfig from '../config/hashing.config.js';
class HashService {
  constructor() {
    /* istanbul ignore next */
    cov_3rf0xbf9s().f[0]++;
    cov_3rf0xbf9s().s[0]++;
    this.config = hashingConfig;
