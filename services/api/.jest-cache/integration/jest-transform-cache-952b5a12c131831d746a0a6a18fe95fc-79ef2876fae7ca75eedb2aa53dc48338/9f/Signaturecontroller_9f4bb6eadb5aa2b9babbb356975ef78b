import { db } from "../../config/db.drizzle.js";
import { signature } from "../../db/schemas/signature.schema.js";
import { eq } from "drizzle-orm";
import { logger } from '../../utils/logger.js';
class SignatureController {
  // Get all signatures
  async index(request, reply) {
    /* istanbul ignore next */
    cov_1dotubw5du().f[0]++;
    cov_1dotubw5du().s[0]++;
    try {
      const signatures =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[1]++, await db.select({
        id: signature.id,
        patient_id: signature.patient_id,
        signature_name: signature.signature_name,
        createdAt: signature.createdAt,
        updatedAt: signature.updatedAt
      }).from(signature));
      /* istanbul ignore next */
      cov_1dotubw5du().s[2]++;
      reply.code(200);
      /* istanbul ignore next */
      cov_1dotubw5du().s[3]++;
      return signatures;
    } catch (error) {
      /* istanbul ignore next */
      cov_1dotubw5du().s[4]++;
      logger.error("Error fetching signatures:", error);
      /* istanbul ignore next */
      cov_1dotubw5du().s[5]++;
      reply.code(500);
      /* istanbul ignore next */
      cov_1dotubw5du().s[6]++;
      return {
        message: "Internal server error",
        error: error.message
      };
    }
  }
  // Store or update signature
  async store(request, reply) {
    /* istanbul ignore next */
    cov_1dotubw5du().f[1]++;
    cov_1dotubw5du().s[7]++;
    try {
      const {
        patient_id,
        signature_name
      } =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[8]++, request.body);

      // Validate required fields
      /* istanbul ignore next */
      cov_1dotubw5du().s[9]++;
      if (!patient_id) {
        /* istanbul ignore next */
        cov_1dotubw5du().b[0][0]++;
        cov_1dotubw5du().s[10]++;
        reply.code(400);
        /* istanbul ignore next */
        cov_1dotubw5du().s[11]++;
        return {
          message: "Patient ID is required"
        };
      } else
      /* istanbul ignore next */
      {
        cov_1dotubw5du().b[0][1]++;
      }

      // signature_name is optional (can be null to clear signature)

      // Convert patient_id to number
      const patientIdNum =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[12]++, typeof patient_id === 'string' ?
      /* istanbul ignore next */
      (cov_1dotubw5du().b[1][0]++, parseInt(patient_id)) :
      /* istanbul ignore next */
      (cov_1dotubw5du().b[1][1]++, patient_id));
      /* istanbul ignore next */
      cov_1dotubw5du().s[13]++;
      if (isNaN(patientIdNum)) {
        /* istanbul ignore next */
        cov_1dotubw5du().b[2][0]++;
        cov_1dotubw5du().s[14]++;
        reply.code(400);
        /* istanbul ignore next */
        cov_1dotubw5du().s[15]++;
        return {
          message: "Invalid patient ID"
        };
      } else
      /* istanbul ignore next */
      {
        cov_1dotubw5du().b[2][1]++;
      }

      // Check if signature already exists for this patient
      const existingSignatures =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[16]++, await db.select({
        id: signature.id
      }).from(signature).where(eq(signature.patient_id, patientIdNum)).limit(1));
      const existingSignature =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[17]++, existingSignatures[0]);
      let result;
      const now =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[18]++, new Date());
      /* istanbul ignore next */
      cov_1dotubw5du().s[19]++;
      if (existingSignature) {
        /* istanbul ignore next */
        cov_1dotubw5du().b[3][0]++;
        cov_1dotubw5du().s[20]++;
        // Update existing signature
        result = await db.update(signature).set({
          signature_name: signature_name,
          updatedAt: now
        }).where(eq(signature.patient_id, patientIdNum)).returning();
        /* istanbul ignore next */
        cov_1dotubw5du().s[21]++;
        result = result[0];
      } else {
        /* istanbul ignore next */
        cov_1dotubw5du().b[3][1]++;
        cov_1dotubw5du().s[22]++;
        // Create new signature
        result = await db.insert(signature).values({
          patient_id: patientIdNum,
          signature_name: signature_name,
          createdAt: now,
          updatedAt: now
        }).returning();
        /* istanbul ignore next */
        cov_1dotubw5du().s[23]++;
        result = result[0];
      }
      /* istanbul ignore next */
      cov_1dotubw5du().s[24]++;
      reply.code(201);
      /* istanbul ignore next */
      cov_1dotubw5du().s[25]++;
      return {
        message: "Signature créée ou mise à jour avec succès.",
        data: result
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1dotubw5du().s[26]++;
      logger.error("Error saving signature:", error);
      /* istanbul ignore next */
      cov_1dotubw5du().s[27]++;
      reply.code(500);
      /* istanbul ignore next */
      cov_1dotubw5du().s[28]++;
      return {
        message: "Internal server error",
        error: error.message
      };
    }
  }

  // Show signature for a specific patient
  // Returns empty data if no signature exists (to allow frontend to render form)
