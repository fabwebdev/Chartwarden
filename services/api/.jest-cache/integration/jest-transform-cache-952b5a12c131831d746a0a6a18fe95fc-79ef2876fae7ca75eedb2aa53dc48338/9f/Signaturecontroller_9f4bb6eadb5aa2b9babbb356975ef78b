      } =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[30]++, request.params);
      const patientId =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[31]++, parseInt(id));
      /* istanbul ignore next */
      cov_1dotubw5du().s[32]++;
      if (isNaN(patientId)) {
        /* istanbul ignore next */
        cov_1dotubw5du().b[4][0]++;
        cov_1dotubw5du().s[33]++;
        reply.code(400);
        /* istanbul ignore next */
        cov_1dotubw5du().s[34]++;
        return {
          error: "Invalid patient ID"
        };
      } else
      /* istanbul ignore next */
      {
        cov_1dotubw5du().b[4][1]++;
      }
      const signatures =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[35]++, await db.select({
        id: signature.id,
        patient_id: signature.patient_id,
        signature_name: signature.signature_name,
        createdAt: signature.createdAt,
        updatedAt: signature.updatedAt
      }).from(signature).where(eq(signature.patient_id, patientId)).limit(1));
      const signatureRecord =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[36]++, signatures[0]);
      /* istanbul ignore next */
      cov_1dotubw5du().s[37]++;
      if (!signatureRecord) {
        /* istanbul ignore next */
        cov_1dotubw5du().b[5][0]++;
        cov_1dotubw5du().s[38]++;
        // Return 200 with empty data instead of 404, so frontend can render form
        reply.code(200);
        /* istanbul ignore next */
        cov_1dotubw5du().s[39]++;
        return {
          id: null,
          patient_id: patientId,
          signature_name: null,
          createdAt: null,
          updatedAt: null
        };
      } else
      /* istanbul ignore next */
      {
        cov_1dotubw5du().b[5][1]++;
      }
      cov_1dotubw5du().s[40]++;
      reply.code(200);
      /* istanbul ignore next */
      cov_1dotubw5du().s[41]++;
      return signatureRecord;
    } catch (error) {
      /* istanbul ignore next */
      cov_1dotubw5du().s[42]++;
      logger.error("Error fetching signature:", error);
      /* istanbul ignore next */
      cov_1dotubw5du().s[43]++;
      reply.code(500);
      /* istanbul ignore next */
      cov_1dotubw5du().s[44]++;
      return {
        message: "Internal server error",
        error: error.message
      };
    }
  }

  // Delete a specific signature
  async delete(request, reply) {
    /* istanbul ignore next */
    cov_1dotubw5du().f[3]++;
    cov_1dotubw5du().s[45]++;
    try {
      const {
        id
      } =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[46]++, request.params);

      // First find the signature
      const signatures =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[47]++, await db.select({
        id: signature.id
      }).from(signature).where(eq(signature.id, id)).limit(1));
      const signatureRecord =
      /* istanbul ignore next */
      (cov_1dotubw5du().s[48]++, signatures[0]);
      /* istanbul ignore next */
      cov_1dotubw5du().s[49]++;
      if (!signatureRecord) {
        /* istanbul ignore next */
        cov_1dotubw5du().b[6][0]++;
        cov_1dotubw5du().s[50]++;
        reply.code(404);
        /* istanbul ignore next */
        cov_1dotubw5du().s[51]++;
        return {
          message: "La signature n'existe pas",
          status: 404
        };
      } else
      /* istanbul ignore next */
      {
        cov_1dotubw5du().b[6][1]++;
      }

      // Delete the signature
      cov_1dotubw5du().s[52]++;
      await db.delete(signature).where(eq(signature.id, id));
      /* istanbul ignore next */
      cov_1dotubw5du().s[53]++;
      reply.code(200);
      /* istanbul ignore next */
      cov_1dotubw5du().s[54]++;
      return {
        message: "Signature supprimée avec succès.",
        status: 200
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1dotubw5du().s[55]++;
      logger.error("Error deleting signature:", error);
      /* istanbul ignore next */
      cov_1dotubw5du().s[56]++;
      reply.code(500);
      /* istanbul ignore next */
      cov_1dotubw5du().s[57]++;
      return {
        message: "Erreur lors de la suppression de la signature.",
        status: 500,
        error: error.message
      };
    }
  }
}
export default new SignatureController();
