    /* istanbul ignore next */
    (cov_n45m2ivy().b[1][0]++, process.env.APP_URL) ||
    /* istanbul ignore next */
    (cov_n45m2ivy().b[1][1]++, 'http://localhost:3000')),
    supportEmail =
    /* istanbul ignore next */
    (cov_n45m2ivy().b[2][0]++,
    /* istanbul ignore next */
    (cov_n45m2ivy().b[3][0]++, process.env.SUPPORT_EMAIL) ||
    /* istanbul ignore next */
    (cov_n45m2ivy().b[3][1]++, 'support@chartwarden.com'))
