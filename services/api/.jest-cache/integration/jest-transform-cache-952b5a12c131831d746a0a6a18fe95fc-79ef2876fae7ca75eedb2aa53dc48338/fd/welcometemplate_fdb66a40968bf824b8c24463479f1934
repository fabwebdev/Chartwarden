    supportEmail =
    /* istanbul ignore next */
    (cov_n45m2ivy().b[6][0]++,
    /* istanbul ignore next */
    (cov_n45m2ivy().b[7][0]++, process.env.SUPPORT_EMAIL) ||
    /* istanbul ignore next */
    (cov_n45m2ivy().b[7][1]++, 'support@chartwarden.com'))
