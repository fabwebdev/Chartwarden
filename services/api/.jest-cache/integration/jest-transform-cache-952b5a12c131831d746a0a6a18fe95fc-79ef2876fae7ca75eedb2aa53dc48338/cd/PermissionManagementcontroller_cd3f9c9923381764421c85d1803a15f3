/**
 * Permission Management and Role Assignment Controller
 *
 * Provides comprehensive permission and role management operations including:
 * - Permission CRUD with soft delete and audit trail
 * - Role Assignment operations with effective date tracking
 * - Permission-Role Association management
 * - User-Role queries and permission checks
 * - Authorization and privilege escalation prevention
 * - Full audit logging for compliance
 *
 * @module PermissionManagement.controller
 */

import { db } from "../config/db.drizzle.js";
import { permissions, roles, users, audit_logs } from "../db/schemas/index.js";
import { role_has_permissions, user_has_roles } from "../db/schemas/index.js";
import { eq, and, or, like, inArray, desc, asc, sql, isNull, isNotNull } from "drizzle-orm";
import { logger } from '../utils/logger.js';
import { clearPermissionCache } from '../middleware/rbac.middleware.js';

// ============================================================================
// UTILITY FUNCTIONS
// ============================================================================

/**
 * Create an audit log entry for permission/role operations
 * @param {Object} params - Audit log parameters
 * @param {string} params.userId - The user performing the action
 * @param {string} params.action - The action performed (CREATE, UPDATE, DELETE, etc.)
 * @param {string} params.resourceType - Type of resource (permission, role, user_role, role_permission)
 * @param {string|number} params.resourceId - ID of the affected resource
 * @param {Object} params.oldValue - Previous state (for updates/deletes)
 * @param {Object} params.newValue - New state (for creates/updates)
 * @param {Object} params.metadata - Additional context
 * @param {string} params.ipAddress - Client IP address
 * @param {string} params.userAgent - Client user agent
 */
async function createAuditLog({
  userId,
  action,
  resourceType,
  resourceId,
  oldValue =
  /* istanbul ignore next */
  (cov_w48otqzfm().b[0][0]++, null),
  newValue =
  /* istanbul ignore next */
  (cov_w48otqzfm().b[1][0]++, null),
  metadata =
  /* istanbul ignore next */
  (cov_w48otqzfm().b[2][0]++, null),
  ipAddress =
  /* istanbul ignore next */
  (cov_w48otqzfm().b[3][0]++, null),
  userAgent =
  /* istanbul ignore next */
  (cov_w48otqzfm().b[4][0]++, null)
}) {
  /* istanbul ignore next */
  cov_w48otqzfm().f[0]++;
  cov_w48otqzfm().s[0]++;
  try {
    /* istanbul ignore next */
    cov_w48otqzfm().s[1]++;
    await db.insert(audit_logs).values({
      user_id: userId,
      action,
      resource_type: resourceType,
      resource_id: resourceId?.toString(),
      old_value: oldValue ?
      /* istanbul ignore next */
      (cov_w48otqzfm().b[5][0]++, JSON.stringify(oldValue)) :
      /* istanbul ignore next */
      (cov_w48otqzfm().b[5][1]++, null),
      new_value: newValue ?
      /* istanbul ignore next */
      (cov_w48otqzfm().b[6][0]++, JSON.stringify(newValue)) :
      /* istanbul ignore next */
      (cov_w48otqzfm().b[6][1]++, null),
      metadata: metadata ?
      /* istanbul ignore next */
      (cov_w48otqzfm().b[7][0]++, JSON.stringify(metadata)) :
      /* istanbul ignore next */
      (cov_w48otqzfm().b[7][1]++, null),
      ip_address: ipAddress,
      user_agent: userAgent,
      status: 'success'
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[2]++;
    logger.error('Error creating audit log:', error);
    // Don't throw - audit logging should not break the main operation
  }
}

/**
 * Extract request context for audit logging
 * @param {Object} request - Fastify request object
 * @returns {Object} Context object with userId, ipAddress, userAgent
 */
function getRequestContext(request) {
  /* istanbul ignore next */
  cov_w48otqzfm().f[1]++;
  cov_w48otqzfm().s[3]++;
  return {
    userId:
    /* istanbul ignore next */
    (cov_w48otqzfm().b[8][0]++, request.user?.id) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[8][1]++, null),
    ipAddress:
    /* istanbul ignore next */
    (cov_w48otqzfm().b[9][0]++, request.ip) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[9][1]++, request.headers['x-forwarded-for']) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[9][2]++, null),
    userAgent:
    /* istanbul ignore next */
    (cov_w48otqzfm().b[10][0]++, request.headers['user-agent']) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[10][1]++, null)
  };
}

/**
 * Build pagination metadata
 * @param {number} total - Total number of items
 * @param {number} page - Current page (1-based)
 * @param {number} perPage - Items per page
 * @returns {Object} Pagination metadata
 */
function buildPaginationMeta(total, page, perPage) {
  /* istanbul ignore next */
  cov_w48otqzfm().f[2]++;
  const totalPages =
  /* istanbul ignore next */
  (cov_w48otqzfm().s[4]++, Math.ceil(total / perPage));
  /* istanbul ignore next */
  cov_w48otqzfm().s[5]++;
  return {
    total,
    page,
    per_page: perPage,
    total_pages: totalPages,
    has_more: page < totalPages,
    has_previous: page > 1
  };
}

/**
 * Get user's highest role hierarchy level (for privilege escalation prevention)
 * @param {string} userId - User ID
 * @returns {Promise<number>} Hierarchy level (lower = higher privilege)
 */
async function getUserHighestPrivilegeLevel(userId) {
  /* istanbul ignore next */
  cov_w48otqzfm().f[3]++;
  const userRolesData =
  /* istanbul ignore next */
  (cov_w48otqzfm().s[6]++, await db.select({
    hierarchy_level: roles.hierarchy_level
  }).from(user_has_roles).innerJoin(roles, eq(user_has_roles.role_id, roles.id)).where(and(eq(user_has_roles.user_id, userId), eq(roles.is_active, true))));
  /* istanbul ignore next */
  cov_w48otqzfm().s[7]++;
  if (userRolesData.length === 0) {
    /* istanbul ignore next */
    cov_w48otqzfm().b[11][0]++;
    cov_w48otqzfm().s[8]++;
    return 1000; // No roles = lowest privilege
  } else
  /* istanbul ignore next */
  {
    cov_w48otqzfm().b[11][1]++;
  }
  cov_w48otqzfm().s[9]++;
  return Math.min(...userRolesData.map(r => {
    /* istanbul ignore next */
    cov_w48otqzfm().f[4]++;
    cov_w48otqzfm().s[10]++;
    return r.hierarchy_level;
  }));
}

