    const pageNum =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[14]++, Math.max(1, parseInt(page)));
    const perPage =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[15]++, Math.min(100, Math.max(1, parseInt(per_page))));
    const offset =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[16]++, (pageNum - 1) * perPage);

    // Build where conditions
    const conditions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[17]++, []);
    /* istanbul ignore next */
    cov_w48otqzfm().s[18]++;
    if (search) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[21][0]++;
      cov_w48otqzfm().s[19]++;
      conditions.push(or(like(permissions.name, `%${search}%`), like(permissions.description, `%${search}%`)));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[21][1]++;
    }
    cov_w48otqzfm().s[20]++;
    if (resource) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[22][0]++;
      cov_w48otqzfm().s[21]++;
      conditions.push(eq(permissions.resource, resource));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[22][1]++;
    }
    cov_w48otqzfm().s[22]++;
    if (action) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[23][0]++;
      cov_w48otqzfm().s[23]++;
      conditions.push(eq(permissions.action, action));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[23][1]++;
    }
    cov_w48otqzfm().s[24]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[25][0]++, is_active !== null) &&
    /* istanbul ignore next */
    (cov_w48otqzfm().b[25][1]++, is_active !== '')) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[24][0]++;
      cov_w48otqzfm().s[25]++;
      conditions.push(eq(permissions.is_active,
      /* istanbul ignore next */
      (cov_w48otqzfm().b[26][0]++, is_active === 'true') ||
      /* istanbul ignore next */
      (cov_w48otqzfm().b[26][1]++, is_active === true)));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[24][1]++;
    }
    cov_w48otqzfm().s[26]++;
    if (guard_name) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[27][0]++;
      cov_w48otqzfm().s[27]++;
      conditions.push(eq(permissions.guard_name, guard_name));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[27][1]++;
    }
    const whereClause =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[28]++, conditions.length > 0 ?
    /* istanbul ignore next */
    (cov_w48otqzfm().b[28][0]++, and(...conditions)) :
    /* istanbul ignore next */
    (cov_w48otqzfm().b[28][1]++, undefined));

    // Get total count
    const countResult =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[29]++, await db.select({
      count: sql`count(*)`
    }).from(permissions).where(whereClause));
    const total =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[30]++, parseInt(countResult[0].count));

    // Determine sort column and direction
    const sortColumn =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[31]++,
    /* istanbul ignore next */
    (cov_w48otqzfm().b[29][0]++, {
      'name': permissions.name,
      'resource': permissions.resource,
      'action': permissions.action,
      'created_at': permissions.createdAt,
      'updated_at': permissions.updatedAt
    }[sort_by]) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[29][1]++, permissions.createdAt));
    const sortDirection =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[32]++, sort_order === 'asc' ?
    /* istanbul ignore next */
    (cov_w48otqzfm().b[30][0]++, asc) :
    /* istanbul ignore next */
    (cov_w48otqzfm().b[30][1]++, desc));

    // Get paginated permissions
    const permissionsList =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[33]++, await db.select({
      id: permissions.id,
      name: permissions.name,
      resource: permissions.resource,
      action: permissions.action,
      description: permissions.description,
      guard_name: permissions.guard_name,
      is_active: permissions.is_active,
      created_at: permissions.createdAt,
      updated_at: permissions.updatedAt
    }).from(permissions).where(whereClause).orderBy(sortDirection(sortColumn)).limit(perPage).offset(offset));
    /* istanbul ignore next */
    cov_w48otqzfm().s[34]++;
    return {
      status: 200,
      data: permissionsList,
      meta: buildPaginationMeta(total, pageNum, perPage)
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[35]++;
    logger.error("Error fetching permissions:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[36]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[37]++;
    return {
      status: 500,
      message: "Server error while fetching permissions"
    };
  }
};

/**
 * Get permission list (names only) for dropdowns
 * @route GET /permissions/list
 */
/* istanbul ignore next */
cov_w48otqzfm().s[38]++;
export const getPermissionList = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[6]++;
  cov_w48otqzfm().s[39]++;
  try {
    const {
      is_active =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[31][0]++, true)
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[40]++, request.query);
    const conditions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[41]++, []);
    /* istanbul ignore next */
    cov_w48otqzfm().s[42]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[33][0]++, is_active !== null) &&
    /* istanbul ignore next */
    (cov_w48otqzfm().b[33][1]++, is_active !== '')) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[32][0]++;
      cov_w48otqzfm().s[43]++;
      conditions.push(eq(permissions.is_active,
      /* istanbul ignore next */
      (cov_w48otqzfm().b[34][0]++, is_active === 'true') ||
      /* istanbul ignore next */
      (cov_w48otqzfm().b[34][1]++, is_active === true)));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[32][1]++;
    }
    const permissionsList =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[44]++, await db.select({
      id: permissions.id,
      name: permissions.name,
      resource: permissions.resource,
      action: permissions.action
    }).from(permissions).where(conditions.length > 0 ?
    /* istanbul ignore next */
    (cov_w48otqzfm().b[35][0]++, and(...conditions)) :
    /* istanbul ignore next */
    (cov_w48otqzfm().b[35][1]++, undefined)).orderBy(asc(permissions.name)));
    /* istanbul ignore next */
    cov_w48otqzfm().s[45]++;
    return {
      status: 200,
      data: permissionsList
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[46]++;
    logger.error("Error fetching permission list:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[47]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[48]++;
    return {
      status: 500,
      message: "Server error while fetching permission list"
    };
  }
};

/**
 * Create a new permission
 * @route POST /permissions
 */
/* istanbul ignore next */
cov_w48otqzfm().s[49]++;
export const createPermission = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[7]++;
  cov_w48otqzfm().s[50]++;
  try {
    const {
      name,
      resource,
      action,
      description,
      guard_name
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[51]++, request.body);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[52]++, getRequestContext(request));

    // Validation
    const errors =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[53]++, []);
    /* istanbul ignore next */
    cov_w48otqzfm().s[54]++;
    if (!name) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[36][0]++;
      cov_w48otqzfm().s[55]++;
      errors.push({
        field: "name",
        message: "Permission name is required"
      });
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[36][1]++;
    }
    cov_w48otqzfm().s[56]++;
    if (!resource) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[37][0]++;
      cov_w48otqzfm().s[57]++;
      errors.push({
        field: "resource",
        message: "Resource is required"
      });
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[37][1]++;
    }
    cov_w48otqzfm().s[58]++;
    if (!action) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[38][0]++;
      cov_w48otqzfm().s[59]++;
      errors.push({
        field: "action",
        message: "Action is required"
      });
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[38][1]++;
    }
    cov_w48otqzfm().s[60]++;
    if (errors.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[39][0]++;
      cov_w48otqzfm().s[61]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[62]++;
      return {
        status: 400,
        message: "Validation failed",
        errors
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[39][1]++;
    }

    // Validate action type
    const validActions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[63]++, ['create', 'read', 'update', 'delete', 'manage', 'view', 'verify', 'process']);
    /* istanbul ignore next */
    cov_w48otqzfm().s[64]++;
    if (!validActions.includes(action.toLowerCase())) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[40][0]++;
      cov_w48otqzfm().s[65]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[66]++;
      return {
        status: 400,
        message: "Validation failed",
        errors: [{
          field: "action",
          message: `Action must be one of: ${validActions.join(', ')}`
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[40][1]++;
    }

    // Check for duplicate name
    const existingByName =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[67]++, await db.select({
      id: permissions.id
    }).from(permissions).where(eq(permissions.name, name)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[68]++;
    if (existingByName.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[41][0]++;
      cov_w48otqzfm().s[69]++;
      reply.code(409);
      /* istanbul ignore next */
      cov_w48otqzfm().s[70]++;
      return {
        status: 409,
        message: "Conflict: Permission with this name already exists",
        errors: [{
          field: "name",
          message: "Permission name must be unique"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[41][1]++;
    }

    // Check for duplicate resource+action combination
    const existingByResourceAction =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[71]++, await db.select({
      id: permissions.id
    }).from(permissions).where(and(eq(permissions.resource, resource), eq(permissions.action, action.toLowerCase()))).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[72]++;
    if (existingByResourceAction.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[42][0]++;
      cov_w48otqzfm().s[73]++;
      reply.code(409);
      /* istanbul ignore next */
      cov_w48otqzfm().s[74]++;
      return {
        status: 409,
        message: "Conflict: Permission for this resource and action already exists",
        errors: [{
          field: "resource",
          message: "This resource+action combination already exists"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[42][1]++;
    }

    // Create permission
    const now =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[75]++, new Date());
    const [newPermission] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[76]++, await db.insert(permissions).values({
      name,
      resource,
      action: action.toLowerCase(),
      description:
      /* istanbul ignore next */
      (cov_w48otqzfm().b[43][0]++, description) ||
      /* istanbul ignore next */
      (cov_w48otqzfm().b[43][1]++, null),
      guard_name:
      /* istanbul ignore next */
      (cov_w48otqzfm().b[44][0]++, guard_name) ||
      /* istanbul ignore next */
      (cov_w48otqzfm().b[44][1]++, "web"),
      is_active: true,
      createdAt: now,
      updatedAt: now
    }).returning());

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[77]++;
    await createAuditLog({
      userId: context.userId,
      action: 'CREATE',
      resourceType: 'permission',
      resourceId: newPermission.id,
      newValue: newPermission,
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[78]++;
    reply.code(201);
    /* istanbul ignore next */
    cov_w48otqzfm().s[79]++;
    reply.header('Location', `/api/permissions/${newPermission.id}`);
    /* istanbul ignore next */
    cov_w48otqzfm().s[80]++;
    return {
      status: 201,
      message: "Permission created successfully",
      data: newPermission
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[81]++;
    logger.error("Error creating permission:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[82]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[83]++;
    return {
      status: 500,
      message: "Server error while creating permission"
    };
  }
};

/**
 * Get permission by ID with associated roles
 * @route GET /permissions/:id
 */
/* istanbul ignore next */
cov_w48otqzfm().s[84]++;
export const getPermissionById = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[8]++;
  cov_w48otqzfm().s[85]++;
  try {
    const {
      id
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[86]++, request.params);
    const permissionId =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[87]++, parseInt(id));
    /* istanbul ignore next */
    cov_w48otqzfm().s[88]++;
    if (isNaN(permissionId)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[45][0]++;
      cov_w48otqzfm().s[89]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[90]++;
      return {
        status: 400,
        message: "Invalid permission ID",
        errors: [{
          field: "id",
          message: "Permission ID must be a number"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[45][1]++;
    }

    // Get permission
    const [permission] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[91]++, await db.select().from(permissions).where(eq(permissions.id, permissionId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[92]++;
    if (!permission) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[46][0]++;
      cov_w48otqzfm().s[93]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[94]++;
      return {
        status: 404,
        message: "Permission not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[46][1]++;
    }

    // Get associated roles using JOIN
    const associatedRoles =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[95]++, await db.select({
      id: roles.id,
      name: roles.name,
      display_name: roles.display_name,
      hierarchy_level: roles.hierarchy_level,
      is_active: roles.is_active,
      assigned_at: role_has_permissions.assigned_at,
      assigned_by: role_has_permissions.assigned_by
    }).from(role_has_permissions).innerJoin(roles, eq(role_has_permissions.role_id, roles.id)).where(eq(role_has_permissions.permission_id, permissionId)));
    /* istanbul ignore next */
    cov_w48otqzfm().s[96]++;
    return {
      status: 200,
      data: {
        ...permission,
        roles: associatedRoles
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[97]++;
    logger.error("Error fetching permission:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[98]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[99]++;
    return {
      status: 500,
      message: "Server error while fetching permission"
    };
  }
};

/**
 * Update permission by ID
 * Note: Permission key/name cannot be changed to maintain referential integrity
 * @route PUT /permissions/:id
 */
/* istanbul ignore next */
cov_w48otqzfm().s[100]++;
export const updatePermission = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[9]++;
  cov_w48otqzfm().s[101]++;
  try {
    const {
      id
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[102]++, request.params);
    const {
      description,
      is_active,
      guard_name
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[103]++, request.body);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[104]++, getRequestContext(request));
    const permissionId =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[105]++, parseInt(id));
    /* istanbul ignore next */
    cov_w48otqzfm().s[106]++;
    if (isNaN(permissionId)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[47][0]++;
      cov_w48otqzfm().s[107]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[108]++;
      return {
        status: 400,
        message: "Invalid permission ID",
        errors: [{
          field: "id",
          message: "Permission ID must be a number"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[47][1]++;
    }

    // Get existing permission
    const [existingPermission] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[109]++, await db.select().from(permissions).where(eq(permissions.id, permissionId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[110]++;
    if (!existingPermission) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[48][0]++;
      cov_w48otqzfm().s[111]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[112]++;
      return {
        status: 404,
        message: "Permission not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[48][1]++;
    }

    // Build update data (excluding name/resource/action which are immutable)
    const updateData =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[113]++, {
      updatedAt: new Date()
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[114]++;
    if (description !== undefined) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[49][0]++;
      cov_w48otqzfm().s[115]++;
      updateData.description = description;
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[49][1]++;
    }
    cov_w48otqzfm().s[116]++;
    if (is_active !== undefined) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[50][0]++;
      cov_w48otqzfm().s[117]++;
      updateData.is_active = is_active;
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[50][1]++;
    }
    cov_w48otqzfm().s[118]++;
    if (guard_name !== undefined) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[51][0]++;
      cov_w48otqzfm().s[119]++;
      updateData.guard_name = guard_name;
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[51][1]++;
    }

    // Check if there are any actual changes
    cov_w48otqzfm().s[120]++;
    if (Object.keys(updateData).length === 1) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[52][0]++;
      cov_w48otqzfm().s[121]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[122]++;
      return {
        status: 400,
        message: "No valid fields provided to update",
        errors: [{
          field: "body",
          message: "Provide description, is_active, or guard_name to update"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[52][1]++;
    }

    // Update permission
    const [updatedPermission] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[123]++, await db.update(permissions).set(updateData).where(eq(permissions.id, permissionId)).returning());

    // Clear permission cache for all users (permission change affects everyone)
    /* istanbul ignore next */
    cov_w48otqzfm().s[124]++;
    clearPermissionCache();

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[125]++;
    await createAuditLog({
      userId: context.userId,
      action: 'UPDATE',
      resourceType: 'permission',
      resourceId: permissionId,
      oldValue: existingPermission,
      newValue: updatedPermission,
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[126]++;
    return {
      status: 200,
      message: "Permission updated successfully",
      data: updatedPermission
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[127]++;
    logger.error("Error updating permission:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[128]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[129]++;
    return {
      status: 500,
      message: "Server error while updating permission"
    };
  }
};

/**
 * Soft delete permission by ID
 * Sets is_active to false instead of hard delete for audit trail
 * @route DELETE /permissions/:id
 */
/* istanbul ignore next */
cov_w48otqzfm().s[130]++;
export const softDeletePermission = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[10]++;
  cov_w48otqzfm().s[131]++;
  try {
    const {
      id
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[132]++, request.params);
    const {
      force =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[53][0]++, false)
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[133]++, request.query);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[134]++, getRequestContext(request));
    const permissionId =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[135]++, parseInt(id));
    /* istanbul ignore next */
    cov_w48otqzfm().s[136]++;
    if (isNaN(permissionId)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[54][0]++;
      cov_w48otqzfm().s[137]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[138]++;
      return {
        status: 400,
        message: "Invalid permission ID",
        errors: [{
          field: "id",
          message: "Permission ID must be a number"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[54][1]++;
    }

    // Get existing permission
    const [existingPermission] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[139]++, await db.select().from(permissions).where(eq(permissions.id, permissionId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[140]++;
    if (!existingPermission) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[55][0]++;
      cov_w48otqzfm().s[141]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[142]++;
      return {
        status: 404,
        message: "Permission not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[55][1]++;
    }

    // Check if permission is assigned to any active roles
    const activeRoleAssignments =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[143]++, await db.select({
      role_id: role_has_permissions.role_id,
      role_name: roles.name
    }).from(role_has_permissions).innerJoin(roles, eq(role_has_permissions.role_id, roles.id)).where(and(eq(role_has_permissions.permission_id, permissionId), eq(roles.is_active, true))));
    /* istanbul ignore next */
    cov_w48otqzfm().s[144]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[57][0]++, activeRoleAssignments.length > 0) &&
    /* istanbul ignore next */
    (cov_w48otqzfm().b[57][1]++, !force)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[56][0]++;
      cov_w48otqzfm().s[145]++;
      reply.code(409);
      /* istanbul ignore next */
      cov_w48otqzfm().s[146]++;
      return {
        status: 409,
        message: "Cannot delete permission that is assigned to active roles",
        errors: [{
          field: "roles",
          message: `Permission is assigned to roles: ${activeRoleAssignments.map(r => {
            /* istanbul ignore next */
            cov_w48otqzfm().f[11]++;
            cov_w48otqzfm().s[147]++;
            return r.role_name;
          }).join(', ')}. Use force=true to proceed.`
        }],
        affected_roles: activeRoleAssignments.map(r => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[12]++;
          cov_w48otqzfm().s[148]++;
          return {
            id: r.role_id,
            name: r.role_name
          };
        })
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[56][1]++;
    }

    // If force delete, remove role associations
    cov_w48otqzfm().s[149]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[59][0]++, force) &&
    /* istanbul ignore next */
    (cov_w48otqzfm().b[59][1]++, activeRoleAssignments.length > 0)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[58][0]++;
      cov_w48otqzfm().s[150]++;
      await db.delete(role_has_permissions).where(eq(role_has_permissions.permission_id, permissionId));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[58][1]++;
    }

    // Soft delete by setting is_active to false
    const [deletedPermission] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[151]++, await db.update(permissions).set({
      is_active: false,
      updatedAt: new Date()
    }).where(eq(permissions.id, permissionId)).returning());

    // Clear permission cache for all users
    /* istanbul ignore next */
    cov_w48otqzfm().s[152]++;
    clearPermissionCache();

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[153]++;
    await createAuditLog({
      userId: context.userId,
      action: 'DELETE',
      resourceType: 'permission',
      resourceId: permissionId,
      oldValue: existingPermission,
      newValue: {
        ...deletedPermission,
        deleted: true
      },
      metadata: {
        soft_delete: true,
        force_delete: force
      },
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[154]++;
    reply.code(200);
    /* istanbul ignore next */
    cov_w48otqzfm().s[155]++;
    return {
      status: 200,
      message: "Permission deleted successfully"
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[156]++;
    logger.error("Error deleting permission:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[157]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[158]++;
    return {
      status: 500,
      message: "Server error while deleting permission"
    };
  }
};

/**
 * Hard delete permission by ID (permanently removes from database)
 * Only allowed if permission is not assigned to any roles
 * @route DELETE /permissions/:id/permanent
 */
/* istanbul ignore next */
cov_w48otqzfm().s[159]++;
export const hardDeletePermission = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[13]++;
  cov_w48otqzfm().s[160]++;
  try {
    const {
      id
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[161]++, request.params);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[162]++, getRequestContext(request));
    const permissionId =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[163]++, parseInt(id));
    /* istanbul ignore next */
    cov_w48otqzfm().s[164]++;
    if (isNaN(permissionId)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[60][0]++;
      cov_w48otqzfm().s[165]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[166]++;
      return {
        status: 400,
        message: "Invalid permission ID",
        errors: [{
          field: "id",
          message: "Permission ID must be a number"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[60][1]++;
    }

    // Get existing permission
    const [existingPermission] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[167]++, await db.select().from(permissions).where(eq(permissions.id, permissionId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[168]++;
    if (!existingPermission) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[61][0]++;
      cov_w48otqzfm().s[169]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[170]++;
      return {
        status: 404,
        message: "Permission not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[61][1]++;
    }

    // Check for any role assignments
    const roleAssignments =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[171]++, await db.select({
      role_id: role_has_permissions.role_id
    }).from(role_has_permissions).where(eq(role_has_permissions.permission_id, permissionId)));
    /* istanbul ignore next */
    cov_w48otqzfm().s[172]++;
    if (roleAssignments.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[62][0]++;
      cov_w48otqzfm().s[173]++;
      reply.code(409);
      /* istanbul ignore next */
      cov_w48otqzfm().s[174]++;
      return {
        status: 409,
        message: "Cannot permanently delete permission with role associations",
        errors: [{
          field: "roles",
          message: "Remove all role associations before permanent deletion"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[62][1]++;
    }

    // Hard delete
    cov_w48otqzfm().s[175]++;
    await db.delete(permissions).where(eq(permissions.id, permissionId));

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[176]++;
    await createAuditLog({
      userId: context.userId,
      action: 'PERMANENT_DELETE',
      resourceType: 'permission',
      resourceId: permissionId,
      oldValue: existingPermission,
      metadata: {
        permanent: true
      },
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[177]++;
    reply.code(204);
    /* istanbul ignore next */
    cov_w48otqzfm().s[178]++;
    return;
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[179]++;
    logger.error("Error permanently deleting permission:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[180]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[181]++;
    return {
      status: 500,
      message: "Server error while deleting permission"
    };
  }
};

// ============================================================================
// ROLE ASSIGNMENT OPERATIONS
// ============================================================================

/**
 * Assign role(s) to a user
 * @route POST /users/:userId/roles
 */
/* istanbul ignore next */
cov_w48otqzfm().s[182]++;
export const assignRolesToUser = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[14]++;
  cov_w48otqzfm().s[183]++;
  try {
    const {
      userId
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[184]++, request.params);
    const {
      role_ids,
      reason
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[185]++, request.body);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[186]++, getRequestContext(request));

    // Validation
    /* istanbul ignore next */
    cov_w48otqzfm().s[187]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[64][0]++, !role_ids) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[64][1]++, !Array.isArray(role_ids)) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[64][2]++, role_ids.length === 0)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[63][0]++;
      cov_w48otqzfm().s[188]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[189]++;
      return {
        status: 400,
        message: "Validation failed",
        errors: [{
          field: "role_ids",
          message: "role_ids array is required and must not be empty"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[63][1]++;
    }

    // Verify user exists
    const [user] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[190]++, await db.select({
      id: users.id,
      name: users.name
    }).from(users).where(eq(users.id, userId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[191]++;
    if (!user) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[65][0]++;
      cov_w48otqzfm().s[192]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[193]++;
      return {
        status: 404,
        message: "User not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[65][1]++;
    }

    // Verify all roles exist and are active
    const rolesList =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[194]++, await db.select({
      id: roles.id,
      name: roles.name,
      hierarchy_level: roles.hierarchy_level,
      is_active: roles.is_active
    }).from(roles).where(inArray(roles.id, role_ids)));
    const validRoleIds =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[195]++, rolesList.filter(r => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[15]++;
      cov_w48otqzfm().s[196]++;
      return r.is_active;
    }).map(r => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[16]++;
      cov_w48otqzfm().s[197]++;
      return r.id;
    }));
    const invalidRoleIds =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[198]++, role_ids.filter(id => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[17]++;
      cov_w48otqzfm().s[199]++;
      return !validRoleIds.includes(parseInt(id));
    }));
    /* istanbul ignore next */
    cov_w48otqzfm().s[200]++;
    if (invalidRoleIds.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[66][0]++;
      cov_w48otqzfm().s[201]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[202]++;
      return {
        status: 400,
        message: "Some roles are invalid or inactive",
        errors: [{
          field: "role_ids",
          message: `Invalid or inactive role IDs: ${invalidRoleIds.join(', ')}`
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[66][1]++;
    }

    // Privilege escalation check - assigning user must have higher privilege than roles being assigned
    const assignerPrivilegeLevel =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[203]++, await getUserHighestPrivilegeLevel(context.userId));
    const rolesToAssign =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[204]++, rolesList.filter(r => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[18]++;
      cov_w48otqzfm().s[205]++;
      return r.is_active;
    }));
    const privilegeViolations =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[206]++, rolesToAssign.filter(r => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[19]++;
      cov_w48otqzfm().s[207]++;
      return r.hierarchy_level < assignerPrivilegeLevel;
    }));
    /* istanbul ignore next */
    cov_w48otqzfm().s[208]++;
    if (privilegeViolations.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[67][0]++;
      cov_w48otqzfm().s[209]++;
      reply.code(403);
      /* istanbul ignore next */
      cov_w48otqzfm().s[210]++;
      return {
        status: 403,
        message: "Privilege escalation denied",
        errors: [{
          field: "role_ids",
          message: `Cannot assign roles with higher privilege than your own: ${privilegeViolations.map(r => {
            /* istanbul ignore next */
            cov_w48otqzfm().f[20]++;
            cov_w48otqzfm().s[211]++;
            return r.name;
          }).join(', ')}`
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[67][1]++;
    }

    // Get existing role assignments to avoid duplicates
    const existingAssignments =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[212]++, await db.select({
      role_id: user_has_roles.role_id
    }).from(user_has_roles).where(eq(user_has_roles.user_id, userId)));
    const existingRoleIds =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[213]++, existingAssignments.map(a => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[21]++;
      cov_w48otqzfm().s[214]++;
      return a.role_id;
    }));
    const newRoleIds =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[215]++, validRoleIds.filter(id => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[22]++;
      cov_w48otqzfm().s[216]++;
      return !existingRoleIds.includes(id);
    }));

    // If all roles are already assigned (idempotent)
    /* istanbul ignore next */
    cov_w48otqzfm().s[217]++;
    if (newRoleIds.length === 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[68][0]++;
      cov_w48otqzfm().s[218]++;
      return {
        status: 200,
        message: "All roles are already assigned to this user",
        data: {
          assigned: 0,
          skipped: validRoleIds.length
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[68][1]++;
    }

    // Insert new role assignments
    const now =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[219]++, new Date());
    const assignmentValues =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[220]++, newRoleIds.map(roleId => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[23]++;
      cov_w48otqzfm().s[221]++;
      return {
        user_id: userId,
        role_id: roleId,
        assigned_at: now,
        assigned_by: context.userId
      };
    }));
    /* istanbul ignore next */
    cov_w48otqzfm().s[222]++;
    await db.insert(user_has_roles).values(assignmentValues);

    // Clear permission cache for this user
    /* istanbul ignore next */
    cov_w48otqzfm().s[223]++;
    clearPermissionCache(userId);

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[224]++;
    await createAuditLog({
      userId: context.userId,
      action: 'ASSIGN_ROLES',
      resourceType: 'user_role',
      resourceId: userId,
      newValue: {
        assigned_roles: newRoleIds,
        reason
      },
      metadata: {
        target_user: userId,
        reason
      },
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[225]++;
    reply.code(201);
    /* istanbul ignore next */
    cov_w48otqzfm().s[226]++;
    return {
      status: 201,
      message: "Roles assigned successfully",
      data: {
        user_id: userId,
        assigned: newRoleIds.length,
        skipped: validRoleIds.length - newRoleIds.length,
        assigned_roles: rolesToAssign.filter(r => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[24]++;
          cov_w48otqzfm().s[227]++;
          return newRoleIds.includes(r.id);
        }).map(r => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[25]++;
          cov_w48otqzfm().s[228]++;
          return {
            id: r.id,
            name: r.name
          };
        })
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[229]++;
    logger.error("Error assigning roles to user:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[230]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[231]++;
    return {
      status: 500,
      message: "Server error while assigning roles"
    };
  }
};

/**
 * Revoke role(s) from a user
 * @route DELETE /users/:userId/roles
 */
/* istanbul ignore next */
cov_w48otqzfm().s[232]++;
export const revokeRolesFromUser = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[26]++;
  cov_w48otqzfm().s[233]++;
  try {
    const {
      userId
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[234]++, request.params);
    const {
      role_ids,
      reason
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[235]++, request.body);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[236]++, getRequestContext(request));

    // Validation
    /* istanbul ignore next */
    cov_w48otqzfm().s[237]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[70][0]++, !role_ids) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[70][1]++, !Array.isArray(role_ids)) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[70][2]++, role_ids.length === 0)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[69][0]++;
      cov_w48otqzfm().s[238]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[239]++;
      return {
        status: 400,
        message: "Validation failed",
        errors: [{
          field: "role_ids",
          message: "role_ids array is required and must not be empty"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[69][1]++;
    }

    // Verify user exists
    const [user] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[240]++, await db.select({
      id: users.id
    }).from(users).where(eq(users.id, userId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[241]++;
    if (!user) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[71][0]++;
      cov_w48otqzfm().s[242]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[243]++;
      return {
        status: 404,
        message: "User not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[71][1]++;
    }

    // Get existing assignments to revoke
    const existingAssignments =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[244]++, await db.select({
      role_id: user_has_roles.role_id,
      role_name: roles.name
    }).from(user_has_roles).innerJoin(roles, eq(user_has_roles.role_id, roles.id)).where(and(eq(user_has_roles.user_id, userId), inArray(user_has_roles.role_id, role_ids))));
    /* istanbul ignore next */
    cov_w48otqzfm().s[245]++;
    if (existingAssignments.length === 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[72][0]++;
      cov_w48otqzfm().s[246]++;
      return {
        status: 200,
        message: "No matching role assignments found to revoke",
        data: {
          revoked: 0
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[72][1]++;
    }

    // Delete role assignments
    cov_w48otqzfm().s[247]++;
    await db.delete(user_has_roles).where(and(eq(user_has_roles.user_id, userId), inArray(user_has_roles.role_id, role_ids)));

    // Clear permission cache for this user
    /* istanbul ignore next */
    cov_w48otqzfm().s[248]++;
    clearPermissionCache(userId);

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[249]++;
    await createAuditLog({
      userId: context.userId,
      action: 'REVOKE_ROLES',
      resourceType: 'user_role',
      resourceId: userId,
      oldValue: {
        revoked_roles: existingAssignments
      },
      metadata: {
        target_user: userId,
        reason:
        /* istanbul ignore next */
        (cov_w48otqzfm().b[73][0]++, reason) ||
        /* istanbul ignore next */
        (cov_w48otqzfm().b[73][1]++, 'No reason provided')
      },
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[250]++;
    return {
      status: 200,
      message: "Roles revoked successfully",
      data: {
        user_id: userId,
        revoked: existingAssignments.length,
        revoked_roles: existingAssignments.map(a => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[27]++;
          cov_w48otqzfm().s[251]++;
          return {
            id: a.role_id,
            name: a.role_name
          };
        })
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[252]++;
    logger.error("Error revoking roles from user:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[253]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[254]++;
    return {
      status: 500,
      message: "Server error while revoking roles"
    };
  }
};

/**
 * Replace all roles for a user (atomic operation)
 * @route PUT /users/:userId/roles
 */
/* istanbul ignore next */
cov_w48otqzfm().s[255]++;
export const replaceUserRoles = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[28]++;
  cov_w48otqzfm().s[256]++;
  try {
    const {
      userId
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[257]++, request.params);
    const {
      role_ids,
      reason
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[258]++, request.body);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[259]++, getRequestContext(request));

    // Validation - allow empty array to remove all roles
    /* istanbul ignore next */
    cov_w48otqzfm().s[260]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[75][0]++, !role_ids) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[75][1]++, !Array.isArray(role_ids))) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[74][0]++;
      cov_w48otqzfm().s[261]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[262]++;
      return {
        status: 400,
        message: "Validation failed",
        errors: [{
          field: "role_ids",
          message: "role_ids array is required"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[74][1]++;
    }

    // Verify user exists
    const [user] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[263]++, await db.select({
      id: users.id
    }).from(users).where(eq(users.id, userId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[264]++;
    if (!user) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[76][0]++;
      cov_w48otqzfm().s[265]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[266]++;
      return {
        status: 404,
        message: "User not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[76][1]++;
    }

    // Get current assignments for audit
    const currentAssignments =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[267]++, await db.select({
      role_id: user_has_roles.role_id,
      role_name: roles.name
    }).from(user_has_roles).innerJoin(roles, eq(user_has_roles.role_id, roles.id)).where(eq(user_has_roles.user_id, userId)));

    // Verify all new roles exist and are active
    let validRoleIds =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[268]++, []);
    /* istanbul ignore next */
    cov_w48otqzfm().s[269]++;
    if (role_ids.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[77][0]++;
      const rolesList =
      /* istanbul ignore next */
      (cov_w48otqzfm().s[270]++, await db.select({
        id: roles.id,
        name: roles.name,
        hierarchy_level: roles.hierarchy_level,
        is_active: roles.is_active
      }).from(roles).where(inArray(roles.id, role_ids)));
      /* istanbul ignore next */
      cov_w48otqzfm().s[271]++;
      validRoleIds = rolesList.filter(r => {
        /* istanbul ignore next */
        cov_w48otqzfm().f[29]++;
        cov_w48otqzfm().s[272]++;
        return r.is_active;
      }).map(r => {
        /* istanbul ignore next */
        cov_w48otqzfm().f[30]++;
        cov_w48otqzfm().s[273]++;
        return r.id;
      });
      const invalidRoleIds =
      /* istanbul ignore next */
      (cov_w48otqzfm().s[274]++, role_ids.filter(id => {
        /* istanbul ignore next */
        cov_w48otqzfm().f[31]++;
        cov_w48otqzfm().s[275]++;
        return !validRoleIds.includes(parseInt(id));
      }));
      /* istanbul ignore next */
      cov_w48otqzfm().s[276]++;
      if (invalidRoleIds.length > 0) {
        /* istanbul ignore next */
        cov_w48otqzfm().b[78][0]++;
        cov_w48otqzfm().s[277]++;
        reply.code(400);
        /* istanbul ignore next */
        cov_w48otqzfm().s[278]++;
        return {
          status: 400,
          message: "Some roles are invalid or inactive",
          errors: [{
            field: "role_ids",
            message: `Invalid or inactive role IDs: ${invalidRoleIds.join(', ')}`
          }]
        };
      } else
      /* istanbul ignore next */
      {
        cov_w48otqzfm().b[78][1]++;
      }

      // Privilege escalation check
      const assignerPrivilegeLevel =
      /* istanbul ignore next */
      (cov_w48otqzfm().s[279]++, await getUserHighestPrivilegeLevel(context.userId));
      const rolesToAssign =
      /* istanbul ignore next */
      (cov_w48otqzfm().s[280]++, rolesList.filter(r => {
        /* istanbul ignore next */
        cov_w48otqzfm().f[32]++;
        cov_w48otqzfm().s[281]++;
        return r.is_active;
      }));
      const privilegeViolations =
      /* istanbul ignore next */
      (cov_w48otqzfm().s[282]++, rolesToAssign.filter(r => {
        /* istanbul ignore next */
        cov_w48otqzfm().f[33]++;
        cov_w48otqzfm().s[283]++;
        return r.hierarchy_level < assignerPrivilegeLevel;
      }));
      /* istanbul ignore next */
      cov_w48otqzfm().s[284]++;
      if (privilegeViolations.length > 0) {
        /* istanbul ignore next */
        cov_w48otqzfm().b[79][0]++;
        cov_w48otqzfm().s[285]++;
        reply.code(403);
        /* istanbul ignore next */
        cov_w48otqzfm().s[286]++;
        return {
          status: 403,
          message: "Privilege escalation denied",
          errors: [{
            field: "role_ids",
            message: `Cannot assign roles with higher privilege than your own: ${privilegeViolations.map(r => {
              /* istanbul ignore next */
              cov_w48otqzfm().f[34]++;
              cov_w48otqzfm().s[287]++;
              return r.name;
            }).join(', ')}`
          }]
        };
      } else
      /* istanbul ignore next */
      {
        cov_w48otqzfm().b[79][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[77][1]++;
    }

    // Delete all current assignments
    cov_w48otqzfm().s[288]++;
    await db.delete(user_has_roles).where(eq(user_has_roles.user_id, userId));

    // Insert new role assignments if any
    /* istanbul ignore next */
    cov_w48otqzfm().s[289]++;
    if (validRoleIds.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[80][0]++;
      const now =
      /* istanbul ignore next */
      (cov_w48otqzfm().s[290]++, new Date());
      const assignmentValues =
      /* istanbul ignore next */
      (cov_w48otqzfm().s[291]++, validRoleIds.map(roleId => {
        /* istanbul ignore next */
        cov_w48otqzfm().f[35]++;
        cov_w48otqzfm().s[292]++;
        return {
          user_id: userId,
          role_id: roleId,
          assigned_at: now,
          assigned_by: context.userId
        };
      }));
      /* istanbul ignore next */
      cov_w48otqzfm().s[293]++;
      await db.insert(user_has_roles).values(assignmentValues);
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[80][1]++;
    }

    // Clear permission cache for this user
    cov_w48otqzfm().s[294]++;
    clearPermissionCache(userId);

    // Get new assignments for response
    const newAssignments =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[295]++, await db.select({
      role_id: user_has_roles.role_id,
      role_name: roles.name,
      assigned_at: user_has_roles.assigned_at
    }).from(user_has_roles).innerJoin(roles, eq(user_has_roles.role_id, roles.id)).where(eq(user_has_roles.user_id, userId)));

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[296]++;
    await createAuditLog({
      userId: context.userId,
      action: 'REPLACE_ROLES',
      resourceType: 'user_role',
      resourceId: userId,
      oldValue: {
        previous_roles: currentAssignments
      },
      newValue: {
        new_roles: newAssignments
      },
      metadata: {
        target_user: userId,
        reason:
        /* istanbul ignore next */
        (cov_w48otqzfm().b[81][0]++, reason) ||
        /* istanbul ignore next */
        (cov_w48otqzfm().b[81][1]++, 'Role replacement')
      },
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[297]++;
    return {
      status: 200,
      message: "User roles replaced successfully",
      data: {
        user_id: userId,
        previous_count: currentAssignments.length,
        new_count: newAssignments.length,
        roles: newAssignments.map(a => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[36]++;
          cov_w48otqzfm().s[298]++;
          return {
            id: a.role_id,
            name: a.role_name,
            assigned_at: a.assigned_at
          };
        })
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[299]++;
    logger.error("Error replacing user roles:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[300]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[301]++;
    return {
      status: 500,
      message: "Server error while replacing roles"
    };
  }
};

// ============================================================================
// PERMISSION-ROLE ASSOCIATION
// ============================================================================

/**
 * Assign permissions to a role
 * @route POST /roles/:roleId/permissions
 */
/* istanbul ignore next */
cov_w48otqzfm().s[302]++;
export const assignPermissionsToRole = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[37]++;
  cov_w48otqzfm().s[303]++;
  try {
    const {
      roleId
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[304]++, request.params);
    const {
      permission_ids
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[305]++, request.body);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[306]++, getRequestContext(request));
    const roleIdNum =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[307]++, parseInt(roleId));

    // Validation
    /* istanbul ignore next */
    cov_w48otqzfm().s[308]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[83][0]++, !permission_ids) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[83][1]++, !Array.isArray(permission_ids)) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[83][2]++, permission_ids.length === 0)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[82][0]++;
      cov_w48otqzfm().s[309]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[310]++;
      return {
        status: 400,
        message: "Validation failed",
        errors: [{
          field: "permission_ids",
          message: "permission_ids array is required and must not be empty"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[82][1]++;
    }

    // Verify role exists
    const [role] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[311]++, await db.select({
      id: roles.id,
      name: roles.name,
      is_system: roles.is_system
    }).from(roles).where(eq(roles.id, roleIdNum)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[312]++;
    if (!role) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[84][0]++;
      cov_w48otqzfm().s[313]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[314]++;
      return {
        status: 404,
        message: "Role not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[84][1]++;
    }

    // Verify all permissions exist and are active
    const permissionsList =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[315]++, await db.select({
      id: permissions.id,
      name: permissions.name,
      is_active: permissions.is_active
    }).from(permissions).where(inArray(permissions.id, permission_ids)));
    const validPermissionIds =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[316]++, permissionsList.filter(p => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[38]++;
      cov_w48otqzfm().s[317]++;
      return p.is_active;
    }).map(p => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[39]++;
      cov_w48otqzfm().s[318]++;
      return p.id;
    }));
    const invalidPermissionIds =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[319]++, permission_ids.filter(id => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[40]++;
      cov_w48otqzfm().s[320]++;
      return !validPermissionIds.includes(parseInt(id));
    }));
    /* istanbul ignore next */
    cov_w48otqzfm().s[321]++;
    if (invalidPermissionIds.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[85][0]++;
      cov_w48otqzfm().s[322]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[323]++;
      return {
        status: 400,
        message: "Some permissions are invalid or inactive",
        errors: [{
          field: "permission_ids",
          message: `Invalid or inactive permission IDs: ${invalidPermissionIds.join(', ')}`
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[85][1]++;
    }

    // Get existing permission assignments to avoid duplicates
    const existingAssignments =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[324]++, await db.select({
      permission_id: role_has_permissions.permission_id
    }).from(role_has_permissions).where(eq(role_has_permissions.role_id, roleIdNum)));
    const existingPermissionIds =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[325]++, existingAssignments.map(a => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[41]++;
      cov_w48otqzfm().s[326]++;
      return a.permission_id;
    }));
    const newPermissionIds =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[327]++, validPermissionIds.filter(id => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[42]++;
      cov_w48otqzfm().s[328]++;
      return !existingPermissionIds.includes(id);
    }));

    // If all permissions are already assigned (idempotent)
    /* istanbul ignore next */
    cov_w48otqzfm().s[329]++;
    if (newPermissionIds.length === 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[86][0]++;
      cov_w48otqzfm().s[330]++;
      return {
        status: 200,
        message: "All permissions are already assigned to this role",
        data: {
          assigned: 0,
          skipped: validPermissionIds.length
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[86][1]++;
    }

    // Insert new permission assignments
    const now =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[331]++, new Date());
    const assignmentValues =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[332]++, newPermissionIds.map(permissionId => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[43]++;
      cov_w48otqzfm().s[333]++;
      return {
        role_id: roleIdNum,
        permission_id: permissionId,
        assigned_at: now,
        assigned_by: context.userId
      };
    }));
    /* istanbul ignore next */
    cov_w48otqzfm().s[334]++;
    await db.insert(role_has_permissions).values(assignmentValues);

    // Clear permission cache for all users with this role
    /* istanbul ignore next */
    cov_w48otqzfm().s[335]++;
    clearPermissionCache();

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[336]++;
    await createAuditLog({
      userId: context.userId,
      action: 'ASSIGN_PERMISSIONS',
      resourceType: 'role_permission',
      resourceId: roleIdNum,
      newValue: {
        assigned_permissions: newPermissionIds
      },
      metadata: {
        role_name: role.name
      },
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[337]++;
    reply.code(201);
    /* istanbul ignore next */
    cov_w48otqzfm().s[338]++;
    return {
      status: 201,
      message: "Permissions assigned to role successfully",
      data: {
        role_id: roleIdNum,
        role_name: role.name,
        assigned: newPermissionIds.length,
        skipped: validPermissionIds.length - newPermissionIds.length,
        assigned_permissions: permissionsList.filter(p => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[44]++;
          cov_w48otqzfm().s[339]++;
          return newPermissionIds.includes(p.id);
        }).map(p => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[45]++;
          cov_w48otqzfm().s[340]++;
          return {
            id: p.id,
            name: p.name
          };
        })
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[341]++;
    logger.error("Error assigning permissions to role:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[342]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[343]++;
    return {
      status: 500,
      message: "Server error while assigning permissions"
    };
  }
};

/**
 * Remove permissions from a role
 * @route DELETE /roles/:roleId/permissions
 */
/* istanbul ignore next */
cov_w48otqzfm().s[344]++;
export const removePermissionsFromRole = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[46]++;
  cov_w48otqzfm().s[345]++;
  try {
    const {
      roleId
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[346]++, request.params);
    const {
      permission_ids
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[347]++, request.body);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[348]++, getRequestContext(request));
    const roleIdNum =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[349]++, parseInt(roleId));

    // Validation
    /* istanbul ignore next */
    cov_w48otqzfm().s[350]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[88][0]++, !permission_ids) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[88][1]++, !Array.isArray(permission_ids)) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[88][2]++, permission_ids.length === 0)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[87][0]++;
      cov_w48otqzfm().s[351]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[352]++;
      return {
        status: 400,
        message: "Validation failed",
        errors: [{
          field: "permission_ids",
          message: "permission_ids array is required and must not be empty"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[87][1]++;
    }

    // Verify role exists
    const [role] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[353]++, await db.select({
      id: roles.id,
      name: roles.name
    }).from(roles).where(eq(roles.id, roleIdNum)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[354]++;
    if (!role) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[89][0]++;
      cov_w48otqzfm().s[355]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[356]++;
      return {
        status: 404,
        message: "Role not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[89][1]++;
    }

    // Get existing assignments to remove
    const existingAssignments =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[357]++, await db.select({
      permission_id: role_has_permissions.permission_id,
      permission_name: permissions.name
    }).from(role_has_permissions).innerJoin(permissions, eq(role_has_permissions.permission_id, permissions.id)).where(and(eq(role_has_permissions.role_id, roleIdNum), inArray(role_has_permissions.permission_id, permission_ids))));
    /* istanbul ignore next */
    cov_w48otqzfm().s[358]++;
    if (existingAssignments.length === 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[90][0]++;
      cov_w48otqzfm().s[359]++;
      return {
        status: 200,
        message: "No matching permission assignments found to remove",
        data: {
          removed: 0
        }
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[90][1]++;
    }

    // Delete permission assignments
    cov_w48otqzfm().s[360]++;
    await db.delete(role_has_permissions).where(and(eq(role_has_permissions.role_id, roleIdNum), inArray(role_has_permissions.permission_id, permission_ids)));

    // Clear permission cache for all users
    /* istanbul ignore next */
    cov_w48otqzfm().s[361]++;
    clearPermissionCache();

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[362]++;
    await createAuditLog({
      userId: context.userId,
      action: 'REMOVE_PERMISSIONS',
      resourceType: 'role_permission',
      resourceId: roleIdNum,
      oldValue: {
        removed_permissions: existingAssignments
      },
      metadata: {
        role_name: role.name
      },
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[363]++;
    return {
      status: 200,
      message: "Permissions removed from role successfully",
      data: {
        role_id: roleIdNum,
        role_name: role.name,
        removed: existingAssignments.length,
        removed_permissions: existingAssignments.map(a => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[47]++;
          cov_w48otqzfm().s[364]++;
          return {
            id: a.permission_id,
            name: a.permission_name
          };
        })
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[365]++;
    logger.error("Error removing permissions from role:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[366]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[367]++;
    return {
      status: 500,
      message: "Server error while removing permissions"
    };
  }
};

/**
 * Get complete permission set for a role (including inherited from hierarchy)
 * @route GET /roles/:roleId/permissions
 */
/* istanbul ignore next */
cov_w48otqzfm().s[368]++;
export const getRolePermissions = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[48]++;
  cov_w48otqzfm().s[369]++;
  try {
    const {
      roleId
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[370]++, request.params);
    const roleIdNum =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[371]++, parseInt(roleId));

    // Verify role exists
    const [role] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[372]++, await db.select().from(roles).where(eq(roles.id, roleIdNum)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[373]++;
    if (!role) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[91][0]++;
      cov_w48otqzfm().s[374]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[375]++;
      return {
        status: 404,
        message: "Role not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[91][1]++;
    }

    // Get permissions assigned to this role using JOIN
    const rolePermissions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[376]++, await db.select({
      id: permissions.id,
      name: permissions.name,
      resource: permissions.resource,
      action: permissions.action,
      description: permissions.description,
      is_active: permissions.is_active,
      assigned_at: role_has_permissions.assigned_at,
      assigned_by: role_has_permissions.assigned_by
    }).from(role_has_permissions).innerJoin(permissions, eq(role_has_permissions.permission_id, permissions.id)).where(eq(role_has_permissions.role_id, roleIdNum)).orderBy(asc(permissions.resource), asc(permissions.action)));
    /* istanbul ignore next */
    cov_w48otqzfm().s[377]++;
    return {
      status: 200,
      data: {
        role: {
          id: role.id,
          name: role.name,
          display_name: role.display_name,
          hierarchy_level: role.hierarchy_level
        },
        permissions: rolePermissions,
        total: rolePermissions.length
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[378]++;
    logger.error("Error fetching role permissions:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[379]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[380]++;
    return {
      status: 500,
      message: "Server error while fetching role permissions"
    };
  }
};

