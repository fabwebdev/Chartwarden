/**
 * Get all roles assigned to a user
 * @route GET /users/:userId/roles
 */
/* istanbul ignore next */
cov_w48otqzfm().s[394]++;
export const getUserRoles = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[50]++;
  cov_w48otqzfm().s[395]++;
  try {
    const {
      userId
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[396]++, request.params);
    const {
      include_inactive =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[93][0]++, false)
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[397]++, request.query);

    // Verify user exists
    const [user] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[398]++, await db.select({
      id: users.id,
      name: users.name,
      email: users.email
    }).from(users).where(eq(users.id, userId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[399]++;
    if (!user) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[94][0]++;
      cov_w48otqzfm().s[400]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[401]++;
      return {
        status: 404,
        message: "User not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[94][1]++;
    }

    // Build query conditions
    const conditions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[402]++, [eq(user_has_roles.user_id, userId)]);
    /* istanbul ignore next */
    cov_w48otqzfm().s[403]++;
    if (!include_inactive) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[95][0]++;
      cov_w48otqzfm().s[404]++;
      conditions.push(eq(roles.is_active, true));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[95][1]++;
    }

    // Get roles assigned to user
    const userRoles =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[405]++, await db.select({
      id: roles.id,
      name: roles.name,
      display_name: roles.display_name,
      description: roles.description,
      hierarchy_level: roles.hierarchy_level,
      is_active: roles.is_active,
      is_system: roles.is_system,
      assigned_at: user_has_roles.assigned_at,
      assigned_by: user_has_roles.assigned_by
    }).from(user_has_roles).innerJoin(roles, eq(user_has_roles.role_id, roles.id)).where(and(...conditions)).orderBy(asc(roles.hierarchy_level), asc(roles.name)));
    /* istanbul ignore next */
    cov_w48otqzfm().s[406]++;
    return {
      status: 200,
      data: {
        user: {
          id: user.id,
          name: user.name,
          email: user.email
        },
        roles: userRoles,
        total: userRoles.length
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[407]++;
    logger.error("Error fetching user roles:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[408]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[409]++;
    return {
      status: 500,
      message: "Server error while fetching user roles"
    };
  }
};

/**
 * Get effective permissions for a user (aggregate across all roles)
 * @route GET /users/:userId/permissions
 */
/* istanbul ignore next */
cov_w48otqzfm().s[410]++;
export const getUserPermissions = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[51]++;
  cov_w48otqzfm().s[411]++;
  try {
    const {
      userId
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[412]++, request.params);

    // Verify user exists
    const [user] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[413]++, await db.select({
      id: users.id,
      name: users.name,
      email: users.email
    }).from(users).where(eq(users.id, userId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[414]++;
    if (!user) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[96][0]++;
      cov_w48otqzfm().s[415]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[416]++;
      return {
        status: 404,
        message: "User not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[96][1]++;
    }

    // Get all permissions through user's active roles
    const userPermissions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[417]++, await db.selectDistinct({
      id: permissions.id,
      name: permissions.name,
      resource: permissions.resource,
      action: permissions.action,
      description: permissions.description
    }).from(user_has_roles).innerJoin(roles, and(eq(user_has_roles.role_id, roles.id), eq(roles.is_active, true))).innerJoin(role_has_permissions, eq(roles.id, role_has_permissions.role_id)).innerJoin(permissions, and(eq(role_has_permissions.permission_id, permissions.id), eq(permissions.is_active, true))).where(eq(user_has_roles.user_id, userId)).orderBy(asc(permissions.resource), asc(permissions.action)));

    // Group permissions by resource for easier consumption
    const groupedByResource =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[418]++, userPermissions.reduce((acc, perm) => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[52]++;
      cov_w48otqzfm().s[419]++;
      if (!acc[perm.resource]) {
        /* istanbul ignore next */
        cov_w48otqzfm().b[97][0]++;
        cov_w48otqzfm().s[420]++;
        acc[perm.resource] = [];
      } else
      /* istanbul ignore next */
      {
        cov_w48otqzfm().b[97][1]++;
      }
      cov_w48otqzfm().s[421]++;
      acc[perm.resource].push({
        id: perm.id,
        name: perm.name,
        action: perm.action,
        description: perm.description
      });
      /* istanbul ignore next */
      cov_w48otqzfm().s[422]++;
      return acc;
    }, {}));
    /* istanbul ignore next */
    cov_w48otqzfm().s[423]++;
    return {
      status: 200,
      data: {
        user: {
          id: user.id,
          name: user.name,
          email: user.email
        },
        permissions: userPermissions,
        grouped_by_resource: groupedByResource,
        total: userPermissions.length
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[424]++;
    logger.error("Error fetching user permissions:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[425]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[426]++;
    return {
      status: 500,
      message: "Server error while fetching user permissions"
    };
  }
};

/**
 * Check if user has a specific permission
 * @route GET /users/:userId/permissions/:permissionName/check
 */
/* istanbul ignore next */
cov_w48otqzfm().s[427]++;
export const checkUserPermission = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[53]++;
  cov_w48otqzfm().s[428]++;
  try {
    const {
      userId,
      permissionName
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[429]++, request.params);

    // Verify user exists
    const [user] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[430]++, await db.select({
      id: users.id
    }).from(users).where(eq(users.id, userId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[431]++;
    if (!user) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[98][0]++;
      cov_w48otqzfm().s[432]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[433]++;
      return {
        status: 404,
        message: "User not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[98][1]++;
    }

    // Check if user has the permission through any active role
    const hasPermission =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[434]++, await db.select({
      id: permissions.id
    }).from(user_has_roles).innerJoin(roles, and(eq(user_has_roles.role_id, roles.id), eq(roles.is_active, true))).innerJoin(role_has_permissions, eq(roles.id, role_has_permissions.role_id)).innerJoin(permissions, and(eq(role_has_permissions.permission_id, permissions.id), eq(permissions.is_active, true), eq(permissions.name, permissionName))).where(eq(user_has_roles.user_id, userId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[435]++;
    return {
      status: 200,
      data: {
        user_id: userId,
        permission: permissionName,
        has_permission: hasPermission.length > 0
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[436]++;
    logger.error("Error checking user permission:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[437]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[438]++;
    return {
      status: 500,
      message: "Server error while checking permission"
    };
  }
};

/**
 * Get all users with a specific role
 * @route GET /roles/:roleId/users
 */
/* istanbul ignore next */
cov_w48otqzfm().s[439]++;
export const getRoleUsers = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[54]++;
  cov_w48otqzfm().s[440]++;
  try {
    const {
      roleId
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[441]++, request.params);
    const {
      page =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[99][0]++, 1),
      per_page =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[100][0]++, 50)
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[442]++, request.query);
    const roleIdNum =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[443]++, parseInt(roleId));
    const pageNum =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[444]++, Math.max(1, parseInt(page)));
    const perPage =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[445]++, Math.min(100, Math.max(1, parseInt(per_page))));
    const offset =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[446]++, (pageNum - 1) * perPage);

    // Verify role exists
    const [role] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[447]++, await db.select({
      id: roles.id,
      name: roles.name,
      display_name: roles.display_name
    }).from(roles).where(eq(roles.id, roleIdNum)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[448]++;
    if (!role) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[101][0]++;
      cov_w48otqzfm().s[449]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[450]++;
      return {
        status: 404,
        message: "Role not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[101][1]++;
    }

    // Get total count
    const countResult =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[451]++, await db.select({
      count: sql`count(*)`
    }).from(user_has_roles).where(eq(user_has_roles.role_id, roleIdNum)));
    const total =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[452]++, parseInt(countResult[0].count));

    // Get users with this role
    const roleUsers =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[453]++, await db.select({
      id: users.id,
      name: users.name,
      email: users.email,
      is_active: users.is_active,
      assigned_at: user_has_roles.assigned_at,
      assigned_by: user_has_roles.assigned_by
    }).from(user_has_roles).innerJoin(users, eq(user_has_roles.user_id, users.id)).where(eq(user_has_roles.role_id, roleIdNum)).orderBy(asc(users.name)).limit(perPage).offset(offset));
    /* istanbul ignore next */
    cov_w48otqzfm().s[454]++;
    return {
      status: 200,
      data: {
        role: {
          id: role.id,
          name: role.name,
          display_name: role.display_name
        },
        users: roleUsers
      },
      meta: buildPaginationMeta(total, pageNum, perPage)
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[455]++;
    logger.error("Error fetching role users:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[456]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[457]++;
    return {
      status: 500,
      message: "Server error while fetching role users"
    };
  }
};

// ============================================================================
// ROLE CRUD OPERATIONS (Enhanced)
// ============================================================================

/**
 * Get all roles with pagination, filtering, and sorting
 * @route GET /roles
 */
/* istanbul ignore next */
cov_w48otqzfm().s[458]++;
export const getAllRoles = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[55]++;
  cov_w48otqzfm().s[459]++;
  try {
    const {
      page =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[102][0]++, 1),
      per_page =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[103][0]++, 50),
      sort_by =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[104][0]++, 'hierarchy_level'),
      sort_order =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[105][0]++, 'asc'),
      search =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[106][0]++, ''),
      is_active =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[107][0]++, null),
      include_permissions =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[108][0]++, false)
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[460]++, request.query);
    const pageNum =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[461]++, Math.max(1, parseInt(page)));
    const perPage =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[462]++, Math.min(100, Math.max(1, parseInt(per_page))));
    const offset =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[463]++, (pageNum - 1) * perPage);

    // Build where conditions
    const conditions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[464]++, []);
    /* istanbul ignore next */
    cov_w48otqzfm().s[465]++;
    if (search) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[109][0]++;
      cov_w48otqzfm().s[466]++;
      conditions.push(or(like(roles.name, `%${search}%`), like(roles.display_name, `%${search}%`), like(roles.description, `%${search}%`)));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[109][1]++;
    }
    cov_w48otqzfm().s[467]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[111][0]++, is_active !== null) &&
    /* istanbul ignore next */
    (cov_w48otqzfm().b[111][1]++, is_active !== '')) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[110][0]++;
      cov_w48otqzfm().s[468]++;
      conditions.push(eq(roles.is_active,
      /* istanbul ignore next */
      (cov_w48otqzfm().b[112][0]++, is_active === 'true') ||
      /* istanbul ignore next */
      (cov_w48otqzfm().b[112][1]++, is_active === true)));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[110][1]++;
    }
    const whereClause =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[469]++, conditions.length > 0 ?
    /* istanbul ignore next */
    (cov_w48otqzfm().b[113][0]++, and(...conditions)) :
    /* istanbul ignore next */
    (cov_w48otqzfm().b[113][1]++, undefined));

    // Get total count
    const countResult =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[470]++, await db.select({
      count: sql`count(*)`
    }).from(roles).where(whereClause));
    const total =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[471]++, parseInt(countResult[0].count));

    // Determine sort column and direction
    const sortColumn =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[472]++,
    /* istanbul ignore next */
    (cov_w48otqzfm().b[114][0]++, {
      'name': roles.name,
      'display_name': roles.display_name,
      'hierarchy_level': roles.hierarchy_level,
      'created_at': roles.createdAt,
      'updated_at': roles.updatedAt
    }[sort_by]) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[114][1]++, roles.hierarchy_level));
    const sortDirection =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[473]++, sort_order === 'desc' ?
    /* istanbul ignore next */
    (cov_w48otqzfm().b[115][0]++, desc) :
    /* istanbul ignore next */
    (cov_w48otqzfm().b[115][1]++, asc));

    // Get paginated roles
    const rolesList =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[474]++, await db.select().from(roles).where(whereClause).orderBy(sortDirection(sortColumn)).limit(perPage).offset(offset));

    // Optionally include permissions for each role
    let rolesWithPermissions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[475]++, rolesList);
    /* istanbul ignore next */
    cov_w48otqzfm().s[476]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[117][0]++, include_permissions === 'true') ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[117][1]++, include_permissions === true)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[116][0]++;
      cov_w48otqzfm().s[477]++;
      rolesWithPermissions = await Promise.all(rolesList.map(async role => {
        /* istanbul ignore next */
        cov_w48otqzfm().f[56]++;
        const rolePerms =
        /* istanbul ignore next */
        (cov_w48otqzfm().s[478]++, await db.select({
          id: permissions.id,
          name: permissions.name,
          resource: permissions.resource,
          action: permissions.action
        }).from(role_has_permissions).innerJoin(permissions, eq(role_has_permissions.permission_id, permissions.id)).where(eq(role_has_permissions.role_id, role.id)));
        /* istanbul ignore next */
        cov_w48otqzfm().s[479]++;
        return {
          ...role,
          permissions: rolePerms
        };
      }));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[116][1]++;
    }
    cov_w48otqzfm().s[480]++;
    return {
      status: 200,
      data: rolesWithPermissions,
      meta: buildPaginationMeta(total, pageNum, perPage)
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[481]++;
    logger.error("Error fetching roles:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[482]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[483]++;
    return {
      status: 500,
      message: "Server error while fetching roles"
    };
  }
};

/**
 * Create a new role
 * @route POST /roles
 */
/* istanbul ignore next */
cov_w48otqzfm().s[484]++;
export const createRole = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[57]++;
  cov_w48otqzfm().s[485]++;
  try {
    const {
      name,
      display_name,
      description,
      guard_name,
      hierarchy_level,
      permission_ids
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[486]++, request.body);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[487]++, getRequestContext(request));

    // Validation
    const errors =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[488]++, []);
    /* istanbul ignore next */
    cov_w48otqzfm().s[489]++;
    if (!name) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[118][0]++;
      cov_w48otqzfm().s[490]++;
      errors.push({
        field: "name",
        message: "Role name is required"
      });
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[118][1]++;
    }
    cov_w48otqzfm().s[491]++;
    if (errors.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[119][0]++;
      cov_w48otqzfm().s[492]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[493]++;
      return {
        status: 400,
        message: "Validation failed",
        errors
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[119][1]++;
    }

    // Check for duplicate name
    const existingRole =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[494]++, await db.select({
      id: roles.id
    }).from(roles).where(eq(roles.name, name)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[495]++;
    if (existingRole.length > 0) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[120][0]++;
      cov_w48otqzfm().s[496]++;
      reply.code(409);
      /* istanbul ignore next */
      cov_w48otqzfm().s[497]++;
      return {
        status: 409,
        message: "Conflict: Role with this name already exists",
        errors: [{
          field: "name",
          message: "Role name must be unique"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[120][1]++;
    }

    // Privilege escalation check for hierarchy level
    const assignerPrivilegeLevel =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[498]++, await getUserHighestPrivilegeLevel(context.userId));
    const requestedHierarchy =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[499]++,
    /* istanbul ignore next */
    (cov_w48otqzfm().b[121][0]++, hierarchy_level) ||
    /* istanbul ignore next */
    (cov_w48otqzfm().b[121][1]++, 100));
    /* istanbul ignore next */
    cov_w48otqzfm().s[500]++;
    if (requestedHierarchy < assignerPrivilegeLevel) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[122][0]++;
      cov_w48otqzfm().s[501]++;
      reply.code(403);
      /* istanbul ignore next */
      cov_w48otqzfm().s[502]++;
      return {
        status: 403,
        message: "Privilege escalation denied",
        errors: [{
          field: "hierarchy_level",
          message: `Cannot create role with hierarchy level ${requestedHierarchy}. Your highest level is ${assignerPrivilegeLevel}.`
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[122][1]++;
    }

    // Create role
    const now =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[503]++, new Date());
    const [newRole] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[504]++, await db.insert(roles).values({
      name,
      display_name:
      /* istanbul ignore next */
      (cov_w48otqzfm().b[123][0]++, display_name) ||
      /* istanbul ignore next */
      (cov_w48otqzfm().b[123][1]++, name),
      description:
      /* istanbul ignore next */
      (cov_w48otqzfm().b[124][0]++, description) ||
      /* istanbul ignore next */
      (cov_w48otqzfm().b[124][1]++, null),
      guard_name:
      /* istanbul ignore next */
      (cov_w48otqzfm().b[125][0]++, guard_name) ||
      /* istanbul ignore next */
      (cov_w48otqzfm().b[125][1]++, "web"),
      hierarchy_level: requestedHierarchy,
      is_active: true,
      is_system: false,
      createdAt: now,
      updatedAt: now
    }).returning());

    // Assign permissions if provided
    /* istanbul ignore next */
    cov_w48otqzfm().s[505]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[127][0]++, permission_ids) &&
    /* istanbul ignore next */
    (cov_w48otqzfm().b[127][1]++, Array.isArray(permission_ids)) &&
    /* istanbul ignore next */
    (cov_w48otqzfm().b[127][2]++, permission_ids.length > 0)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[126][0]++;
      const validPermissionIds =
      /* istanbul ignore next */
      (cov_w48otqzfm().s[506]++, permission_ids.filter(id => {
        /* istanbul ignore next */
        cov_w48otqzfm().f[58]++;
        cov_w48otqzfm().s[507]++;
        return /* istanbul ignore next */(cov_w48otqzfm().b[128][0]++, typeof id === 'number') ||
        /* istanbul ignore next */
        (cov_w48otqzfm().b[128][1]++, typeof id === 'string') &&
        /* istanbul ignore next */
        (cov_w48otqzfm().b[128][2]++, !isNaN(id));
      }).map(id => {
        /* istanbul ignore next */
        cov_w48otqzfm().f[59]++;
        cov_w48otqzfm().s[508]++;
        return parseInt(id);
      }));
      /* istanbul ignore next */
      cov_w48otqzfm().s[509]++;
      if (validPermissionIds.length > 0) {
        /* istanbul ignore next */
        cov_w48otqzfm().b[129][0]++;
        const assignmentValues =
        /* istanbul ignore next */
        (cov_w48otqzfm().s[510]++, validPermissionIds.map(permissionId => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[60]++;
          cov_w48otqzfm().s[511]++;
          return {
            role_id: newRole.id,
            permission_id: permissionId,
            assigned_at: now,
            assigned_by: context.userId
          };
        }));
        /* istanbul ignore next */
        cov_w48otqzfm().s[512]++;
        await db.insert(role_has_permissions).values(assignmentValues);
      } else
      /* istanbul ignore next */
      {
        cov_w48otqzfm().b[129][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[126][1]++;
    }

    // Get role with permissions for response
    const rolePerms =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[513]++, await db.select({
      id: permissions.id,
      name: permissions.name,
      resource: permissions.resource,
      action: permissions.action
    }).from(role_has_permissions).innerJoin(permissions, eq(role_has_permissions.permission_id, permissions.id)).where(eq(role_has_permissions.role_id, newRole.id)));
    const roleWithPermissions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[514]++, {
      ...newRole,
      permissions: rolePerms
    });

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[515]++;
    await createAuditLog({
      userId: context.userId,
      action: 'CREATE',
      resourceType: 'role',
      resourceId: newRole.id,
      newValue: roleWithPermissions,
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[516]++;
    reply.code(201);
    /* istanbul ignore next */
    cov_w48otqzfm().s[517]++;
    reply.header('Location', `/api/roles/${newRole.id}`);
    /* istanbul ignore next */
    cov_w48otqzfm().s[518]++;
    return {
      status: 201,
      message: "Role created successfully",
      data: roleWithPermissions
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[519]++;
    logger.error("Error creating role:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[520]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[521]++;
    return {
      status: 500,
      message: "Server error while creating role"
    };
  }
};

/**
 * Get role by ID with permissions
 * @route GET /roles/:id
 */
/* istanbul ignore next */
cov_w48otqzfm().s[522]++;
export const getRoleById = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[61]++;
  cov_w48otqzfm().s[523]++;
  try {
    const {
      id
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[524]++, request.params);
    const roleId =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[525]++, parseInt(id));
    /* istanbul ignore next */
    cov_w48otqzfm().s[526]++;
    if (isNaN(roleId)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[130][0]++;
      cov_w48otqzfm().s[527]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[528]++;
      return {
        status: 400,
        message: "Invalid role ID",
        errors: [{
          field: "id",
          message: "Role ID must be a number"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[130][1]++;
    }

    // Get role
    const [role] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[529]++, await db.select().from(roles).where(eq(roles.id, roleId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[530]++;
    if (!role) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[131][0]++;
      cov_w48otqzfm().s[531]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[532]++;
      return {
        status: 404,
        message: "Role not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[131][1]++;
    }

    // Get permissions
    const rolePerms =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[533]++, await db.select({
      id: permissions.id,
      name: permissions.name,
      resource: permissions.resource,
      action: permissions.action,
      description: permissions.description,
      assigned_at: role_has_permissions.assigned_at
    }).from(role_has_permissions).innerJoin(permissions, eq(role_has_permissions.permission_id, permissions.id)).where(eq(role_has_permissions.role_id, roleId)).orderBy(asc(permissions.resource), asc(permissions.action)));

    // Get user count
    const userCountResult =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[534]++, await db.select({
      count: sql`count(*)`
    }).from(user_has_roles).where(eq(user_has_roles.role_id, roleId)));
    const userCount =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[535]++, parseInt(userCountResult[0].count));
    /* istanbul ignore next */
    cov_w48otqzfm().s[536]++;
    return {
      status: 200,
      data: {
        ...role,
        permissions: rolePerms,
        user_count: userCount
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[537]++;
    logger.error("Error fetching role:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[538]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[539]++;
    return {
      status: 500,
      message: "Server error while fetching role"
    };
  }
};

/**
 * Update role by ID
 * @route PUT /roles/:id
 */
/* istanbul ignore next */
cov_w48otqzfm().s[540]++;
export const updateRole = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[62]++;
  cov_w48otqzfm().s[541]++;
  try {
    const {
      id
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[542]++, request.params);
    const {
      display_name,
      description,
      hierarchy_level,
      is_active,
      permission_ids
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[543]++, request.body);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[544]++, getRequestContext(request));
    const roleId =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[545]++, parseInt(id));
    /* istanbul ignore next */
    cov_w48otqzfm().s[546]++;
    if (isNaN(roleId)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[132][0]++;
      cov_w48otqzfm().s[547]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[548]++;
      return {
        status: 400,
        message: "Invalid role ID",
        errors: [{
          field: "id",
          message: "Role ID must be a number"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[132][1]++;
    }

    // Get existing role
    const [existingRole] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[549]++, await db.select().from(roles).where(eq(roles.id, roleId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[550]++;
    if (!existingRole) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[133][0]++;
      cov_w48otqzfm().s[551]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[552]++;
      return {
        status: 404,
        message: "Role not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[133][1]++;
    }

    // Prevent modification of system roles (except certain fields)
    cov_w48otqzfm().s[553]++;
    if (existingRole.is_system) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[134][0]++;
      cov_w48otqzfm().s[554]++;
      reply.code(403);
      /* istanbul ignore next */
      cov_w48otqzfm().s[555]++;
      return {
        status: 403,
        message: "Cannot modify system role",
        errors: [{
          field: "is_system",
          message: "System roles are protected and cannot be modified"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[134][1]++;
    }

    // Privilege escalation check for hierarchy level
    cov_w48otqzfm().s[556]++;
    if (hierarchy_level !== undefined) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[135][0]++;
      const assignerPrivilegeLevel =
      /* istanbul ignore next */
      (cov_w48otqzfm().s[557]++, await getUserHighestPrivilegeLevel(context.userId));
      /* istanbul ignore next */
      cov_w48otqzfm().s[558]++;
      if (hierarchy_level < assignerPrivilegeLevel) {
        /* istanbul ignore next */
        cov_w48otqzfm().b[136][0]++;
        cov_w48otqzfm().s[559]++;
        reply.code(403);
        /* istanbul ignore next */
        cov_w48otqzfm().s[560]++;
        return {
          status: 403,
          message: "Privilege escalation denied",
          errors: [{
            field: "hierarchy_level",
            message: `Cannot set hierarchy level to ${hierarchy_level}. Your highest level is ${assignerPrivilegeLevel}.`
          }]
        };
      } else
      /* istanbul ignore next */
      {
        cov_w48otqzfm().b[136][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[135][1]++;
    }

    // Build update data
    const updateData =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[561]++, {
      updatedAt: new Date()
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[562]++;
    if (display_name !== undefined) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[137][0]++;
      cov_w48otqzfm().s[563]++;
      updateData.display_name = display_name;
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[137][1]++;
    }
    cov_w48otqzfm().s[564]++;
    if (description !== undefined) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[138][0]++;
      cov_w48otqzfm().s[565]++;
      updateData.description = description;
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[138][1]++;
    }
    cov_w48otqzfm().s[566]++;
    if (hierarchy_level !== undefined) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[139][0]++;
      cov_w48otqzfm().s[567]++;
      updateData.hierarchy_level = hierarchy_level;
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[139][1]++;
    }
    cov_w48otqzfm().s[568]++;
    if (is_active !== undefined) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[140][0]++;
      cov_w48otqzfm().s[569]++;
      updateData.is_active = is_active;
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[140][1]++;
    }

    // Update role
    const [updatedRole] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[570]++, await db.update(roles).set(updateData).where(eq(roles.id, roleId)).returning());

    // Update permissions if provided
    /* istanbul ignore next */
    cov_w48otqzfm().s[571]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[142][0]++, permission_ids !== undefined) &&
    /* istanbul ignore next */
    (cov_w48otqzfm().b[142][1]++, Array.isArray(permission_ids))) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[141][0]++;
      cov_w48otqzfm().s[572]++;
      // Remove existing permissions
      await db.delete(role_has_permissions).where(eq(role_has_permissions.role_id, roleId));

      // Add new permissions
      /* istanbul ignore next */
      cov_w48otqzfm().s[573]++;
      if (permission_ids.length > 0) {
        /* istanbul ignore next */
        cov_w48otqzfm().b[143][0]++;
        const validPermissionIds =
        /* istanbul ignore next */
        (cov_w48otqzfm().s[574]++, permission_ids.filter(id => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[63]++;
          cov_w48otqzfm().s[575]++;
          return /* istanbul ignore next */(cov_w48otqzfm().b[144][0]++, typeof id === 'number') ||
          /* istanbul ignore next */
          (cov_w48otqzfm().b[144][1]++, typeof id === 'string') &&
          /* istanbul ignore next */
          (cov_w48otqzfm().b[144][2]++, !isNaN(id));
        }).map(id => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[64]++;
          cov_w48otqzfm().s[576]++;
          return parseInt(id);
        }));
        /* istanbul ignore next */
        cov_w48otqzfm().s[577]++;
        if (validPermissionIds.length > 0) {
          /* istanbul ignore next */
          cov_w48otqzfm().b[145][0]++;
          const assignmentValues =
          /* istanbul ignore next */
          (cov_w48otqzfm().s[578]++, validPermissionIds.map(permissionId => {
            /* istanbul ignore next */
            cov_w48otqzfm().f[65]++;
            cov_w48otqzfm().s[579]++;
            return {
              role_id: roleId,
              permission_id: permissionId,
              assigned_at: new Date(),
              assigned_by: context.userId
            };
          }));
          /* istanbul ignore next */
          cov_w48otqzfm().s[580]++;
          await db.insert(role_has_permissions).values(assignmentValues);
        } else
        /* istanbul ignore next */
        {
          cov_w48otqzfm().b[145][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_w48otqzfm().b[143][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[141][1]++;
    }

    // Clear permission cache
    cov_w48otqzfm().s[581]++;
    clearPermissionCache();

    // Get role with updated permissions
    const rolePerms =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[582]++, await db.select({
      id: permissions.id,
      name: permissions.name,
      resource: permissions.resource,
      action: permissions.action
    }).from(role_has_permissions).innerJoin(permissions, eq(role_has_permissions.permission_id, permissions.id)).where(eq(role_has_permissions.role_id, roleId)));
    const roleWithPermissions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[583]++, {
      ...updatedRole,
      permissions: rolePerms
    });

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[584]++;
    await createAuditLog({
      userId: context.userId,
      action: 'UPDATE',
      resourceType: 'role',
      resourceId: roleId,
      oldValue: existingRole,
      newValue: roleWithPermissions,
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[585]++;
    return {
      status: 200,
      message: "Role updated successfully",
      data: roleWithPermissions
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[586]++;
    logger.error("Error updating role:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[587]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[588]++;
    return {
      status: 500,
      message: "Server error while updating role"
    };
  }
};

/**
 * Delete role by ID (with cascade cleanup)
 * @route DELETE /roles/:id
 */
/* istanbul ignore next */
cov_w48otqzfm().s[589]++;
export const deleteRole = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[66]++;
  cov_w48otqzfm().s[590]++;
  try {
    const {
      id
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[591]++, request.params);
    const {
      force =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[146][0]++, false)
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[592]++, request.query);
    const context =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[593]++, getRequestContext(request));
    const roleId =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[594]++, parseInt(id));
    /* istanbul ignore next */
    cov_w48otqzfm().s[595]++;
    if (isNaN(roleId)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[147][0]++;
      cov_w48otqzfm().s[596]++;
      reply.code(400);
      /* istanbul ignore next */
      cov_w48otqzfm().s[597]++;
      return {
        status: 400,
        message: "Invalid role ID",
        errors: [{
          field: "id",
          message: "Role ID must be a number"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[147][1]++;
    }

    // Get existing role
    const [existingRole] =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[598]++, await db.select().from(roles).where(eq(roles.id, roleId)).limit(1));
    /* istanbul ignore next */
    cov_w48otqzfm().s[599]++;
    if (!existingRole) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[148][0]++;
      cov_w48otqzfm().s[600]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_w48otqzfm().s[601]++;
      return {
        status: 404,
        message: "Role not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[148][1]++;
    }

    // Prevent deletion of system roles
    cov_w48otqzfm().s[602]++;
    if (existingRole.is_system) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[149][0]++;
      cov_w48otqzfm().s[603]++;
      reply.code(403);
      /* istanbul ignore next */
      cov_w48otqzfm().s[604]++;
      return {
        status: 403,
        message: "Cannot delete system role",
        errors: [{
          field: "is_system",
          message: "System roles are protected and cannot be deleted"
        }]
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[149][1]++;
    }

    // Check for user assignments
    const userAssignments =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[605]++, await db.select({
      user_id: user_has_roles.user_id,
      user_name: users.name
    }).from(user_has_roles).innerJoin(users, eq(user_has_roles.user_id, users.id)).where(eq(user_has_roles.role_id, roleId)));
    /* istanbul ignore next */
    cov_w48otqzfm().s[606]++;
    if (
    /* istanbul ignore next */
    (cov_w48otqzfm().b[151][0]++, userAssignments.length > 0) &&
    /* istanbul ignore next */
    (cov_w48otqzfm().b[151][1]++, !force)) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[150][0]++;
      cov_w48otqzfm().s[607]++;
      reply.code(409);
      /* istanbul ignore next */
      cov_w48otqzfm().s[608]++;
      return {
        status: 409,
        message: "Cannot delete role that is assigned to users",
        errors: [{
          field: "users",
          message: `Role is assigned to ${userAssignments.length} user(s). Use force=true to proceed.`
        }],
        affected_users: userAssignments.slice(0, 10).map(u => {
          /* istanbul ignore next */
          cov_w48otqzfm().f[67]++;
          cov_w48otqzfm().s[609]++;
          return {
            id: u.user_id,
            name: u.user_name
          };
        })
      };
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[150][1]++;
    }

    // Delete role-permission associations
    cov_w48otqzfm().s[610]++;
    await db.delete(role_has_permissions).where(eq(role_has_permissions.role_id, roleId));

    // Delete user-role associations
    /* istanbul ignore next */
    cov_w48otqzfm().s[611]++;
    await db.delete(user_has_roles).where(eq(user_has_roles.role_id, roleId));

    // Delete role
    /* istanbul ignore next */
    cov_w48otqzfm().s[612]++;
    await db.delete(roles).where(eq(roles.id, roleId));

    // Clear permission cache for all users
    /* istanbul ignore next */
    cov_w48otqzfm().s[613]++;
    clearPermissionCache();

    // Audit log
    /* istanbul ignore next */
    cov_w48otqzfm().s[614]++;
    await createAuditLog({
      userId: context.userId,
      action: 'DELETE',
      resourceType: 'role',
      resourceId: roleId,
      oldValue: existingRole,
      metadata: {
        force_delete: force,
        affected_users: userAssignments.length
      },
      ipAddress: context.ipAddress,
      userAgent: context.userAgent
    });
    /* istanbul ignore next */
    cov_w48otqzfm().s[615]++;
    reply.code(200);
    /* istanbul ignore next */
    cov_w48otqzfm().s[616]++;
    return {
      status: 200,
      message: "Role deleted successfully",
      data: {
        removed_user_assignments: userAssignments.length,
        removed_permission_assignments: true
      }
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[617]++;
    logger.error("Error deleting role:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[618]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[619]++;
    return {
      status: 500,
      message: "Server error while deleting role"
    };
  }
};

// ============================================================================
// AUDIT TRAIL RETRIEVAL
// ============================================================================

/**
 * Get audit trail for permissions/roles
 * @route GET /audit/permissions
 */
/* istanbul ignore next */
cov_w48otqzfm().s[620]++;
export const getPermissionAuditTrail = async (request, reply) => {
  /* istanbul ignore next */
  cov_w48otqzfm().f[68]++;
  cov_w48otqzfm().s[621]++;
  try {
    const {
      page =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[152][0]++, 1),
      per_page =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[153][0]++, 50),
      resource_type =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[154][0]++, ''),
      resource_id =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[155][0]++, ''),
      action =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[156][0]++, ''),
      user_id =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[157][0]++, ''),
      start_date =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[158][0]++, ''),
      end_date =
      /* istanbul ignore next */
      (cov_w48otqzfm().b[159][0]++, '')
    } =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[622]++, request.query);
    const pageNum =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[623]++, Math.max(1, parseInt(page)));
    const perPage =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[624]++, Math.min(100, Math.max(1, parseInt(per_page))));
    const offset =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[625]++, (pageNum - 1) * perPage);

    // Build where conditions
    const conditions =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[626]++, []);

    // Filter to permission-related resources only
    /* istanbul ignore next */
    cov_w48otqzfm().s[627]++;
    conditions.push(or(eq(audit_logs.resource_type, 'permission'), eq(audit_logs.resource_type, 'role'), eq(audit_logs.resource_type, 'user_role'), eq(audit_logs.resource_type, 'role_permission')));
    /* istanbul ignore next */
    cov_w48otqzfm().s[628]++;
    if (resource_type) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[160][0]++;
      cov_w48otqzfm().s[629]++;
      conditions.push(eq(audit_logs.resource_type, resource_type));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[160][1]++;
    }
    cov_w48otqzfm().s[630]++;
    if (resource_id) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[161][0]++;
      cov_w48otqzfm().s[631]++;
      conditions.push(eq(audit_logs.resource_id, resource_id));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[161][1]++;
    }
    cov_w48otqzfm().s[632]++;
    if (action) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[162][0]++;
      cov_w48otqzfm().s[633]++;
      conditions.push(eq(audit_logs.action, action));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[162][1]++;
    }
    cov_w48otqzfm().s[634]++;
    if (user_id) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[163][0]++;
      cov_w48otqzfm().s[635]++;
      conditions.push(eq(audit_logs.user_id, user_id));
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[163][1]++;
    }
    cov_w48otqzfm().s[636]++;
    if (start_date) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[164][0]++;
      cov_w48otqzfm().s[637]++;
      conditions.push(sql`${audit_logs.created_at} >= ${new Date(start_date)}`);
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[164][1]++;
    }
    cov_w48otqzfm().s[638]++;
    if (end_date) {
      /* istanbul ignore next */
      cov_w48otqzfm().b[165][0]++;
      cov_w48otqzfm().s[639]++;
      conditions.push(sql`${audit_logs.created_at} <= ${new Date(end_date)}`);
    } else
    /* istanbul ignore next */
    {
      cov_w48otqzfm().b[165][1]++;
    }
    const whereClause =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[640]++, and(...conditions));

    // Get total count
    const countResult =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[641]++, await db.select({
      count: sql`count(*)`
    }).from(audit_logs).where(whereClause));
    const total =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[642]++, parseInt(countResult[0].count));

    // Get paginated audit logs
    const auditLogs =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[643]++, await db.select({
      id: audit_logs.id,
      user_id: audit_logs.user_id,
      action: audit_logs.action,
      resource_type: audit_logs.resource_type,
      resource_id: audit_logs.resource_id,
      old_value: audit_logs.old_value,
      new_value: audit_logs.new_value,
      status: audit_logs.status,
      ip_address: audit_logs.ip_address,
      created_at: audit_logs.created_at
    }).from(audit_logs).where(whereClause).orderBy(desc(audit_logs.created_at)).limit(perPage).offset(offset));

    // Parse JSON values
    const parsedLogs =
    /* istanbul ignore next */
    (cov_w48otqzfm().s[644]++, auditLogs.map(log => {
      /* istanbul ignore next */
      cov_w48otqzfm().f[69]++;
      cov_w48otqzfm().s[645]++;
      return {
        ...log,
        old_value: log.old_value ?
        /* istanbul ignore next */
        (cov_w48otqzfm().b[166][0]++, JSON.parse(log.old_value)) :
        /* istanbul ignore next */
        (cov_w48otqzfm().b[166][1]++, null),
        new_value: log.new_value ?
        /* istanbul ignore next */
        (cov_w48otqzfm().b[167][0]++, JSON.parse(log.new_value)) :
        /* istanbul ignore next */
        (cov_w48otqzfm().b[167][1]++, null)
      };
    }));
    /* istanbul ignore next */
    cov_w48otqzfm().s[646]++;
    return {
      status: 200,
      data: parsedLogs,
      meta: buildPaginationMeta(total, pageNum, perPage)
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_w48otqzfm().s[647]++;
    logger.error("Error fetching permission audit trail:", error);
    /* istanbul ignore next */
    cov_w48otqzfm().s[648]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_w48otqzfm().s[649]++;
    return {
      status: 500,
      message: "Server error while fetching audit trail"
    };
  }
};
export default {
  // Permission CRUD
  getAllPermissions,
  getPermissionList,
  createPermission,
  getPermissionById,
  updatePermission,
  softDeletePermission,
  hardDeletePermission,
  // Role Assignment
  assignRolesToUser,
  revokeRolesFromUser,
  replaceUserRoles,
  // Permission-Role Association
  assignPermissionsToRole,
  removePermissionsFromRole,
  getRolePermissions,
  getPermissionRoles,
  // User-Role Queries
  getUserRoles,
  getUserPermissions,
  checkUserPermission,
  getRoleUsers,
  // Role CRUD
  getAllRoles,
  createRole,
  getRoleById,
  updateRole,
  deleteRole,
  // Audit
  getPermissionAuditTrail
};
//# sourceMappingURL=data:application/json;charset=utf-8;base64,