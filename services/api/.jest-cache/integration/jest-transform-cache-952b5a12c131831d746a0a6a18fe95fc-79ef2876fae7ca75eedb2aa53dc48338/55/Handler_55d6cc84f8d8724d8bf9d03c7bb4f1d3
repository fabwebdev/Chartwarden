      /* istanbul ignore next */
      cov_23326n9792().b[1][0]++;
      cov_23326n9792().s[7]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[1][1]++;
    }

    // Prepare error details for logging
    const errorDetails =
    /* istanbul ignore next */
    (cov_23326n9792().s[8]++, {
      errorId,
      timestamp: new Date().toISOString(),
      name: error.name,
      message: error.message,
      code: error.code,
      status:
      /* istanbul ignore next */
      (cov_23326n9792().b[2][0]++, error.status) ||
      /* istanbul ignore next */
      (cov_23326n9792().b[2][1]++, error.statusCode) ||
      /* istanbul ignore next */
      (cov_23326n9792().b[2][2]++, 500),
