      // Sanitize database hints
      cov_23326n9792().s[52]++;
      if (error.detail) {
        /* istanbul ignore next */
        cov_23326n9792().b[31][0]++;
        cov_23326n9792().s[53]++;
        response.detail = redactPII(error.detail, {
          aggressive: true
        });
      } else
      /* istanbul ignore next */
      {
        cov_23326n9792().b[31][1]++;
      }
      cov_23326n9792().s[54]++;
      if (error.hint) {
