  }

  /**
   * Render an exception into an HTTP response
   * @param {Object} request
   * @param {Object} reply
   * @param {Error} error
   */
  render(request, reply, error) {
    /* istanbul ignore next */
    cov_23326n9792().f[11]++;
    const errorId =
    /* istanbul ignore next */
    (cov_23326n9792().s[57]++, this.generateErrorId());
    const status =
    /* istanbul ignore next */
    (cov_23326n9792().s[58]++,
    /* istanbul ignore next */
    (cov_23326n9792().b[33][0]++, error.status) ||
    /* istanbul ignore next */
    (cov_23326n9792().b[33][1]++, error.statusCode) ||
    /* istanbul ignore next */
    (cov_23326n9792().b[33][2]++, 500));

    // Build context for logging
    const context =
    /* istanbul ignore next */
    (cov_23326n9792().s[59]++, {
      path: request.url,
      method: request.method,
      ip: request.ip,
      userAgent: request.headers['user-agent'],
      userId: request.user?.id
    });

    // Report error with PII redaction
    /* istanbul ignore next */
    cov_23326n9792().s[60]++;
    this.report(error, errorId, context);

    // Build response based on environment
    let response;
    /* istanbul ignore next */
    cov_23326n9792().s[61]++;
    if (process.env.NODE_ENV === 'production') {
      /* istanbul ignore next */
      cov_23326n9792().b[34][0]++;
      cov_23326n9792().s[62]++;
      response = this.buildProductionResponse(error, errorId);
    } else {
      /* istanbul ignore next */
      cov_23326n9792().b[34][1]++;
      cov_23326n9792().s[63]++;
      response = this.buildDevelopmentResponse(error, errorId);
    }

    // Send response
    /* istanbul ignore next */
    cov_23326n9792().s[64]++;
    return reply.code(status).send(response);
  }

  /**
   * Register the exception handling callbacks for the application.
   * @param {Function} callback
   */
  reportable(callback) {
    /* istanbul ignore next */
    cov_23326n9792().f[12]++;
    cov_23326n9792().s[65]++;
    this.reportCallback = callback;
  }

  /**
   * Render an exception for console command errors.
   * @param {Error} error
   */
  renderForConsole(error) {
    /* istanbul ignore next */
    cov_23326n9792().f[13]++;
    const errorId =
    /* istanbul ignore next */
    (cov_23326n9792().s[66]++, this.generateErrorId());
    const message =
    /* istanbul ignore next */
    (cov_23326n9792().s[67]++, redactPII(error.message, {
      aggressive: true
    }));
    /* istanbul ignore next */
    cov_23326n9792().s[68]++;
    logger.error(`[${errorId}] Console command error:`, message);
    /* istanbul ignore next */
    cov_23326n9792().s[69]++;
    if (process.env.NODE_ENV === 'development') {
      /* istanbul ignore next */
      cov_23326n9792().b[35][0]++;
      cov_23326n9792().s[70]++;
      logger.error(error.stack);
    } else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[35][1]++;
    }
  }
}
