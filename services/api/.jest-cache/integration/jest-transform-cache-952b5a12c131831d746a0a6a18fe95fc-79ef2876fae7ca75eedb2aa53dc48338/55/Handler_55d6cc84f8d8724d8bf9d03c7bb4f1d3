    cov_23326n9792().s[12]++;
    logger.error('ðŸ”´ Error Report:', JSON.stringify(redactedDetails, null, 2));

    // In production, send to logging service (Sentry, Loggly, etc.)
    /* istanbul ignore next */
    cov_23326n9792().s[13]++;
    if (process.env.NODE_ENV === 'production') {
      /* istanbul ignore next */
      cov_23326n9792().b[4][0]++;
    } // TODO: Send to external logging service
    // Example: Sentry.captureException(error, { extra: redactedDetails });
    else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[4][1]++;
    }
  }

  /**
   * Sanitize error message - remove database hints, SQL, and PHI
   * @param {string} message
   * @returns {string}
   */
  sanitizeMessage(message) {
    /* istanbul ignore next */
    cov_23326n9792().f[5]++;
    cov_23326n9792().s[14]++;
    if (!message) {
      /* istanbul ignore next */
      cov_23326n9792().b[5][0]++;
      cov_23326n9792().s[15]++;
      return 'An error occurred';
    } else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[5][1]++;
    }

    // Redact PII first
    let sanitized =
    /* istanbul ignore next */
    (cov_23326n9792().s[16]++, redactPII(message, {
      aggressive: true
    }));

    // Remove database-specific error patterns
    const dangerousPatterns =
    /* istanbul ignore next */
    (cov_23326n9792().s[17]++, [/relation ".*" does not exist/gi, /column ".*" does not exist/gi, /duplicate key value violates unique constraint/gi, /null value in column ".*" violates not-null constraint/gi, /foreign key constraint/gi, /syntax error at or near/gi, /invalid input syntax for type/gi, /ERROR:.*DETAIL:/gi, /ERROR:.*HINT:/gi, /at .* line \d+/gi, /in \/.*\.js:\d+/gi]);
    /* istanbul ignore next */
    cov_23326n9792().s[18]++;
    for (const pattern of dangerousPatterns) {
      /* istanbul ignore next */
      cov_23326n9792().s[19]++;
      if (pattern.test(sanitized)) {
        /* istanbul ignore next */
        cov_23326n9792().b[6][0]++;
        cov_23326n9792().s[20]++;
        return 'A database error occurred. Please contact support.';
      } else
      /* istanbul ignore next */
      {
        cov_23326n9792().b[6][1]++;
      }
    }

    // Truncate very long messages
    /* istanbul ignore next */
    cov_23326n9792().s[21]++;
    if (sanitized.length > 200) {
      /* istanbul ignore next */
      cov_23326n9792().b[7][0]++;
      cov_23326n9792().s[22]++;
      sanitized = sanitized.substring(0, 200) + '...';
    } else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[7][1]++;
    }
    cov_23326n9792().s[23]++;
    return sanitized;
  }

  /**
   * Determine if this is a validation error
   * @param {Error} error
   * @returns {boolean}
   */
  isValidationError(error) {
    /* istanbul ignore next */
    cov_23326n9792().f[6]++;
    cov_23326n9792().s[24]++;
    return /* istanbul ignore next */(cov_23326n9792().b[8][0]++, error.statusCode === 422) ||
    /* istanbul ignore next */
    (cov_23326n9792().b[8][1]++, error.status === 422) ||
    /* istanbul ignore next */
    (cov_23326n9792().b[8][2]++, error.name === 'ValidationError') ||
    /* istanbul ignore next */
    (cov_23326n9792().b[8][3]++, error.validation !== undefined) ||
    /* istanbul ignore next */
    (cov_23326n9792().b[8][4]++, Array.isArray(error.errors));
  }

  /**
   * Sanitize validation errors - keep field names, remove values
   * @param {Array} errors
   * @returns {Array}
   */
  sanitizeValidationErrors(errors) {
    /* istanbul ignore next */
    cov_23326n9792().f[7]++;
    cov_23326n9792().s[25]++;
    if (!Array.isArray(errors)) {
      /* istanbul ignore next */
      cov_23326n9792().b[9][0]++;
      cov_23326n9792().s[26]++;
      return [];
    } else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[9][1]++;
    }
    cov_23326n9792().s[27]++;
    return errors.map(error => {
      /* istanbul ignore next */
      cov_23326n9792().f[8]++;
      // Keep only safe fields
      const sanitized =
      /* istanbul ignore next */
      (cov_23326n9792().s[28]++, {
        type:
        /* istanbul ignore next */
        (cov_23326n9792().b[10][0]++, error.type) ||
        /* istanbul ignore next */
        (cov_23326n9792().b[10][1]++, 'field'),
        msg: this.sanitizeMessage(
        /* istanbul ignore next */
        (cov_23326n9792().b[11][0]++, error.msg) ||
        /* istanbul ignore next */
        (cov_23326n9792().b[11][1]++, error.message)),
        path:
        /* istanbul ignore next */
        (cov_23326n9792().b[12][0]++, error.path) ||
        /* istanbul ignore next */
        (cov_23326n9792().b[12][1]++, error.field),
        location:
        /* istanbul ignore next */
        (cov_23326n9792().b[13][0]++, error.location) ||
        /* istanbul ignore next */
        (cov_23326n9792().b[13][1]++, 'body')
      });

      // Remove the actual invalid value (could contain PHI)
      // Never include: error.value, error.actual, error.received
      /* istanbul ignore next */
      cov_23326n9792().s[29]++;
      return sanitized;
    });
  }

  /**
   * Build production-safe error response
   * @param {Error} error
   * @param {string} errorId
   * @returns {Object}
   */
  buildProductionResponse(error, errorId) {
    /* istanbul ignore next */
    cov_23326n9792().f[9]++;
    const status =
    /* istanbul ignore next */
    (cov_23326n9792().s[30]++,
    /* istanbul ignore next */
    (cov_23326n9792().b[14][0]++, error.status) ||
    /* istanbul ignore next */
    (cov_23326n9792().b[14][1]++, error.statusCode) ||
    /* istanbul ignore next */
    (cov_23326n9792().b[14][2]++, 500));

    // Base response with generic message
    const response =
    /* istanbul ignore next */
    (cov_23326n9792().s[31]++, {
      status,
      error:
      /* istanbul ignore next */
      (cov_23326n9792().b[15][0]++, this.genericMessages[status]) ||
      /* istanbul ignore next */
      (cov_23326n9792().b[15][1]++, 'Error'),
      message:
      /* istanbul ignore next */
      (cov_23326n9792().b[16][0]++, this.genericMessages[status]) ||
      /* istanbul ignore next */
      (cov_23326n9792().b[16][1]++, 'An error occurred'),
      errorId // Include error ID so user can reference it when contacting support
    });

    // For validation errors, include sanitized field-level errors
    /* istanbul ignore next */
    cov_23326n9792().s[32]++;
    if (this.isValidationError(error)) {
      /* istanbul ignore next */
      cov_23326n9792().b[17][0]++;
      cov_23326n9792().s[33]++;
      response.error = 'Validation Error';
      /* istanbul ignore next */
      cov_23326n9792().s[34]++;
      response.message = 'The request contains invalid data';
      /* istanbul ignore next */
      cov_23326n9792().s[35]++;
      if (
      /* istanbul ignore next */
      (cov_23326n9792().b[19][0]++, error.errors) ||
      /* istanbul ignore next */
      (cov_23326n9792().b[19][1]++, error.validation)) {
        /* istanbul ignore next */
        cov_23326n9792().b[18][0]++;
        cov_23326n9792().s[36]++;
        response.errors = this.sanitizeValidationErrors(
        /* istanbul ignore next */
        (cov_23326n9792().b[20][0]++, error.errors) ||
        /* istanbul ignore next */
        (cov_23326n9792().b[20][1]++, error.validation));
      } else
      /* istanbul ignore next */
      {
        cov_23326n9792().b[18][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[17][1]++;
    }

    // For 404s, we can be a bit more specific
    cov_23326n9792().s[37]++;
    if (status === 404) {
      /* istanbul ignore next */
      cov_23326n9792().b[21][0]++;
      cov_23326n9792().s[38]++;
      response.message = 'The requested resource was not found';
    } else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[21][1]++;
    }

    // For 401/403, generic auth messages
    cov_23326n9792().s[39]++;
    if (status === 401) {
      /* istanbul ignore next */
      cov_23326n9792().b[22][0]++;
      cov_23326n9792().s[40]++;
      response.message = 'Authentication required';
    } else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[22][1]++;
    }
    cov_23326n9792().s[41]++;
    if (status === 403) {
      /* istanbul ignore next */
      cov_23326n9792().b[23][0]++;
      cov_23326n9792().s[42]++;
      response.message = 'You do not have permission to access this resource';
    } else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[23][1]++;
    }
    cov_23326n9792().s[43]++;
    return response;
  }

  /**
   * Build development error response (includes more details)
   * @param {Error} error
   * @param {string} errorId
   * @returns {Object}
   */
  buildDevelopmentResponse(error, errorId) {
    /* istanbul ignore next */
    cov_23326n9792().f[10]++;
    const status =
    /* istanbul ignore next */
    (cov_23326n9792().s[44]++,
    /* istanbul ignore next */
    (cov_23326n9792().b[24][0]++, error.status) ||
    /* istanbul ignore next */
    (cov_23326n9792().b[24][1]++, error.statusCode) ||
    /* istanbul ignore next */
    (cov_23326n9792().b[24][2]++, 500));
    const response =
    /* istanbul ignore next */
    (cov_23326n9792().s[45]++, {
      status,
      error:
      /* istanbul ignore next */
      (cov_23326n9792().b[25][0]++, error.name) ||
      /* istanbul ignore next */
      (cov_23326n9792().b[25][1]++, 'Error'),
      message: this.sanitizeMessage(error.message),
      errorId
    });

    // Include validation errors
    /* istanbul ignore next */
    cov_23326n9792().s[46]++;
    if (
    /* istanbul ignore next */
    (cov_23326n9792().b[27][0]++, this.isValidationError(error)) && (
    /* istanbul ignore next */
    (cov_23326n9792().b[27][1]++, error.errors) ||
    /* istanbul ignore next */
    (cov_23326n9792().b[27][2]++, error.validation))) {
      /* istanbul ignore next */
      cov_23326n9792().b[26][0]++;
      cov_23326n9792().s[47]++;
      response.errors = this.sanitizeValidationErrors(
      /* istanbul ignore next */
      (cov_23326n9792().b[28][0]++, error.errors) ||
      /* istanbul ignore next */
      (cov_23326n9792().b[28][1]++, error.validation));
    } else
    /* istanbul ignore next */
    {
      cov_23326n9792().b[26][1]++;
    }

    // Include stack trace in development only
    cov_23326n9792().s[48]++;
    if (process.env.NODE_ENV === 'development') {
      /* istanbul ignore next */
      cov_23326n9792().b[29][0]++;
      cov_23326n9792().s[49]++;
      response.stack = error.stack;

      // Include additional error details (but redact PII)
      /* istanbul ignore next */
      cov_23326n9792().s[50]++;
