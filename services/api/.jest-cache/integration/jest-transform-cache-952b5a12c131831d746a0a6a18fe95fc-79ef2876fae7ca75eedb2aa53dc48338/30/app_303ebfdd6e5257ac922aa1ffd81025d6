// Bind Console Kernel
/* istanbul ignore next */
cov_5z4129b7().s[2]++;
app.consoleKernel = ConsoleKernel;

// Bind Exception Handler
/* istanbul ignore next */
cov_5z4129b7().s[3]++;
app.exceptionHandler = Handler;

/*
|--------------------------------------------------------------------------
