  cov_2qnx9foqgg().s[20]++;
  fastify.get("/tls/info", {
    schema: {
      description: "Get TLS configuration information",
      tags: ["Security"]
    }
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_2qnx9foqgg().f[3]++;
    // Get connection info
    const connectionInfo =
    /* istanbul ignore next */
    (cov_2qnx9foqgg().s[21]++, {
      protocol: request.protocol,
      secure:
      /* istanbul ignore next */
      (cov_2qnx9foqgg().b[6][0]++, request.socket?.encrypted) ||
      /* istanbul ignore next */
      (cov_2qnx9foqgg().b[6][1]++, false),
      remoteAddress: request.ip
    });

    // Get TLS details if available
    let tlsDetails =
    /* istanbul ignore next */
    (cov_2qnx9foqgg().s[22]++, null);
    /* istanbul ignore next */
    cov_2qnx9foqgg().s[23]++;
    if (
    /* istanbul ignore next */
    (cov_2qnx9foqgg().b[8][0]++, request.socket?.encrypted) &&
    /* istanbul ignore next */
    (cov_2qnx9foqgg().b[8][1]++, request.socket?.getPeerCertificate)) {
      /* istanbul ignore next */
      cov_2qnx9foqgg().b[7][0]++;
      cov_2qnx9foqgg().s[24]++;
      try {
        const peerCert =
        /* istanbul ignore next */
        (cov_2qnx9foqgg().s[25]++, request.socket.getPeerCertificate());
        /* istanbul ignore next */
        cov_2qnx9foqgg().s[26]++;
        tlsDetails = {
          tlsVersion:
          /* istanbul ignore next */
          (cov_2qnx9foqgg().b[9][0]++, request.socket.getProtocol?.()) ||
          /* istanbul ignore next */
          (cov_2qnx9foqgg().b[9][1]++, "unknown"),
          cipher:
          /* istanbul ignore next */
          (cov_2qnx9foqgg().b[10][0]++, request.socket.getCipher?.()) ||
          /* istanbul ignore next */
          (cov_2qnx9foqgg().b[10][1]++, "unknown"),
          peerCertificate: peerCert ?
          /* istanbul ignore next */
          (cov_2qnx9foqgg().b[11][0]++, {
            subject: peerCert.subject,
            issuer: peerCert.issuer,
            validFrom: peerCert.valid_from,
            validTo: peerCert.valid_to
          }) :
          /* istanbul ignore next */
          (cov_2qnx9foqgg().b[11][1]++, null)
        };
      } catch {
        // TLS info not available
      }
    } else
    /* istanbul ignore next */
    {
      cov_2qnx9foqgg().b[7][1]++;
    }
    cov_2qnx9foqgg().s[27]++;
    return {
      connection: connectionInfo,
      tls: tlsDetails,
      config: {
        httpsEnabled: tlsServerConfig.enabled,
        ciphers: tlsServerConfig.ciphers.split(":").slice(0, 5) // First 5 ciphers
      },
      timestamp: new Date().toISOString()
    };
  });

  /**
   * GET /api/security/headers
   *
   * Returns security headers that are set on responses.
   * Useful for verifying security header configuration.
   */
  /* istanbul ignore next */
  cov_2qnx9foqgg().s[28]++;
  fastify.get("/headers", {
    schema: {
      description: "Get security headers information",
      tags: ["Security"]
    }
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_2qnx9foqgg().f[4]++;
    // These headers should be set by Helmet
    const securityHeaders =
    /* istanbul ignore next */
    (cov_2qnx9foqgg().s[29]++, ["Content-Security-Policy", "Strict-Transport-Security", "X-Content-Type-Options", "X-Frame-Options", "X-XSS-Protection", "Referrer-Policy", "Permissions-Policy", "Cache-Control", "Cross-Origin-Resource-Policy", "Cross-Origin-Opener-Policy"]);

    // Return info about expected headers
    /* istanbul ignore next */
    cov_2qnx9foqgg().s[30]++;
    return {
      message: "Security headers are configured via Helmet middleware",
      expectedHeaders: securityHeaders,
      note: "Check response headers on any endpoint to verify these are set",
      timestamp: new Date().toISOString()
    };
  });
}
export default securityRoutes;
