    preHandler: [requireAdmin],
    schema: {
      body: {
        type: 'object',
        required: ['name', 'resource', 'action'],
        properties: {
          name: {
            type: 'string',
            minLength: 1,
            maxLength: 255
          },
          resource: {
            type: 'string',
            minLength: 1,
            maxLength: 100
          },
          action: {
            type: 'string',
            minLength: 1,
            maxLength: 50
          },
          description: {
            type: 'string'
          },
          guard_name: {
            type: 'string',
            default: 'web'
          }
        }
      }
    }
  }, createPermission);

  /**
   * GET /permissions/:id
   * Get permission by ID with associated roles
   */
  /* istanbul ignore next */
  cov_2ew4bybopv().s[11]++;
