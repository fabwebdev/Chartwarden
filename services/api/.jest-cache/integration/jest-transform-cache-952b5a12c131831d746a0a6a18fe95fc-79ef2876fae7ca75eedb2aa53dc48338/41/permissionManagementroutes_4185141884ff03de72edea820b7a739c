/**
 * Permission Management Routes
 *
 * Provides comprehensive REST API endpoints for:
 * - Permission CRUD operations
 * - Role Assignment operations
 * - Permission-Role Association management
 * - User-Role queries
 * - Audit trail retrieval
 *
 * All routes require authentication and appropriate permissions.
 *
 * @module permissionManagement.routes
 */

import {
// Permission CRUD
getAllPermissions, getPermissionList, createPermission, getPermissionById, updatePermission, softDeletePermission, hardDeletePermission,
// Role Assignment
assignRolesToUser, revokeRolesFromUser, replaceUserRoles,
// Permission-Role Association
getPermissionRoles,
// User-Role Queries
getUserRoles, getUserPermissions, checkUserPermission, getRoleUsers,
// Audit
getPermissionAuditTrail } from '../controllers/PermissionManagement.controller.js';
import { requireAdmin, requirePermission } from '../middleware/rbac.middleware.js';
import { PERMISSIONS } from '../config/rbac.js';

/**
 * Middleware to check if user can view their own data
 * Allows users to view their own roles/permissions without admin rights
 */
/* istanbul ignore next */
cov_2ew4bybopv().s[0]++;
const requireAdminOrSelf = async (request, reply) => {
  /* istanbul ignore next */
  cov_2ew4bybopv().f[0]++;
  cov_2ew4bybopv().s[1]++;
  if (!request.user) {
    /* istanbul ignore next */
    cov_2ew4bybopv().b[0][0]++;
    cov_2ew4bybopv().s[2]++;
    return reply.code(401).send({
      status: 401,
      message: 'Access denied. Authentication required.'
    });
  } else
  /* istanbul ignore next */
  {
    cov_2ew4bybopv().b[0][1]++;
  }
  const requestedUserId =
  /* istanbul ignore next */
  (cov_2ew4bybopv().s[3]++,
  /* istanbul ignore next */
  (cov_2ew4bybopv().b[1][0]++, request.params.userId) ||
  /* istanbul ignore next */
  (cov_2ew4bybopv().b[1][1]++, request.params.id));
  const isViewingOwnData =
  /* istanbul ignore next */
  (cov_2ew4bybopv().s[4]++, request.user.id === requestedUserId);
  const isAdmin =
  /* istanbul ignore next */
  (cov_2ew4bybopv().s[5]++, request.user.role === 'admin');
  /* istanbul ignore next */
  cov_2ew4bybopv().s[6]++;
  if (
  /* istanbul ignore next */
  (cov_2ew4bybopv().b[3][0]++, !isViewingOwnData) &&
  /* istanbul ignore next */
  (cov_2ew4bybopv().b[3][1]++, !isAdmin)) {
    /* istanbul ignore next */
    cov_2ew4bybopv().b[2][0]++;
    cov_2ew4bybopv().s[7]++;
    return reply.code(403).send({
      status: 403,
      message: 'Access denied. Can only view your own data unless admin.'
    });
  } else
  /* istanbul ignore next */
  {
    cov_2ew4bybopv().b[2][1]++;
  }
};

// Fastify plugin for permission management routes
async function permissionManagementRoutes(fastify, options) {
  /* istanbul ignore next */
  cov_2ew4bybopv().f[1]++;
  cov_2ew4bybopv().s[8]++;
  // ============================================================================
  // PERMISSION CRUD ENDPOINTS
  // ============================================================================

  /**
   * GET /permissions
   * Get all permissions with pagination, filtering, and sorting
   * Query params: page, per_page, sort_by, sort_order, search, resource, action, is_active, guard_name
   */
  fastify.get('/permissions', {
    preHandler: [requirePermission(PERMISSIONS.MANAGE_PERMISSIONS)],
    schema: {
      querystring: {
        type: 'object',
        properties: {
          page: {
            type: 'integer',
            minimum: 1,
            default: 1
          },
          per_page: {
            type: 'integer',
            minimum: 1,
            maximum: 100,
            default: 50
          },
          sort_by: {
            type: 'string',
            enum: ['name', 'resource', 'action', 'created_at', 'updated_at'],
            default: 'created_at'
          },
          sort_order: {
            type: 'string',
            enum: ['asc', 'desc'],
            default: 'desc'
          },
          search: {
            type: 'string'
          },
          resource: {
            type: 'string'
          },
          action: {
            type: 'string'
          },
          is_active: {
            type: 'string'
          },
          guard_name: {
            type: 'string'
          }
        }
      }
    }
  }, getAllPermissions);

  /**
   * GET /permissions/list
   * Get permission list for dropdowns (names only)
   */
  /* istanbul ignore next */
  cov_2ew4bybopv().s[9]++;
  fastify.get('/permissions/list', {
    preHandler: [requirePermission(PERMISSIONS.MANAGE_PERMISSIONS)]
  }, getPermissionList);

  /**
   * POST /permissions
   * Create a new permission
   */
  /* istanbul ignore next */
  cov_2ew4bybopv().s[10]++;
