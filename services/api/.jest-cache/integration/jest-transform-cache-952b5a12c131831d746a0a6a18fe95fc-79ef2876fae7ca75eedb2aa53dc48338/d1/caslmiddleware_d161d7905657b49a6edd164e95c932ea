    }));
    /* istanbul ignore next */
    cov_ogsd5x3bm().s[20]++;
    if (!hasAnyAbility) {
      /* istanbul ignore next */
      cov_ogsd5x3bm().b[6][0]++;
      cov_ogsd5x3bm().s[21]++;
      return reply.code(403).send({
        status: 403,
        message: "Access denied. Insufficient permissions."
      });
    } else
    /* istanbul ignore next */
    {
      cov_ogsd5x3bm().b[6][1]++;
    }
  };
};

