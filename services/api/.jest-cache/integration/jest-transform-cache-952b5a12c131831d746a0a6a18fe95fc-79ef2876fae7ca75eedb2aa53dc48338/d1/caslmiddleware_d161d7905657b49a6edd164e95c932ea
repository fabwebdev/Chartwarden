    if (!request.user) {
      /* istanbul ignore next */
      cov_ogsd5x3bm().b[4][0]++;
      cov_ogsd5x3bm().s[15]++;
      return reply.code(401).send({
        status: 401,
        message: "Access denied. Authentication required."
      });
    } else
