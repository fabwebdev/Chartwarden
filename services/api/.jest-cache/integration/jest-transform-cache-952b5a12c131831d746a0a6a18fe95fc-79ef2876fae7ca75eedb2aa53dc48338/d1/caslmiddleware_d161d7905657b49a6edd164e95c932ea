    const hasAnyAbility =
    /* istanbul ignore next */
    (cov_ogsd5x3bm().s[18]++, actions.some(action => {
      /* istanbul ignore next */
      cov_ogsd5x3bm().f[5]++;
      cov_ogsd5x3bm().s[19]++;
