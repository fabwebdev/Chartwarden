    /* istanbul ignore next */
    (cov_ogsd5x3bm().s[28]++, actions.every(action => {
      /* istanbul ignore next */
      cov_ogsd5x3bm().f[8]++;
      cov_ogsd5x3bm().s[29]++;
      return request.ability.can(action, subject);
    }));
    /* istanbul ignore next */
    cov_ogsd5x3bm().s[30]++;
    if (!hasAllAbilities) {
      /* istanbul ignore next */
      cov_ogsd5x3bm().b[9][0]++;
      cov_ogsd5x3bm().s[31]++;
      return reply.code(403).send({
        status: 403,
        message: "Access denied. Insufficient permissions."
      });
    } else
    /* istanbul ignore next */
    {
      cov_ogsd5x3bm().b[9][1]++;
    }
  };
};
export default {
  injectCaslAbility,
  requireCaslAbility,
  requireAnyCaslAbility,
  requireAllCaslAbilities
};
