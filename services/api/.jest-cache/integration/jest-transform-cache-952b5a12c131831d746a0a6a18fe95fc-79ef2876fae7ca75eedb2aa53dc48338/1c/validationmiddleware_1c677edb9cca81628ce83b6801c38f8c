export const fields =
/* istanbul ignore next */
(cov_dve9g5g8o().s[49]++, {
  /**
   * UUID field validator
   */
  uuid: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[12]++;
    cov_dve9g5g8o().s[50]++;
    return yup.string().uuid('Invalid ID format');
  },
  /**
   * Required UUID (for route params)
   */
  requiredUuid: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[13]++;
    cov_dve9g5g8o().s[51]++;
    return yup.string().uuid('Invalid ID format').required('ID is required');
  },
  /**
   * Email field with sanitization
   */
  email: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[14]++;
    cov_dve9g5g8o().s[52]++;
    return sanitizedString().email('Please enter a valid email address').max(255, 'Email must be at most 255 characters').lowercase();
  },
  /**
   * Required email
   */
  requiredEmail: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[15]++;
    cov_dve9g5g8o().s[53]++;
    return fields.email().required('Email is required');
  },
  /**
   * Phone number with basic formatting validation
   */
  phone: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[16]++;
    cov_dve9g5g8o().s[54]++;
    return sanitizedString().matches(/^[\d\s\-\+\(\)\.]+$/, 'Please enter a valid phone number').max(20, 'Phone number is too long');
  },
  /**
   * Name field (first name, last name, etc.)
   */
  name: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[17]++;
    cov_dve9g5g8o().s[55]++;
    return sanitizedString().min(1, 'Name is required').max(100, 'Name must be at most 100 characters');
  },
  /**
   * Required name
   */
  requiredName: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[18]++;
    cov_dve9g5g8o().s[56]++;
    return fields.name().required('Name is required');
  },
  /**
   * Password field (no sanitization to preserve special chars)
   */
  password: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[19]++;
    cov_dve9g5g8o().s[57]++;
    return yup.string().min(8, 'Password must be at least 8 characters').max(128, 'Password must be at most 128 characters');
  },
  /**
   * Required password
   */
  requiredPassword: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[20]++;
    cov_dve9g5g8o().s[58]++;
    return fields.password().required('Password is required');
  },
  /**
   * Date field (accepts ISO date strings)
   */
  date: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[21]++;
    cov_dve9g5g8o().s[59]++;
    return yup.date().typeError('Please enter a valid date');
  },
  /**
   * Required date
   */
  requiredDate: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[22]++;
    cov_dve9g5g8o().s[60]++;
    return fields.date().required('Date is required');
  },
  /**
   * Text area / long text content
   */
  text: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[23]++;
    cov_dve9g5g8o().s[61]++;
    return sanitizedString().max(10000, 'Text content is too long');
  },
  /**
   * Short description or notes
   */
  shortText: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[24]++;
    cov_dve9g5g8o().s[62]++;
    return sanitizedString().max(500, 'Text must be at most 500 characters');
  },
  /**
   * Medical Record Number (MRN)
   */
  mrn: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[25]++;
    cov_dve9g5g8o().s[63]++;
    return sanitizedString().matches(/^[A-Za-z0-9\-]+$/, 'Please enter a valid MRN');
  },
  /**
   * Social Security Number (format: XXX-XX-XXXX or XXXXXXXXX)
   */
  ssn: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[26]++;
    cov_dve9g5g8o().s[64]++;
    return sanitizedString().matches(/^(\d{3}-?\d{2}-?\d{4}|\d{9})$/, 'Please enter a valid SSN');
  },
  /**
   * National Provider Identifier (NPI)
   */
  npi: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[27]++;
    cov_dve9g5g8o().s[65]++;
    return sanitizedString().matches(/^\d{10}$/, 'Please enter a valid 10-digit NPI');
  },
  /**
   * Positive integer
   */
  positiveInt: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[28]++;
    cov_dve9g5g8o().s[66]++;
    return yup.number().integer('Must be a whole number').positive('Must be a positive number');
  },
  /**
   * Boolean field
