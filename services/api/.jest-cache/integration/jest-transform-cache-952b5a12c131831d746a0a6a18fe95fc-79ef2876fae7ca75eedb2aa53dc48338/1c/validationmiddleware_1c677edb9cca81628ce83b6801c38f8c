  boolean: () => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[29]++;
    cov_dve9g5g8o().s[67]++;
    return yup.boolean();
  },
  /**
   * Enum/select field
   * @param {Array} values - Allowed values
   * @param {string} [message] - Custom error message
