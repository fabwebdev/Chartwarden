import { ErrorCodes } from '../constants/errorCodes.js';

/**
 * Sanitize string input to prevent XSS and clean whitespace
 * @param {string} value - Input value to sanitize
 * @returns {string} Sanitized string
 */
function sanitizeString(value) {
  /* istanbul ignore next */
  cov_dve9g5g8o().f[0]++;
  cov_dve9g5g8o().s[0]++;
  if (typeof value !== 'string') {
    /* istanbul ignore next */
    cov_dve9g5g8o().b[0][0]++;
    cov_dve9g5g8o().s[1]++;
    return value;
  } else
  /* istanbul ignore next */
  {
    cov_dve9g5g8o().b[0][1]++;
  }
  cov_dve9g5g8o().s[2]++;
  return value.trim()
  // Remove null bytes
  .replace(/\0/g, '')
  // Escape HTML entities to prevent XSS
  .replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;').replace(/"/g, '&quot;').replace(/'/g, '&#x27;');
}

/**
 * Create a sanitized string schema
 * Use this for any user-provided text input
 */
/* istanbul ignore next */
cov_dve9g5g8o().s[3]++;
export const sanitizedString = () => {
  /* istanbul ignore next */
  cov_dve9g5g8o().f[1]++;
  cov_dve9g5g8o().s[4]++;
  return yup.string().transform(value => {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[2]++;
    cov_dve9g5g8o().s[5]++;
    return value ?
    /* istanbul ignore next */
    (cov_dve9g5g8o().b[1][0]++, sanitizeString(value)) :
    /* istanbul ignore next */
    (cov_dve9g5g8o().b[1][1]++, value);
  });
};

