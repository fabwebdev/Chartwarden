/**
 * Transform Yup validation errors into application ValidationError
 * @param {yup.ValidationError} yupError - Yup validation error
 * @returns {ValidationError} Application validation error
 */
function transformYupError(yupError) {
  /* istanbul ignore next */
  cov_dve9g5g8o().f[6]++;
  const errors =
  /* istanbul ignore next */
  (cov_dve9g5g8o().s[25]++, []);
  /* istanbul ignore next */
  cov_dve9g5g8o().s[26]++;
  if (
  /* istanbul ignore next */
  (cov_dve9g5g8o().b[13][0]++, yupError.inner) &&
  /* istanbul ignore next */
  (cov_dve9g5g8o().b[13][1]++, yupError.inner.length > 0)) {
    /* istanbul ignore next */
    cov_dve9g5g8o().b[12][0]++;
    cov_dve9g5g8o().s[27]++;
    // Multiple validation errors
    for (const err of yupError.inner) {
      /* istanbul ignore next */
      cov_dve9g5g8o().s[28]++;
      errors.push({
        field:
        /* istanbul ignore next */
        (cov_dve9g5g8o().b[14][0]++, err.path) ||
        /* istanbul ignore next */
        (cov_dve9g5g8o().b[14][1]++, 'unknown'),
        message: err.message,
        code: getValidationErrorCode(err.type, err.path)
      });
    }
  } else {
    /* istanbul ignore next */
    cov_dve9g5g8o().b[12][1]++;
    cov_dve9g5g8o().s[29]++;
    // Single validation error
    errors.push({
      field:
      /* istanbul ignore next */
      (cov_dve9g5g8o().b[15][0]++, yupError.path) ||
      /* istanbul ignore next */
      (cov_dve9g5g8o().b[15][1]++, 'unknown'),
      message: yupError.message,
      code: getValidationErrorCode(yupError.type, yupError.path)
    });
  }
  const validationError =
  /* istanbul ignore next */
  (cov_dve9g5g8o().s[30]++, new ValidationError('The request contains invalid data', {
    code: ErrorCodes.VALIDATION_FAILED,
    errors
  }));
  /* istanbul ignore next */
  cov_dve9g5g8o().s[31]++;
  return validationError;
}

/**
 * Validation configuration options
 * @typedef {Object} ValidationOptions
 * @property {yup.Schema} [body] - Schema for request body
 * @property {yup.Schema} [query] - Schema for query parameters
 * @property {yup.Schema} [params] - Schema for URL parameters
 * @property {boolean} [stripUnknown=true] - Remove fields not in schema
 * @property {boolean} [abortEarly=false] - Return all errors, not just first
 */

/**
 * Create validation middleware from Yup schema(s)
 *
 * @param {ValidationOptions|yup.Schema} schemaOrOptions - Yup schema or options object
 * @returns {Function} Fastify preHandler middleware
 *
 * @example
 * // Simple body validation
 * validate(yup.object({ name: yup.string().required() }))
 *
 * @example
 * // Full options
 * validate({
 *   body: createPatientSchema,
 *   query: paginationSchema,
 *   params: yup.object({ id: yup.string().uuid().required() })
 * })
 */
export function validate(schemaOrOptions) {
  /* istanbul ignore next */
  cov_dve9g5g8o().f[7]++;
  // Normalize input to options object
  const options =
  /* istanbul ignore next */
  (cov_dve9g5g8o().s[32]++, schemaOrOptions instanceof yup.Schema ?
  /* istanbul ignore next */
  (cov_dve9g5g8o().b[16][0]++, {
    body: schemaOrOptions
  }) :
  /* istanbul ignore next */
  (cov_dve9g5g8o().b[16][1]++, schemaOrOptions));
  const {
    body: bodySchema,
    query: querySchema,
    params: paramsSchema,
    stripUnknown =
    /* istanbul ignore next */
    (cov_dve9g5g8o().b[17][0]++, true),
    abortEarly =
    /* istanbul ignore next */
    (cov_dve9g5g8o().b[18][0]++, false)
  } =
  /* istanbul ignore next */
  (cov_dve9g5g8o().s[33]++, options);
  /* istanbul ignore next */
  cov_dve9g5g8o().s[34]++;
  return async function validationMiddleware(request, reply) {
    /* istanbul ignore next */
    cov_dve9g5g8o().f[8]++;
    const validationContext =
    /* istanbul ignore next */
    (cov_dve9g5g8o().s[35]++, {
      abortEarly,
      stripUnknown
    });
    /* istanbul ignore next */
    cov_dve9g5g8o().s[36]++;
    try {
      /* istanbul ignore next */
      cov_dve9g5g8o().s[37]++;
      // Validate body
      if (
      /* istanbul ignore next */
      (cov_dve9g5g8o().b[20][0]++, bodySchema) &&
      /* istanbul ignore next */
      (cov_dve9g5g8o().b[20][1]++, request.body)) {
        /* istanbul ignore next */
        cov_dve9g5g8o().b[19][0]++;
        cov_dve9g5g8o().s[38]++;
        request.body = await bodySchema.validate(request.body, validationContext);
      } else
      /* istanbul ignore next */
      {
        cov_dve9g5g8o().b[19][1]++;
      }

      // Validate query parameters
      cov_dve9g5g8o().s[39]++;
      if (
      /* istanbul ignore next */
      (cov_dve9g5g8o().b[22][0]++, querySchema) &&
      /* istanbul ignore next */
      (cov_dve9g5g8o().b[22][1]++, request.query)) {
        /* istanbul ignore next */
        cov_dve9g5g8o().b[21][0]++;
        cov_dve9g5g8o().s[40]++;
        request.query = await querySchema.validate(request.query, validationContext);
      } else
      /* istanbul ignore next */
      {
        cov_dve9g5g8o().b[21][1]++;
      }

      // Validate URL parameters
      cov_dve9g5g8o().s[41]++;
      if (
      /* istanbul ignore next */
      (cov_dve9g5g8o().b[24][0]++, paramsSchema) &&
      /* istanbul ignore next */
      (cov_dve9g5g8o().b[24][1]++, request.params)) {
        /* istanbul ignore next */
        cov_dve9g5g8o().b[23][0]++;
        cov_dve9g5g8o().s[42]++;
        request.params = await paramsSchema.validate(request.params, validationContext);
      } else
      /* istanbul ignore next */
      {
        cov_dve9g5g8o().b[23][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_dve9g5g8o().s[43]++;
      if (error instanceof yup.ValidationError) {
        /* istanbul ignore next */
        cov_dve9g5g8o().b[25][0]++;
        cov_dve9g5g8o().s[44]++;
