/**
 * Get error code from Yup validation error type
 * @param {string} type - Yup error type
 * @param {string} path - Field path for specific field codes
 * @returns {string} Error code
 */
function getValidationErrorCode(type, path) {
  /* istanbul ignore next */
  cov_dve9g5g8o().f[5]++;
  cov_dve9g5g8o().s[10]++;
  // Check for specific field types
  if (path) {
    /* istanbul ignore next */
    cov_dve9g5g8o().b[3][0]++;
    const fieldName =
    /* istanbul ignore next */
    (cov_dve9g5g8o().s[11]++, path.toLowerCase());
    /* istanbul ignore next */
    cov_dve9g5g8o().s[12]++;
    if (fieldName.includes('email')) {
      /* istanbul ignore next */
      cov_dve9g5g8o().b[4][0]++;
      cov_dve9g5g8o().s[13]++;
      return ErrorCodes.VALIDATION_INVALID_EMAIL;
    } else
    /* istanbul ignore next */
    {
      cov_dve9g5g8o().b[4][1]++;
    }
    cov_dve9g5g8o().s[14]++;
    if (fieldName.includes('phone')) {
      /* istanbul ignore next */
      cov_dve9g5g8o().b[5][0]++;
      cov_dve9g5g8o().s[15]++;
      return ErrorCodes.VALIDATION_INVALID_PHONE;
    } else
    /* istanbul ignore next */
    {
      cov_dve9g5g8o().b[5][1]++;
    }
    cov_dve9g5g8o().s[16]++;
    if (fieldName.includes('ssn')) {
      /* istanbul ignore next */
      cov_dve9g5g8o().b[6][0]++;
      cov_dve9g5g8o().s[17]++;
      return ErrorCodes.VALIDATION_INVALID_SSN;
    } else
    /* istanbul ignore next */
    {
      cov_dve9g5g8o().b[6][1]++;
    }
    cov_dve9g5g8o().s[18]++;
    if (fieldName.includes('mrn')) {
      /* istanbul ignore next */
      cov_dve9g5g8o().b[7][0]++;
      cov_dve9g5g8o().s[19]++;
      return ErrorCodes.VALIDATION_INVALID_MRN;
