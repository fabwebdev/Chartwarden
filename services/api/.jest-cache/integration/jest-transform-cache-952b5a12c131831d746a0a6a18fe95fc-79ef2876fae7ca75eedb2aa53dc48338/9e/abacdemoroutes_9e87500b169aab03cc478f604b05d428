import { authenticate } from "../middleware/betterAuth.middleware.js";
import { requireRole, requirePermission } from "../middleware/rbac.middleware.js";
import { requireAbacAccess, requirePatientRecordAccess, requireRbacAndAbac, requireRbacOrAbac } from "../middleware/abac.middleware.js";
import { ROLES, PERMISSIONS } from "../config/rbac.js";

// Fastify plugin for ABAC demo routes
async function abacDemoRoutes(fastify, options) {
  /* istanbul ignore next */
  cov_1ftjep2fj9().f[0]++;
  cov_1ftjep2fj9().s[0]++;
  // Demo route showing RBAC only - Admins can access
  fastify.get("/rbac-only/admin", {
    preHandler: [authenticate, requireRole(ROLES.ADMIN)]
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_1ftjep2fj9().f[1]++;
    cov_1ftjep2fj9().s[1]++;
    return {
      status: 200,
      message: "RBAC Only: Admins can access this route",
      user: request.user
    };
  });

  // Demo route showing RBAC only - Healthcare staff can access
  /* istanbul ignore next */
  cov_1ftjep2fj9().s[2]++;
  fastify.get("/rbac-only/healthcare", {
    preHandler: [authenticate, requireRole(ROLES.ADMIN, ROLES.DOCTOR, ROLES.NURSE)]
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_1ftjep2fj9().f[2]++;
    cov_1ftjep2fj9().s[3]++;
    return {
      status: 200,
      message: "RBAC Only: Healthcare staff can access this route",
      user: request.user
    };
  });

  // Demo route showing ABAC only - Access based on patient record ownership
  /* istanbul ignore next */
  cov_1ftjep2fj9().s[4]++;
  fastify.get("/abac-only/patient/:id", {
    preHandler: [authenticate, requirePatientRecordAccess(request => {
      /* istanbul ignore next */
      cov_1ftjep2fj9().f[3]++;
      cov_1ftjep2fj9().s[5]++;
      return request.params.id;
    })]
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_1ftjep2fj9().f[4]++;
    cov_1ftjep2fj9().s[6]++;
    return {
      status: 200,
      message: "ABAC Only: Access granted based on patient record ownership",
      user: request.user,
      patientId: request.params.id
    };
  });

  // Demo route showing RBAC and ABAC together - User must have permission AND satisfy ABAC policies
  /* istanbul ignore next */
  cov_1ftjep2fj9().s[7]++;
  fastify.get("/rbac-and-abac/patient/:id", {
    preHandler: [authenticate, requireRbacAndAbac([PERMISSIONS.VIEW_PATIENT], {
      type: "patient_record",
      ownerId: null /* will be set dynamically */
    })]
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_1ftjep2fj9().f[5]++;
    cov_1ftjep2fj9().s[8]++;
    return {
      status: 200,
      message: "RBAC and ABAC: Both permission and attribute policies satisfied",
      user: request.user,
      patientId: request.params.id
    };
  });

  // Demo route showing RBAC or ABAC - User can satisfy either RBAC permissions or ABAC policies
  /* istanbul ignore next */
  cov_1ftjep2fj9().s[9]++;
  fastify.get("/rbac-or-abac/patient/:id", {
    preHandler: [authenticate, requireRbacOrAbac([PERMISSIONS.VIEW_PATIENT], {
      type: "patient_record",
      ownerId: null /* will be set dynamically */
    })]
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_1ftjep2fj9().f[6]++;
    cov_1ftjep2fj9().s[10]++;
    return {
      status: 200,
      message: "RBAC or ABAC: Either permission or attribute policies satisfied",
      user: request.user,
      patientId: request.params.id
    };
  });

  // Demo route showing complex access control - Admins always have access, doctors have access during business hours
  /* istanbul ignore next */
  cov_1ftjep2fj9().s[11]++;
  fastify.get("/complex-access/patient/:id", {
    preHandler: [authenticate, async (request, reply) => {
      /* istanbul ignore next */
      cov_1ftjep2fj9().f[7]++;
      cov_1ftjep2fj9().s[12]++;
      // First check if user is admin (they always have access)
      if (request.user.role === ROLES.ADMIN) {
        /* istanbul ignore next */
        cov_1ftjep2fj9().b[0][0]++;
        cov_1ftjep2fj9().s[13]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_1ftjep2fj9().b[0][1]++;
      }

      // For non-admins, apply ABAC access control
      cov_1ftjep2fj9().s[14]++;
      await requirePatientRecordAccess(request => {
        /* istanbul ignore next */
        cov_1ftjep2fj9().f[8]++;
        cov_1ftjep2fj9().s[15]++;
        return request.params.id;
      })(request, reply);
    }]
  }, async (request, reply) => {
    /* istanbul ignore next */
    cov_1ftjep2fj9().f[9]++;
    cov_1ftjep2fj9().s[16]++;
    return {
      status: 200,
      message: "Complex Access Control: Access granted based on role and attributes",
      user: request.user,
      patientId: request.params.id
    };
  });
}
export default abacDemoRoutes;
