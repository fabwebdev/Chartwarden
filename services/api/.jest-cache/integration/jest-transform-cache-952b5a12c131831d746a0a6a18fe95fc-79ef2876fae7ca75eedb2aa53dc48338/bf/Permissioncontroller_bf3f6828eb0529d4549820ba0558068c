    cov_2nytf8d6bw().s[78]++;
    logger.error("Error updating permission:", error);
    /* istanbul ignore next */
    cov_2nytf8d6bw().s[79]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_2nytf8d6bw().s[80]++;
    return {
      status: 500,
      message: "Server error while updating permission"
    };
  }
};

// Delete permission by ID
/* istanbul ignore next */
cov_2nytf8d6bw().s[81]++;
export const deletePermission = async (request, reply) => {
  /* istanbul ignore next */
  cov_2nytf8d6bw().f[6]++;
  cov_2nytf8d6bw().s[82]++;
  try {
    const {
      id
    } =
    /* istanbul ignore next */
    (cov_2nytf8d6bw().s[83]++, request.params);

    // Find permission
    const permissionResult =
    /* istanbul ignore next */
    (cov_2nytf8d6bw().s[84]++, await db.select({
      id: permissions.id
    }).from(permissions).where(eq(permissions.id, parseInt(id))).limit(1));
    /* istanbul ignore next */
    cov_2nytf8d6bw().s[85]++;
    if (permissionResult.length === 0) {
      /* istanbul ignore next */
      cov_2nytf8d6bw().b[13][0]++;
      cov_2nytf8d6bw().s[86]++;
      reply.code(404);
      /* istanbul ignore next */
      cov_2nytf8d6bw().s[87]++;
      return {
        status: 404,
        message: "Permission not found"
      };
    } else
    /* istanbul ignore next */
    {
      cov_2nytf8d6bw().b[13][1]++;
    }

    // Delete role-permission associations first
    cov_2nytf8d6bw().s[88]++;
    await db.delete(role_has_permissions).where(eq(role_has_permissions.permission_id, parseInt(id)));

    // Delete permission
    /* istanbul ignore next */
    cov_2nytf8d6bw().s[89]++;
    await db.delete(permissions).where(eq(permissions.id, parseInt(id)));
    /* istanbul ignore next */
    cov_2nytf8d6bw().s[90]++;
    return {
      status: 200,
      message: "Permission deleted successfully"
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_2nytf8d6bw().s[91]++;
    logger.error("Error deleting permission:", error);
    /* istanbul ignore next */
    cov_2nytf8d6bw().s[92]++;
    reply.code(500);
    /* istanbul ignore next */
    cov_2nytf8d6bw().s[93]++;
    return {
      status: 500,
      message: "Server error while deleting permission"
    };
  }
};

// Validation rules for creating/updating permission
// Note: In Fastify, validation should be done using schema in route definitions
// This is kept for compatibility but should be replaced with Fastify schema validation
/* istanbul ignore next */
cov_2nytf8d6bw().s[94]++;
export const permissionValidation = async (request, reply) => {
  /* istanbul ignore next */
  cov_2nytf8d6bw().f[7]++;
  cov_2nytf8d6bw().s[95]++;
  // Basic validation - should be done in route schema
  if (!request.body.name) {
    /* istanbul ignore next */
    cov_2nytf8d6bw().b[14][0]++;
    cov_2nytf8d6bw().s[96]++;
    reply.code(400);
    /* istanbul ignore next */
    cov_2nytf8d6bw().s[97]++;
    return {
      status: 400,
      message: "Validation failed",
      errors: [{
        field: "name",
        message: "Permission name is required"
      }]
    };
  } else
  /* istanbul ignore next */
  {
    cov_2nytf8d6bw().b[14][1]++;
  }
};
