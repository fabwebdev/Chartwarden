export const authenticate = async (request, reply) => {
  /* istanbul ignore next */
  cov_2fvf0hqmmr().f[0]++;
  cov_2fvf0hqmmr().s[1]++;
  try {
    // Debug: Log cookies received
    const sessionToken =
    /* istanbul ignore next */
    (cov_2fvf0hqmmr().s[2]++, request.cookies?.["better-auth.session_token"]);
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[3]++;
    debug("Authentication attempt", {
      hasCookie: !!sessionToken,
      cookieCount: Object.keys(
      /* istanbul ignore next */
      (cov_2fvf0hqmmr().b[0][0]++, request.cookies) ||
      /* istanbul ignore next */
      (cov_2fvf0hqmmr().b[0][1]++, {})).length
    });

    // Get session from Better Auth
    let session;
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[4]++;
    try {
      /* istanbul ignore next */
      cov_2fvf0hqmmr().s[5]++;
      session = await auth.api.getSession({
        headers: fromNodeHeaders(request.headers),
        cookies: request.cookies
      });
    } catch (err) {
      /* istanbul ignore next */
      cov_2fvf0hqmmr().s[6]++;
      error("Better Auth getSession error", {
        message: err.message,
        code: err.code,
        detail: err.detail,
        hint: err.hint
      });

      // If it's a table not found error, provide helpful message
      /* istanbul ignore next */
      cov_2fvf0hqmmr().s[7]++;
      if (
      /* istanbul ignore next */
      (cov_2fvf0hqmmr().b[2][0]++, err.message?.includes("does not exist")) ||
      /* istanbul ignore next */
      (cov_2fvf0hqmmr().b[2][1]++, err.code === "42P01")) {
        /* istanbul ignore next */
        cov_2fvf0hqmmr().b[1][0]++;
        cov_2fvf0hqmmr().s[8]++;
        warn("Database table error detected. Check if sessions table exists in public schema.");
      } else
      /* istanbul ignore next */
      {
        cov_2fvf0hqmmr().b[1][1]++;
      }
      cov_2fvf0hqmmr().s[9]++;
      return reply.code(500).send({
        status: 500,
        message: "Server error during session validation.",
        error: process.env.NODE_ENV === "development" ?
        /* istanbul ignore next */
        (cov_2fvf0hqmmr().b[3][0]++, err.message) :
        /* istanbul ignore next */
        (cov_2fvf0hqmmr().b[3][1]++, undefined)
      });
    }
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[10]++;
    if (!session) {
      /* istanbul ignore next */
      cov_2fvf0hqmmr().b[4][0]++;
      cov_2fvf0hqmmr().s[11]++;
      // Debug: Check if session exists in database
      if (sessionToken) {
        /* istanbul ignore next */
        cov_2fvf0hqmmr().b[5][0]++;
        cov_2fvf0hqmmr().s[12]++;
        try {
          const {
            db
          } =
          /* istanbul ignore next */
          (cov_2fvf0hqmmr().s[13]++, await import("../config/db.drizzle.js"));
          const {
            sessions
          } =
          /* istanbul ignore next */
          (cov_2fvf0hqmmr().s[14]++, await import("../db/schemas/index.js"));
          const {
            eq
          } =
          /* istanbul ignore next */
          (cov_2fvf0hqmmr().s[15]++, await import("drizzle-orm"));
          const dbSession =
          /* istanbul ignore next */
          (cov_2fvf0hqmmr().s[16]++, await db.select().from(sessions).where(eq(sessions.token, sessionToken)).limit(1));
          /* istanbul ignore next */
          cov_2fvf0hqmmr().s[17]++;
          warn("Better Auth cannot read session, but DB has record", {
            foundInDB: dbSession.length > 0
          });
        } catch (dbError) {
          /* istanbul ignore next */
          cov_2fvf0hqmmr().s[18]++;
          error("Error checking database session", dbError);
        }
      } else
      /* istanbul ignore next */
      {
        cov_2fvf0hqmmr().b[5][1]++;
      }
      cov_2fvf0hqmmr().s[19]++;
      return reply.code(401).send({
        status: 401,
        message: "Access denied. No valid session found."
      });
    } else
    /* istanbul ignore next */
    {
      cov_2fvf0hqmmr().b[4][1]++;
    }
    cov_2fvf0hqmmr().s[20]++;
    debug("Session found for authenticated user");

    // Fetch full user record from database to get firstName and lastName
    let fullUser =
    /* istanbul ignore next */
    (cov_2fvf0hqmmr().s[21]++, session.user);
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[22]++;
    try {
      const dbUser =
      /* istanbul ignore next */
      (cov_2fvf0hqmmr().s[23]++, await db.select().from(users).where(eq(users.id, session.user.id)).limit(1));
      /* istanbul ignore next */
      cov_2fvf0hqmmr().s[24]++;
      if (dbUser.length > 0) {
        /* istanbul ignore next */
        cov_2fvf0hqmmr().b[6][0]++;
        cov_2fvf0hqmmr().s[25]++;
        // Merge Better Auth user data with database user data (including firstName and lastName)
        // Use database email as-is (it should have original case if stored correctly)
        // Better Auth stores lowercase, but we'll use what's in database
        fullUser = {
          ...session.user,
          email: dbUser[0].email,
          // Use email from database (may have original case)
          firstName:
          /* istanbul ignore next */
          (cov_2fvf0hqmmr().b[7][0]++, dbUser[0].firstName) ||
          /* istanbul ignore next */
          (cov_2fvf0hqmmr().b[7][1]++, null),
          lastName:
          /* istanbul ignore next */
          (cov_2fvf0hqmmr().b[8][0]++, dbUser[0].lastName) ||
          /* istanbul ignore next */
          (cov_2fvf0hqmmr().b[8][1]++, null)
        };
      } else
      /* istanbul ignore next */
      {
        cov_2fvf0hqmmr().b[6][1]++;
      }
    } catch (userFetchError) {
      /* istanbul ignore next */
      cov_2fvf0hqmmr().s[26]++;
      error("Error fetching user details from database", userFetchError);
      // Continue with session.user if database fetch fails
    }

    // Add user info to request object
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[27]++;
    request.user = fullUser;
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[28]++;
    request.betterAuthSession = session; // Use a different property name to avoid conflicts

    // Load user's role from database
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[29]++;
    try {
      // Get user roles from user_has_roles table
      const userRoles =
      /* istanbul ignore next */
      (cov_2fvf0hqmmr().s[30]++, await db.select().from(user_has_roles).where(eq(user_has_roles.user_id, session.user.id)).limit(1));
      /* istanbul ignore next */
      cov_2fvf0hqmmr().s[31]++;
      if (userRoles.length > 0) {
        /* istanbul ignore next */
        cov_2fvf0hqmmr().b[9][0]++;
        // Get the role name from roles table
        const roleRecords =
        /* istanbul ignore next */
        (cov_2fvf0hqmmr().s[32]++, await db.select().from(roles).where(eq(roles.id, userRoles[0].role_id)).limit(1));
        /* istanbul ignore next */
        cov_2fvf0hqmmr().s[33]++;
        if (roleRecords.length > 0) {
          /* istanbul ignore next */
          cov_2fvf0hqmmr().b[10][0]++;
          cov_2fvf0hqmmr().s[34]++;
          request.user.role = roleRecords[0].name; // Set actual role from database
        } else {
          /* istanbul ignore next */
          cov_2fvf0hqmmr().b[10][1]++;
          cov_2fvf0hqmmr().s[35]++;
          request.user.role = ROLES.PATIENT; // Default if role not found
        }
      } else {
        /* istanbul ignore next */
        cov_2fvf0hqmmr().b[9][1]++;
        cov_2fvf0hqmmr().s[36]++;
        request.user.role = ROLES.PATIENT; // Default if no role assigned
      }
    } catch (roleError) {
      /* istanbul ignore next */
      cov_2fvf0hqmmr().s[37]++;
      error("Error loading user role", roleError);
      /* istanbul ignore next */
      cov_2fvf0hqmmr().s[38]++;
      request.user.role = ROLES.PATIENT; // Default on error
    }
  } catch (err) {
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[39]++;
    error("Authentication error", {
      err,
      method: request.method,
      path: request.url
    });
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[40]++;
    return reply.code(500).send({
      status: 500,
      message: "Server error during authentication.",
      error: err.message,
      details: process.env.NODE_ENV === "development" ?
      /* istanbul ignore next */
      (cov_2fvf0hqmmr().b[11][0]++, err.stack) :
      /* istanbul ignore next */
      (cov_2fvf0hqmmr().b[11][1]++, undefined)
    });
  }
};

// For compatibility with Laravel's verifyToken
export const verifyToken =
/* istanbul ignore next */
(cov_2fvf0hqmmr().s[41]++, authenticate);

// Middleware to get optional session (for routes that can be accessed by both authenticated and unauthenticated users)
/* istanbul ignore next */
cov_2fvf0hqmmr().s[42]++;
export const optionalAuth = async (request, reply) => {
  /* istanbul ignore next */
  cov_2fvf0hqmmr().f[1]++;
  cov_2fvf0hqmmr().s[43]++;
  try {
    // Get session from Better Auth (if exists)
    const session =
    /* istanbul ignore next */
    (cov_2fvf0hqmmr().s[44]++, await auth.api.getSession({
      headers: fromNodeHeaders(request.headers),
      cookies: request.cookies
    }));

    // Add user info to request object if session exists
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[45]++;
    if (session) {
      /* istanbul ignore next */
      cov_2fvf0hqmmr().b[12][0]++;
      cov_2fvf0hqmmr().s[46]++;
      request.user = session.user;
      /* istanbul ignore next */
      cov_2fvf0hqmmr().s[47]++;
      request.betterAuthSession = session; // Use a different property name to avoid conflicts

      // Load user's role from database
      /* istanbul ignore next */
      cov_2fvf0hqmmr().s[48]++;
      try {
        // Get user roles from user_has_roles table
        const userRoles =
        /* istanbul ignore next */
        (cov_2fvf0hqmmr().s[49]++, await db.select().from(user_has_roles).where(eq(user_has_roles.user_id, session.user.id)).limit(1));
        /* istanbul ignore next */
        cov_2fvf0hqmmr().s[50]++;
        if (userRoles.length > 0) {
          /* istanbul ignore next */
          cov_2fvf0hqmmr().b[13][0]++;
          // Get the role name from roles table
          const roleRecords =
          /* istanbul ignore next */
          (cov_2fvf0hqmmr().s[51]++, await db.select().from(roles).where(eq(roles.id, userRoles[0].role_id)).limit(1));
          /* istanbul ignore next */
          cov_2fvf0hqmmr().s[52]++;
          if (roleRecords.length > 0) {
            /* istanbul ignore next */
            cov_2fvf0hqmmr().b[14][0]++;
            cov_2fvf0hqmmr().s[53]++;
            request.user.role = roleRecords[0].name; // Set actual role from database
          } else {
            /* istanbul ignore next */
            cov_2fvf0hqmmr().b[14][1]++;
            cov_2fvf0hqmmr().s[54]++;
            request.user.role = ROLES.PATIENT; // Default if role not found
          }
        } else {
          /* istanbul ignore next */
          cov_2fvf0hqmmr().b[13][1]++;
          cov_2fvf0hqmmr().s[55]++;
          request.user.role = ROLES.PATIENT; // Default if no role assigned
        }
      } catch (roleError) {
        /* istanbul ignore next */
        cov_2fvf0hqmmr().s[56]++;
        error("Error loading user role", roleError);
        /* istanbul ignore next */
        cov_2fvf0hqmmr().s[57]++;
        request.user.role = ROLES.PATIENT; // Default on error
      }
    } else
    /* istanbul ignore next */
    {
      cov_2fvf0hqmmr().b[12][1]++;
    }
  } catch (err) {
    /* istanbul ignore next */
    cov_2fvf0hqmmr().s[58]++;
    error("Optional authentication error", err);
    // Still continue even if there's an error, as this is optional auth
  }
};
export default authenticate;
