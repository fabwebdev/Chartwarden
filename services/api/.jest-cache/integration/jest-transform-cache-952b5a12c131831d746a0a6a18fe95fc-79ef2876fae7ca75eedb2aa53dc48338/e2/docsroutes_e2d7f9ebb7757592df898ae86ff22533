      (cov_1trjw7lmvq().s[35]++, path.join(docsPath, 'openapi.yaml'));
      const yamlContent =
      /* istanbul ignore next */
      (cov_1trjw7lmvq().s[36]++, fs.readFileSync(yamlPath, 'utf8'));
      const parsed =
      /* istanbul ignore next */
      (cov_1trjw7lmvq().s[37]++, parseYamlSimple(yamlContent));
      /* istanbul ignore next */
      cov_1trjw7lmvq().s[38]++;
      reply.send({
        status: 200,
        data: {
          title: parsed.info.title,
          version: parsed.info.version,
          openApiVersion: parsed.info.openapi,
          stats: parsed.stats,
          endpoints: {
            swaggerUi: '/api/docs',
            openapiYaml: '/api/docs/openapi.yaml',
            openapiJson: '/api/docs/openapi.json',
            info: '/api/docs/info'
          },
          lastUpdated: new Date().toISOString()
        }
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_1trjw7lmvq().s[39]++;
      fastify.log.error('Error getting API info:', error);
      /* istanbul ignore next */
      cov_1trjw7lmvq().s[40]++;
      reply.code(500).send({
        status: 500,
        message: 'Error loading API information'
      });
    }
  });
}
export default docsRoutes;
