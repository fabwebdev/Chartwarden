# Development Auto-Login Configuration
# Copy this file to .env.local and customize as needed

# Enable/disable auto-login in development mode
# Set to "false" to disable auto-login and use manual login
NEXT_PUBLIC_DEV_AUTO_LOGIN=true

# Development credentials (only used when auto-login is enabled)
NEXT_PUBLIC_DEV_EMAIL=admin@chartwarden.dev
NEXT_PUBLIC_DEV_PASSWORD=AdminPass123!

# API URL
NEXT_PUBLIC_API_URL=http://localhost:3001/api
